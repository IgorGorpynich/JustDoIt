[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m[32m  self.debug = false[m
[32m+[m[32m  self.use_git_ruby = true[m
[32m+[m
[32m+[m[32m  @logger ||= ::Logger.new(STDOUT)[m
[32m+[m
[32m+[m[32m  def self.version[m

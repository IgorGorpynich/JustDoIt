[36m@@ -0,0 +1,365 @@[m
[32m+[m[32mrequire File.dirname(__FILE__) + '/helper'[m
[32m+[m
[32m+[m[32mclass TestRepo < Test::Unit::TestCase[m
[32m+[m[32m  def setup[m
[32m+[m[32m    @r = Repo.new(GRIT_REPO)[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def create_temp_repo(clone_path)[m
[32m+[m[32m    filename = 'git_test' + Time.now.to_i.to_s + rand(300).to_s.rjust(3, '0')[m
[32m+[m[32m    tmp_path = File.join("/tmp/", filename)[m
[32m+[m[32m    FileUtils.mkdir_p(tmp_path)[m
[32m+[m[32m    FileUtils.cp_r(clone_path, tmp_path)[m
[32m+[m[32m    File.join(tmp_path, 'dot_git')[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_update_refs_packed[m
[32m+[m[32m    gpath = create_temp_repo(File.join(File.dirname(__FILE__), *%w[dot_git]))[m
[32m+[m[32m    @git = Grit::Repo.new(gpath, :is_bare => true)[m
[32m+[m
[32m+[m[32m    # new and existing[m
[32m+[m[32m    test   = 'ac9a30f5a7f0f163bbe3b6f0abf18a6c83b06872'[m
[32m+[m[32m    master = 'ca8a30f5a7f0f163bbe3b6f0abf18a6c83b0687a'[m
[32m+[m
[32m+[m[32m    @git.update_ref('testref', test)[m
[32m+[m[32m    new_t = @git.get_head('testref').commit.sha[m
[32m+[m[32m    assert new_t != master[m
[32m+[m
[32m+[m[32m    @git.update_ref('master', test)[m
[32m+[m[32m    new_m = @git.get_head('master').commit.sha[m
[32m+[m[32m    assert new_m != master[m
[32m+[m
[32m+[m[32m    old = @git.get_head('nonpack').commit.sha[m
[32m+[m[32m    @git.update_ref('nonpack', test)[m
[32m+[m[32m    newp = @git.get_head('nonpack').commit.sha[m
[32m+[m[32m    assert newp != old[m
[32m+[m
[32m+[m[32m    FileUtils.rm_r(gpath)[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  # new[m
[32m+[m
[32m+[m[32m  def test_new_should_raise_on_invalid_repo_location[m
[32m+[m[32m    assert_raise(InvalidGitRepositoryError) do[m
[32m+[m[32m      Repo.new("/tmp")[m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_new_should_raise_on_non_existant_path[m
[32m+[m[32m    assert_raise(NoSuchPathError) do[m
[32m+[m[32m      Repo.new("/foobar")[m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  # descriptions[m
[32m+[m
[32m+[m[32m  def test_description[m
[32m+[m[32m    assert @r.description.include?("Unnamed repository; edit this file")[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  # refs[m

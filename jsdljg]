[32m+[m[32m    out = @git.ls_tree({}, @tree_sha, paths)[m
[32m+[m[32m    assert_equal out, "100644 blob 6afcf64c80da8253fa47228eb09bc0eea217e5d1\tlib/grit.rb\n040000 tree 6244414d0229fb2bd58bc426a2afb5ba66773498\tlib/grit"[m

[32m+[m[32m@@ -0,0 +1,24 @@[m
[32m+[m[32m+class Grit[m
[32m+[m[32m+  attr_accessor :path[m
[32m+[m[32m+[m[41m  [m
[32m+[m[32m+  # Create a new Grit instance[m
[32m+[m[32m+  #   +path+ is the path to either the root git directory or the bare git repo[m
[32m+[m[32m+  #[m
[32m+[m[32m+  # Examples[m
[32m+[m[32m+  #   g = Grit.new("/Users/tom/dev/grit")[m
[32m+[m[32m+  #   g = Grit.new("/Users/tom/public/grit.git")[m
[32m+[m[32m+  def initialize(path)[m
[32m+[m[32m+    if File.exist?(File.join(path, '.git'))[m
[32m+[m[32m+      self.path = File.join(path, '.git')[m
[32m+[m[32m+    elsif File.exist?(path) && path =~ /\.git$/[m
[32m+[m[32m+      self.path = path[m
[32m+[m[32m+    else[m
[32m+[m[32m+      raise InvalidGitRepositoryError.new(path) unless File.exist?(path)[m
[32m+[m[32m+    end[m
[32m+[m[32m+  end[m
[32m+[m[32m+[m[41m  [m
[32m+[m[32m+  # Return the project's description. Taken verbatim from REPO/description[m
[32m+[m[32m+  def description[m
[32m+[m[32m+    File.open(File.join(self.path, 'description')).read.chomp[m
[32m+[m[32m+  end[m
[32m+[m[32m+end[m
[32m+[m[32m\ No newline at end of file[m
[32m+[m[32mdiff --git a/test/helper.rb b/test/helper.rb[m
[32m+[m[32mnew file mode 100644[m

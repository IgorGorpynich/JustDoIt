[32m+[m[32mrequire File.dirname(__FILE__) + '/helper'[m
[32m+[m
[32m+[m[32mclass TestCommit < Test::Unit::TestCase[m
[32m+[m[32m  def setup[m
[32m+[m[32m    @r = Repo.new(File.join(File.dirname(__FILE__), *%w[dot_git]), :is_bare => true)[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  # __bake__[m
[32m+[m
[32m+[m[32m  def test_bake[m
[32m+[m[32m    Git.any_instance.expects(:rev_list).returns(fixture('rev_list_single'))[m
[32m+[m[32m    @c = Commit.create(@r, :id => '4c8124ffcf4039d292442eeccabdeca5af5c5017')[m
[32m+[m[32m    @c.author # bake[m
[32m+[m
[32m+[m[32m    assert_equal "Tom Preston-Werner", @c.author.name[m
[32m+[m[32m    assert_equal "tom@mojombo.com", @c.author.email[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  # short_name[m
[32m+[m
[32m+[m[32m  def test_id_abbrev[m
[32m+[m[32m    assert_equal '80f136f', @r.commit('80f136f500dfdb8c3e8abf4ae716f875f0a1b57f').id_abbrev[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  # count[m
[32m+[m
[32m+[m[32m  def test_count[m
[32m+[m[32m    assert_equal 107, Commit.count(@r, 'master')[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  # diff[m
[32m+[m
[32m+[m[32m  def test_diff[m
[32m+[m[32m    # git diff --full-index 91169e1f5fa4de2eaea3f176461f5dc784796769 > test/fixtures/diff_p[m
[32m+[m
[32m+[m[32m    Git.any_instance.expects(:diff).with({:full_index => true}, 'master').returns(fixture('diff_p'))[m
[32m+[m[32m    diffs = Commit.diff(@r, 'master')[m
[32m+[m
[32m+[m[32m    assert_equal 15, diffs.size[m
[32m+[m
[32m+[m[32m    assert_equal '.gitignore', diffs.first.a_path[m
[32m+[m[32m    assert_equal '.gitignore', diffs.first.b_path[m
[32m+[m[32m    assert_equal '4ebc8aea50e0a67e000ba29a30809d0a7b9b2666', diffs.first.a_blob.id[m
[32m+[m[32m    assert_equal '2dd02534615434d88c51307beb0f0092f21fd103', diffs.first.b_blob.id[m
[32m+[m[32m    assert_equal '100644', diffs.first.b_mode[m
[32m+[m[32m    assert_equal false, diffs.first.new_file[m
[32m+[m[32m    assert_equal false, diffs.first.deleted_file[m
[32m+[m[32m    assert_equal "--- a/.gitignore\n+++ b/.gitignore\n@@ -1 +1,2 @@\n coverage\n+pkg", diffs.first.diff[m
[32m+[m
[32m+[m[32m    assert_equal 'lib/grit/actor.rb', diffs[5].a_path[m
[32m+[m[32m    assert_equal nil, diffs[5].a_blob[m
[32m+[m[32m    assert_equal 'f733bce6b57c0e5e353206e692b0e3105c2527f4', diffs[5].b_blob.id[m
[32m+[m[32m    assert_equal true, diffs[5].new_file[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_diff_with_two_commits[m
[32m+[m[32m    # git diff --full-index 59ddc32 13d27d5 > test/fixtures/diff_2[m
[32m+[m[32m    Git.any_instance.expects(:diff).with({:full_index => true}, '59ddc32', '13d27d5').returns(fixture('diff_2'))[m
[32m+[m[32m    diffs = Commit.diff(@r, '59ddc32', '13d27d5')[m
[32m+[m
[32m+[m[32m    assert_equal 3, diffs.size[m
[32m+[m[32m    assert_equal %w(lib/grit/commit.rb test/fixtures/show_empty_commit test/test_commit.rb), diffs.collect { |d| d.a_path }[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_diff_with_files[m
[32m+[m[32m    # git diff --full-index 59ddc32 -- lib > test/fixtures/diff_f[m
[32m+[m[32m    Git.any_instance.expects(:diff).with({:full_index => true}, '59ddc32', '--', 'lib').returns(fixture('diff_f'))[m
[32m+[m[32m    diffs = Commit.diff(@r, '59ddc32', %w(lib))[m
[32m+[m
[32m+[m[32m    assert_equal 1, diffs.size[m
[32m+[m[32m    assert_equal 'lib/grit/diff.rb', diffs.first.a_path[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_diff_with_two_commits_and_files[m
[32m+[m[32m    # git diff --full-index 59ddc32 13d27d5 -- lib > test/fixtures/diff_2f[m
[32m+[m[32m    Git.any_instance.expects(:diff).with({:full_index => true}, '59ddc32', '13d27d5', '--', 'lib').returns(fixture('diff_2f'))[m
[32m+[m[32m    diffs = Commit.diff(@r, '59ddc32', '13d27d5', %w(lib))[m
[32m+[m
[32m+[m[32m    assert_equal 1, diffs.size[m
[32m+[m[32m    assert_equal 'lib/grit/commit.rb', diffs.first.a_path[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  # diffs[m
[32m+[m[32m  def test_diffs[m
[32m+[m[32m    # git diff --full-index 91169e1f5fa4de2eaea3f176461f5dc784796769 > test/fixtures/diff_p[m
[32m+[m
[32m+[m[32m    Git.any_instance.expects(:diff).returns(fixture('diff_p'))[m
[32m+[m[32m    @c = Commit.create(@r, :id => '91169e1f5fa4de2eaea3f176461f5dc784796769')[m
[32m+[m[32m    diffs = @c.diffs[m
[32m+[m
[32m+[m[32m    assert_equal 15, diffs.size[m
[32m+[m
[32m+[m[32m    assert_equal '.gitignore', diffs.first.a_path[m
[32m+[m[32m    assert_equal '.gitignore', diffs.first.b_path[m
[32m+[m[32m    assert_equal '4ebc8aea50e0a67e000ba29a30809d0a7b9b2666', diffs.first.a_blob.id[m
[32m+[m[32m    assert_equal '2dd02534615434d88c51307beb0f0092f21fd103', diffs.first.b_blob.id[m
[32m+[m[32m    assert_equal '100644', diffs.first.b_mode[m
[32m+[m[32m    assert_equal false, diffs.first.new_file[m
[32m+[m[32m    assert_equal false, diffs.first.deleted_file[m
[32m+[m[32m    assert_equal "--- a/.gitignore\n+++ b/.gitignore\n@@ -1 +1,2 @@\n coverage\n+pkg", diffs.first.diff[m
[32m+[m
[32m+[m[32m    assert_equal 'lib/grit/actor.rb', diffs[5].a_path[m
[32m+[m[32m    assert_equal nil, diffs[5].a_blob[m
[32m+[m[32m    assert_equal 'f733bce6b57c0e5e353206e692b0e3105c2527f4', diffs[5].b_blob.id[m
[32m+[m[32m    assert_equal true, diffs[5].new_file[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_diffs_on_initial_import[m
[32m+[m[32m    # git show --full-index 634396b2f541a9f2d58b00be1a07f0c358b999b3 > test/fixtures/diff_i[m
[32m+[m
[32m+[m[32m    Git.any_instance.expects(:show).with({:full_index => true, :pretty => 'raw'}, '634396b2f541a9f2d58b00be1a07f0c358b999b3').returns(fixture('diff_i'))[m
[32m+[m[32m    @c = Commit.create(@r, :id => '634396b2f541a9f2d58b00be1a07f0c358b999b3')[m
[32m+[m[32m    diffs = @c.diffs[m
[32m+[m
[32m+[m[32m    assert_equal 10, diffs.size[m
[32m+[m
[32m+[m[32m    assert_equal 'History.txt', diffs.first.a_path[m
[32m+[m[32m    assert_equal 'History.txt', diffs.first.b_path[m
[32m+[m[32m    assert_equal nil, diffs.first.a_blob[m
[32m+[m[32m    assert_equal nil, diffs.first.b_mode[m
[32m+[m[32m    assert_equal '81d2c27608b352814cbe979a6acd678d30219678', diffs.first.b_blob.id[m
[32m+[m[32m    assert_equal true, diffs.first.new_file[m
[32m+[m[32m    assert_equal false, diffs.first.deleted_file[m
[32m+[m[32m    assert_equal "--- /dev/null\n+++ b/History.txt\n@@ -0,0 +1,5 @@\n+== 1.0.0 / 2007-10-09\n+\n+* 1 major enhancement\n+  * Birthday!\n+", diffs.first.diff[m
[32m+[m
[32m+[m
[32m+[m[32m    assert_equal 'lib/grit.rb', diffs[5].a_path[m
[32m+[m[32m    assert_equal nil, diffs[5].a_blob[m
[32m+[m[32m    assert_equal '32cec87d1e78946a827ddf6a8776be4d81dcf1d1', diffs[5].b_blob.id[m
[32m+[m[32m    assert_equal true, diffs[5].new_file[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_diffs_on_initial_import_with_empty_commit[m
[32m+[m[32m    Git.any_instance.expects(:show).with([m
[32m+[m[32m      {:full_index => true, :pretty => 'raw'},[m
[32m+[m[32m      '634396b2f541a9f2d58b00be1a07f0c358b999b3'[m
[32m+[m[32m    ).returns(fixture('show_empty_commit'))[m
[32m+[m
[32m+[m[32m    @c = Commit.create(@r, :id => '634396b2f541a9f2d58b00be1a07f0c358b999b3')[m
[32m+[m[32m    diffs = @c.diffs[m
[32m+[m
[32m+[m[32m    assert_equal [], diffs[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_diffs_with_mode_only_change[m
[32m+[m[32m    Git.any_instance.expects(:diff).returns(fixture('diff_mode_only'))[m
[32m+[m[32m    @c = Commit.create(@r, :id => '91169e1f5fa4de2eaea3f176461f5dc784796769')[m
[32m+[m[32m    diffs = @c.diffs[m
[32m+[m
[32m+[m[32m    assert_equal 23, diffs.size[m
[32m+[m[32m    assert_equal '100644', diffs[0].a_mode[m
[32m+[m[32m    assert_equal '100755', diffs[0].b_mode[m
[32m+[m[32m  end[m

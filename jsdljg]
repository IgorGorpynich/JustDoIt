[32m+[m[32m  # commits[m
[32m+[m
[32m+[m[32m  def test_commits[m
[32m+[m[32m    Git.any_instance.expects(:rev_list).returns(fixture('rev_list'))[m
[32m+[m
[32m+[m[32m    commits = @r.commits('master', 10)[m
[32m+[m
[32m+[m[32m    c = commits[0][m
[32m+[m[32m    assert_equal '4c8124ffcf4039d292442eeccabdeca5af5c5017', c.id[m
[32m+[m[32m    assert_equal ["634396b2f541a9f2d58b00be1a07f0c358b999b3"], c.parents.map { |p| p.id }[m
[32m+[m[32m    assert_equal "672eca9b7f9e09c22dcb128c283e8c3c8d7697a4", c.tree.id[m
[32m+[m[32m    assert_equal "Tom Preston-Werner", c.author.name[m
[32m+[m[32m    assert_equal "tom@mojombo.com", c.author.email[m
[32m+[m[32m    assert_equal Time.at(1191999972), c.authored_date[m
[32m+[m[32m    assert_equal "Tom Preston-Werner", c.committer.name[m
[32m+[m[32m    assert_equal "tom@mojombo.com", c.committer.email[m
[32m+[m[32m    assert_equal Time.at(1191999972), c.committed_date[m
[32m+[m[32m    assert_equal "implement Grit#heads", c.message[m
[32m+[m
[32m+[m[32m    c = commits[1][m
[32m+[m[32m    assert_equal [], c.parents[m
[32m+[m
[32m+[m[32m    c = commits[2][m
[32m+[m[32m    assert_equal ["6e64c55896aabb9a7d8e9f8f296f426d21a78c2c", "7f874954efb9ba35210445be456c74e037ba6af2"], c.parents.map { |p| p.id }[m
[32m+[m[32m    assert_equal "Merge branch 'site'\n\n  * Some other stuff\n  * just one more", c.message[m
[32m+[m[32m    assert_equal "Merge branch 'site'", c.short_message[m
[32m+[m[32m  end[m
[32m+[m

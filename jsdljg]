[32m+[m
[32m+[m[32mclass TestRubyGitIv2 < Test::Unit::TestCase[m
[32m+[m
[32m+[m[32m  def setup[m
[32m+[m[32m    @git = Grit::Repo.new(File.join(File.dirname(__FILE__), *%w[dot_git_iv2]), :is_bare => true)[m
[32m+[m[32m    @rgit = @git.git.ruby_git[m
[32m+[m[32m    @commit_sha = 'ca8a30f5a7f0f163bbe3b6f0abf18a6c83b0687a'[m
[32m+[m[32m    @tree_sha   = 'cd7422af5a2e0fff3e94d6fb1a8fff03b2841881'[m
[32m+[m[32m    @blob_sha   = '4232d073306f01cf0b895864e5a5cfad7dd76fce'[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_basic[m
[32m+[m[32m    assert @rgit.object_exists?(@commit_sha)[m
[32m+[m[32m    assert_equal 10, @git.commits.size[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_objects[m
[32m+[m[32m    commit = @rgit.get_object_by_sha1(@commit_sha)[m
[32m+[m[32m    assert_equal commit.author.email, 'schacon@gmail.com'[m
[32m+[m[32m    tree = @rgit.get_object_by_sha1(@tree_sha)[m
[32m+[m[32m    assert_equal 7, tree.entry.size[m
[32m+[m[32m    blob = @rgit.get_object_by_sha1(@blob_sha)[m
[32m+[m[32m    assert_match 'First public release', blob.content[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32mend[m

[32m+[m[32mdef main[m
[32m+[m[32m  @wbare = File.expand_path(File.join('../../', 'test', 'dot_git'))[m
[32m+[m[41m  [m
[32m+[m[32m  in_temp_dir do[m
[32m+[m[32m    #result = RubyProf.profile do[m
[32m+[m
[32m+[m[32m      git = Grit::Repo.new('.')[m
[32m+[m[32m      puts Grit::VERSION[m
[32m+[m[32m      #Grit::GitRuby.cache_client = MemCache.new 'localhost:11211', :namespace => 'grit'[m
[32m+[m[32m      #Grit.debug = true[m
[32m+[m[41m    [m
[32m+[m[32m      #pp Grit::GitRuby.cache_client.stats[m[41m [m

[32m+[m[32mrequire 'tempfile'[m
[32m+[m[32mmodule Grit[m
[32m+[m
[32m+[m[32m  class Git[m
[32m+[m[32m    class GitTimeout < RuntimeError[m
[32m+[m[32m      attr_reader :command, :bytes_read[m
[32m+[m
[32m+[m[32m      def initialize(command = nil, bytes_read = nil)[m
[32m+[m[32m        @command = command[m
[32m+[m[32m        @bytes_read = bytes_read[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    undef_method :clone[m
[32m+[m
[32m+[m[32m    include GitRuby[m
[32m+[m
[32m+[m[32m    def exist?[m
[32m+[m[32m      File.exist?(self.git_dir)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def put_raw_object(content, type)[m
[32m+[m[32m      ruby_git.put_raw_object(content, type)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def object_exists?(object_id)[m
[32m+[m[32m      ruby_git.object_exists?(object_id)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def select_existing_objects(object_ids)[m
[32m+[m[32m      object_ids.select do |object_id|[m
[32m+[m[32m        object_exists?(object_id)[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    class << self[m
[32m+[m[32m      attr_accessor :git_binary, :git_timeout, :git_max_size[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    if RUBY_PLATFORM.downcase =~ /mswin(?!ce)|mingw|bccwin/[m
[32m+[m[32m      self.git_binary   = "git" # using search path[m
[32m+[m[32m    else[m
[32m+[m[32m      self.git_binary   = "/usr/bin/env git"[m
[32m+[m[32m    end[m
[32m+[m[32m    self.git_timeout  = 10[m
[32m+[m[32m    self.git_max_size = 5242880 # 5.megabytes[m
[32m+[m
[32m+[m[32m    def self.with_timeout(timeout = 10.seconds)[m
[32m+[m[32m      old_timeout = Grit::Git.git_timeout[m
[32m+[m[32m      Grit::Git.git_timeout = timeout[m
[32m+[m[32m      yield[m
[32m+[m[32m      Grit::Git.git_timeout = old_timeout[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    attr_accessor :git_dir, :bytes_read, :work_tree[m
[32m+[m
[32m+[m[32m    def initialize(git_dir)[m
[32m+[m[32m      self.git_dir    = git_dir[m
[32m+[m[32m      self.work_tree  = git_dir.gsub(/\/\.git$/,'')[m
[32m+[m[32m      self.bytes_read = 0[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def shell_escape(str)[m
[32m+[m[32m      str.to_s.gsub("'", "\\\\'").gsub(";", '\\;')[m
[32m+[m[32m    end[m
[32m+[m[32m    alias_method :e, :shell_escape[m
[32m+[m
[32m+[m[32m    # Check if a normal file exists on the filesystem[m
[32m+[m[32m    #   +file+ is the relative path from the Git dir[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Boolean[m
[32m+[m[32m    def fs_exist?(file)[m
[32m+[m[32m      File.exist?(File.join(self.git_dir, file))[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Read a normal file from the filesystem.[m
[32m+[m[32m    #   +file+ is the relative path from the Git dir[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns the String contents of the file[m
[32m+[m[32m    def fs_read(file)[m
[32m+[m[32m      File.read(File.join(self.git_dir, file))[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Write a normal file to the filesystem.[m
[32m+[m[32m    #   +file+ is the relative path from the Git dir[m
[32m+[m[32m    #   +contents+ is the String content to be written[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns nothing[m
[32m+[m[32m    def fs_write(file, contents)[m
[32m+[m[32m      path = File.join(self.git_dir, file)[m
[32m+[m[32m      FileUtils.mkdir_p(File.dirname(path))[m
[32m+[m[32m      File.open(path, 'w') do |f|[m
[32m+[m[32m        f.write(contents)[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Delete a normal file from the filesystem[m
[32m+[m[32m    #   +file+ is the relative path from the Git dir[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns nothing[m
[32m+[m[32m    def fs_delete(file)[m
[32m+[m[32m      FileUtils.rm_rf(File.join(self.git_dir, file))[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Move a normal file[m
[32m+[m[32m    #   +from+ is the relative path to the current file[m
[32m+[m[32m    #   +to+ is the relative path to the destination file[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns nothing[m
[32m+[m[32m    def fs_move(from, to)[m
[32m+[m[32m      FileUtils.mv(File.join(self.git_dir, from), File.join(self.git_dir, to))[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Make a directory[m
[32m+[m[32m    #   +dir+ is the relative path to the directory to create[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns nothing[m
[32m+[m[32m    def fs_mkdir(dir)[m
[32m+[m[32m      FileUtils.mkdir_p(File.join(self.git_dir, dir))[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Chmod the the file or dir and everything beneath[m
[32m+[m[32m    #   +file+ is the relative path from the Git dir[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns nothing[m
[32m+[m[32m    def fs_chmod(mode, file = '/')[m
[32m+[m[32m      FileUtils.chmod_R(mode, File.join(self.git_dir, file))[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def list_remotes[m
[32m+[m[32m      remotes = [][m
[32m+[m[32m      Dir.chdir(File.join(self.git_dir, 'refs/remotes')) do[m
[32m+[m[32m        remotes = Dir.glob('*')[m
[32m+[m[32m      end[m
[32m+[m[32m      remotes[m
[32m+[m[32m    rescue[m
[32m+[m[32m      [][m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def create_tempfile(seed, unlink = false)[m
[32m+[m[32m      path = Tempfile.new(seed).path[m
[32m+[m[32m      File.unlink(path) if unlink[m
[32m+[m[32m      return path[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def commit_from_sha(id)[m
[32m+[m[32m      git_ruby_repo = GitRuby::Repository.new(self.git_dir)[m
[32m+[m[32m      object = git_ruby_repo.get_object_by_sha1(id)[m
[32m+[m
[32m+[m[32m      if object.type == :commit[m
[32m+[m[32m        id[m
[32m+[m[32m      elsif object.type == :tag[m
[32m+[m[32m        object.object[m
[32m+[m[32m      else[m
[32m+[m[32m        ''[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def check_applies(head_sha, applies_sha)[m
[32m+[m[32m      git_index = create_tempfile('index', true)[m
[32m+[m[32m      (o1, exit1) = raw_git("git read-tree #{head_sha} 2>/dev/null", git_index)[m
[32m+[m[32m      (o2, exit2) = raw_git("git diff #{applies_sha}^ #{applies_sha} | git apply --check --cached >/dev/null 2>/dev/null", git_index)[m
[32m+[m[32m      return (exit1 + exit2)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def get_patch(applies_sha)[m
[32m+[m[32m      git_index = create_tempfile('index', true)[m
[32m+[m[32m      (patch, exit2) = raw_git("git diff #{applies_sha}^ #{applies_sha}", git_index)[m
[32m+[m[32m      patch[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def apply_patch(head_sha, patch)[m
[32m+[m[32m      git_index = create_tempfile('index', true)[m
[32m+[m
[32m+[m[32m      git_patch = create_tempfile('patch')[m
[32m+[m[32m      File.open(git_patch, 'w+') { |f| f.print patch }[m
[32m+[m
[32m+[m[32m      raw_git("git read-tree #{head_sha} 2>/dev/null", git_index)[m
[32m+[m[32m      (op, exit) = raw_git("git apply --cached < #{git_patch}", git_index)[m
[32m+[m[32m      if exit == 0[m
[32m+[m[32m        return raw_git("git write-tree", git_index).first.chomp[m
[32m+[m[32m      end[m
[32m+[m[32m      false[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # RAW CALLS WITH ENV SETTINGS[m
[32m+[m[32m    def raw_git_call(command, index)[m
[32m+[m[32m      tmp = ENV['GIT_INDEX_FILE'][m
[32m+[m[32m      ENV['GIT_INDEX_FILE'] = index[m
[32m+[m[32m      out = `#{command}`[m
[32m+[m[32m      after = ENV['GIT_INDEX_FILE'] # someone fucking with us ??[m
[32m+[m[32m      ENV['GIT_INDEX_FILE'] = tmp[m
[32m+[m[32m      if after != index[m
[32m+[m[32m        raise 'environment was changed for the git call'[m
[32m+[m[32m      end[m
[32m+[m[32m      [out, $?.exitstatus][m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def raw_git(command, index)[m
[32m+[m[32m      output = nil[m
[32m+[m[32m      Dir.chdir(self.git_dir) do[m
[32m+[m[32m        output = raw_git_call(command, index)[m
[32m+[m[32m      end[m
[32m+[m[32m      output[m
[32m+[m[32m    end[m
[32m+[m[32m    # RAW CALLS WITH ENV SETTINGS END[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m    # Run the given git command with the specified arguments and return[m
[32m+[m[32m    # the result as a String[m
[32m+[m[32m    #   +cmd+ is the command[m
[32m+[m[32m    #   +options+ is a hash of Ruby style options[m
[32m+[m[32m    #   +args+ is the list of arguments (to be joined by spaces)[m
[32m+[m[32m    #[m
[32m+[m[32m    # Examples[m
[32m+[m[32m    #   git.rev_list({:max_count => 10, :header => true}, "master")[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns String[m
[32m+[m[32m    def method_missing(cmd, options = {}, *args)[m
[32m+[m[32m      run('', cmd, '', options, args)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Bypass any pure Ruby implementations and go straight to the native Git command[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns String[m
[32m+[m[32m    def native(cmd, options = {}, *args)[m
[32m+[m[32m      method_missing(cmd, options, *args)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def run(prefix, cmd, postfix, options, args)[m
[32m+[m[32m      timeout  = options.delete(:timeout) rescue nil[m
[32m+[m[32m      timeout  = true if timeout.nil?[m
[32m+[m
[32m+[m[32m      opt_args = transform_options(options)[m
[32m+[m
[32m+[m[32m      if RUBY_PLATFORM.downcase =~ /mswin(?!ce)|mingw|bccwin/[m
[32m+[m[32m        ext_args = args.reject { |a| a.empty? }.map { |a| (a == '--' || a[0].chr == '|') ? a : "\"#{e(a)}\"" }[m
[32m+[m[32m        call = "#{prefix}#{Git.git_binary} --git-dir=\"#{self.git_dir}\" #{cmd.to_s.gsub(/_/, '-')} #{(opt_args + ext_args).join(' ')}#{e(postfix)}"[m
[32m+[m[32m      else[m
[32m+[m[32m        ext_args = args.reject { |a| a.empty? }.map { |a| (a == '--' || a[0].chr == '|') ? a : "'#{e(a)}'" }[m
[32m+[m[32m        call = "#{prefix}#{Git.git_binary} --git-dir='#{self.git_dir}' #{cmd.to_s.gsub(/_/, '-')} #{(opt_args + ext_args).join(' ')}#{e(postfix)}"[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      Grit.log(call) if Grit.debug[m
[32m+[m[32m      response, err = timeout ? sh(call) : wild_sh(call)[m
[32m+[m[32m      Grit.log(response) if Grit.debug[m
[32m+[m[32m      Grit.log(err) if Grit.debug[m
[32m+[m[32m      response[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def sh(command)[m
[32m+[m[32m      ret, err = '', ''[m
[32m+[m[32m      Open3.popen3(command) do |_, stdout, stderr|[m
[32m+[m[32m        Timeout.timeout(self.class.git_timeout) do[m
[32m+[m[32m          while tmp = stdout.read(1024)[m
[32m+[m[32m            ret += tmp[m
[32m+[m[32m            if (@bytes_read += tmp.size) > self.class.git_max_size[m
[32m+[m[32m              bytes = @bytes_read[m
[32m+[m[32m              @bytes_read = 0[m
[32m+[m[32m              raise GitTimeout.new(command, bytes)[m
[32m+[m[32m            end[m
[32m+[m[32m          end[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        while tmp = stderr.read(1024)[m
[32m+[m[32m          err += tmp[m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m[32m      [ret, err][m
[32m+[m[32m    rescue Timeout::Error, Grit::Git::GitTimeout[m
[32m+[m[32m      bytes = @bytes_read[m
[32m+[m[32m      @bytes_read = 0[m
[32m+[m[32m      raise GitTimeout.new(command, bytes)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def wild_sh(command)[m
[32m+[m[32m      ret, err = '', ''[m
[32m+[m[32m      Open3.popen3(command) do |_, stdout, stderr|[m
[32m+[m[32m        while tmp = stdout.read(1024)[m
[32m+[m[32m          ret += tmp[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        while tmp = stderr.read(1024)[m
[32m+[m[32m          err += tmp[m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m[32m      [ret, err][m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Transform Ruby style options into git command line options[m
[32m+[m[32m    #   +options+ is a hash of Ruby style options[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns String[][m
[32m+[m[32m    #   e.g. ["--max-count=10", "--header"][m
[32m+[m[32m    def transform_options(options)[m
[32m+[m[32m      args = [][m
[32m+[m[32m      options.keys.each do |opt|[m
[32m+[m[32m        if opt.to_s.size == 1[m
[32m+[m[32m          if options[opt] == true[m
[32m+[m[32m            args << "-#{opt}"[m
[32m+[m[32m          elsif options[opt] == false[m
[32m+[m[32m            # ignore[m
[32m+[m[32m          else[m
[32m+[m[32m            val = options.delete(opt)[m
[32m+[m[32m            args << "-#{opt.to_s} '#{e(val)}'"[m
[32m+[m[32m          end[m
[32m+[m[32m        else[m
[32m+[m[32m          if options[opt] == true[m
[32m+[m[32m            args << "--#{opt.to_s.gsub(/_/, '-')}"[m
[32m+[m[32m          elsif options[opt] == false[m
[32m+[m[32m            # ignore[m
[32m+[m[32m          else[m
[32m+[m[32m            val = options.delete(opt)[m
[32m+[m[32m            args << "--#{opt.to_s.gsub(/_/, '-')}='#{e(val)}'"[m
[32m+[m[32m          end[m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m[32m      args[m
[32m+[m[32m    end[m
[32m+[m[32m  end # Git[m
[32m+[m
[32m+[m[32mend # Grit[m
[1mdiff --git a/grit/lib/grit/index.rb b/grit/lib/grit/index.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..203715c[m
[1m--- /dev/null[m
[1m+++ b/grit/lib/grit/index.rb[m
[36m@@ -0,0 +1,122 @@[m
[32m+[m[32mmodule Grit[m
[32m+[m
[32m+[m[32m  class Index[m
[32m+[m[32m    attr_accessor :repo, :tree, :current_tree[m
[32m+[m
[32m+[m[32m    def initialize(repo)[m
[32m+[m[32m      self.repo = repo[m
[32m+[m[32m      self.tree = {}[m
[32m+[m[32m      self.current_tree = nil[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Add a file to the index[m
[32m+[m[32m    #   +path+ is the path (including filename)[m
[32m+[m[32m    #   +data+ is the binary contents of the file[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns nothing[m
[32m+[m[32m    def add(file_path, data)[m
[32m+[m[32m      path = file_path.split('/')[m
[32m+[m[32m      filename = path.pop[m
[32m+[m
[32m+[m[32m      current = self.tree[m
[32m+[m
[32m+[m[32m      path.each do |dir|[m
[32m+[m[32m        current[dir] ||= {}[m
[32m+[m[32m        node = current[dir][m
[32m+[m[32m        current = node[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      current[filename] = data[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Sets the current tree[m
[32m+[m[32m    #   +tree+ the branch/tag/sha... to use - a string[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns index (self)[m
[32m+[m[32m    def read_tree(tree)[m
[32m+[m[32m      self.current_tree = self.repo.tree(tree)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Commit the contents of the index[m
[32m+[m[32m    #   +message+ is the commit message [nil][m
[32m+[m[32m    #   +parents+ is one or more commits to attach this commit to to form a new head [nil][m
[32m+[m[32m    #   +actor+ is the details of the user making the commit [nil][m
[32m+[m[32m    #   +last_tree+ is a tree to compare with - to avoid making empty commits [nil][m
[32m+[m[32m    #   +head+ is the branch to write this head to [master][m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns a String of the SHA1 of the commit[m
[32m+[m[32m    def commit(message, parents = nil, actor = nil, last_tree = nil, head = 'master')[m
[32m+[m[32m      tree_sha1 = write_tree(self.tree, self.current_tree)[m
[32m+[m[32m      return false if tree_sha1 == last_tree # don't write identical commits[m
[32m+[m
[32m+[m[32m      contents = [][m
[32m+[m[32m      contents << ['tree', tree_sha1].join(' ')[m
[32m+[m[32m      parents.each do |p|[m
[32m+[m[32m        contents << ['parent', p].join(' ') if p[m
[32m+[m[32m      end if parents[m
[32m+[m
[32m+[m[32m      if actor[m
[32m+[m[32m        name = actor.name[m
[32m+[m[32m        email = actor.email[m
[32m+[m[32m      else[m
[32m+[m[32m        config = Config.new(self.repo)[m
[32m+[m[32m        name = config['user.name'][m
[32m+[m[32m        email = config['user.email'][m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      author_string = "#{name} <#{email}> #{Time.now.to_i} -0700" # !! TODO : gotta fix this[m
[32m+[m[32m      contents << ['author', author_string].join(' ')[m
[32m+[m[32m      contents << ['committer', author_string].join(' ')[m
[32m+[m[32m      contents << ''[m
[32m+[m[32m      contents << message[m
[32m+[m
[32m+[m[32m      commit_sha1 = self.repo.git.put_raw_object(contents.join("\n"), 'commit')[m
[32m+[m
[32m+[m[32m      self.repo.update_ref(head, commit_sha1)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Recursively write a tree to the index[m
[32m+[m[32m    #   +tree+ is the tree[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns the SHA1 String of the tree[m
[32m+[m[32m    def write_tree(tree, now_tree = nil)[m
[32m+[m[32m      tree_contents = {}[m
[32m+[m
[32m+[m[32m      # fill in original tree[m
[32m+[m[32m      now_tree.contents.each do |obj|[m
[32m+[m[32m        sha = [obj.id].pack("H*")[m
[32m+[m[32m        k = obj.name[m
[32m+[m[32m        k += '/' if (obj.class == Grit::Tree)[m
[32m+[m[32m        tree_contents[k] = "%s %s\0%s" % [obj.mode.to_s, obj.name, sha][m
[32m+[m[32m      end if now_tree[m
[32m+[m
[32m+[m[32m      # overwrite with new tree contents[m
[32m+[m[32m      tree.each do |k, v|[m
[32m+[m[32m        case v[m
[32m+[m[32m          when String[m
[32m+[m[32m            sha = write_blob(v)[m
[32m+[m[32m            sha = [sha].pack("H*")[m
[32m+[m[32m            str = "%s %s\0%s" % ['100644', k, sha][m
[32m+[m[32m            tree_contents[k] = str[m
[32m+[m[32m          when Hash[m
[32m+[m[32m            ctree = now_tree/k if now_tree[m
[32m+[m[32m            sha = write_tree(v, ctree)[m
[32m+[m[32m            sha = [sha].pack("H*")[m
[32m+[m[32m            str = "%s %s\0%s" % ['040000', k, sha][m
[32m+[m[32m            tree_contents[k + '/'] = str[m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m[32m      tr = tree_contents.sort.map { |k, v| v }.join('')[m
[32m+[m[32m      self.repo.git.put_raw_object(tr, 'tree')[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Write the blob to the index[m
[32m+[m[32m    #   +data+ is the data to write[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns the SHA1 String of the blob[m
[32m+[m[32m    def write_blob(data)[m
[32m+[m[32m      self.repo.git.put_raw_object(data, 'blob')[m
[32m+[m[32m    end[m
[32m+[m[32m  end # Index[m
[32m+[m
[32m+[m[32mend # Grit[m
[1mdiff --git a/grit/lib/grit/lazy.rb b/grit/lib/grit/lazy.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..763fa23[m
[1m--- /dev/null[m
[1m+++ b/grit/lib/grit/lazy.rb[m
[36m@@ -0,0 +1,33 @@[m
[32m+[m[32m##[m
[32m+[m[32m# Allows attributes to be declared as lazy, meaning that they won't be[m
[32m+[m[32m# computed until they are asked for.[m
[32m+[m[32m#[m
[32m+[m[32m# Works by delegating each lazy_reader to a cached lazy_source method.[m
[32m+[m[32m#[m
[32m+[m[32m# class Person[m
[32m+[m[32m#   lazy_reader :eyes[m
[32m+[m[32m#[m
[32m+[m[32m#   def lazy_source[m
[32m+[m[32m#     OpenStruct.new(:eyes => 2)[m
[32m+[m[32m#   end[m
[32m+[m[32m# end[m
[32m+[m[32m#[m
[32m+[m[32m# >> Person.new.eyes[m
[32m+[m[32m# => 2[m
[32m+[m[32m#[m
[32m+[m[32mmodule Lazy[m
[32m+[m[32m  def lazy_reader(*args)[m
[32m+[m[32m    args.each do |arg|[m
[32m+[m[32m      ivar = "@#{arg}"[m
[32m+[m[32m      define_method(arg) do[m
[32m+[m[32m        if instance_variable_defined?(ivar)[m
[32m+[m[32m          val = instance_variable_get(ivar)[m
[32m+[m[32m          return val if val[m
[32m+[m[32m        end[m
[32m+[m[32m        instance_variable_set(ivar, (@lazy_source ||= lazy_source).send(arg))[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m[32mend[m
[32m+[m
[32m+[m[32mObject.extend Lazy unless Object.ancestors.include? Lazy[m
[1mdiff --git a/grit/lib/grit/merge.rb b/grit/lib/grit/merge.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..23e1407[m
[1m--- /dev/null[m
[1m+++ b/grit/lib/grit/merge.rb[m
[36m@@ -0,0 +1,45 @@[m
[32m+[m[32mmodule Grit[m
[32m+[m
[32m+[m[32m  class Merge[m
[32m+[m
[32m+[m[32m    STATUS_BOTH = 'both'[m
[32m+[m[32m    STATUS_OURS = 'ours'[m
[32m+[m[32m    STATUS_THEIRS = 'theirs'[m
[32m+[m
[32m+[m[32m    attr_reader :conflicts, :text, :sections[m
[32m+[m
[32m+[m[32m    def initialize(str)[m
[32m+[m[32m      status = STATUS_BOTH[m
[32m+[m
[32m+[m[32m      section = 1[m
[32m+[m[32m      @conflicts = 0[m
[32m+[m[32m      @text = {}[m
[32m+[m
[32m+[m[32m      lines = str.split("\n")[m
[32m+[m[32m      lines.each do |line|[m
[32m+[m[32m        if /^<<<<<<< (.*?)/.match(line)[m
[32m+[m[32m          status = STATUS_OURS[m
[32m+[m[32m          @conflicts += 1[m
[32m+[m[32m          section += 1[m
[32m+[m[32m        elsif line == '======='[m
[32m+[m[32m          status = STATUS_THEIRS[m
[32m+[m[32m        elsif /^>>>>>>> (.*?)/.match(line)[m
[32m+[m[32m          status = STATUS_BOTH[m
[32m+[m[32m          section += 1[m
[32m+[m[32m        else[m
[32m+[m[32m          @text[section] ||= {}[m
[32m+[m[32m          @text[section][status] ||= [][m
[32m+[m[32m          @text[section][status] << line[m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m[32m      @text = @text.values[m
[32m+[m[32m      @sections = @text.size[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Pretty object inspection[m
[32m+[m[32m    def inspect[m
[32m+[m[32m      %Q{#<Grit::Merge}[m
[32m+[m[32m    end[m
[32m+[m[32m  end # Merge[m
[32m+[m
[32m+[m[32mend # Grit[m
[1mdiff --git a/grit/lib/grit/ref.rb b/grit/lib/grit/ref.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..405c6cd[m
[1m--- /dev/null[m
[1m+++ b/grit/lib/grit/ref.rb[m
[36m@@ -0,0 +1,74 @@[m
[32m+[m[32mmodule Grit[m
[32m+[m
[32m+[m[32m  class Ref[m
[32m+[m
[32m+[m[32m    class << self[m
[32m+[m
[32m+[m[32m      # Find all Refs[m
[32m+[m[32m      #   +repo+ is the Repo[m
[32m+[m[32m      #   +options+ is a Hash of options[m
[32m+[m[32m      #[m
[32m+[m[32m      # Returns Grit::Ref[] (baked)[m
[32m+[m[32m      def find_all(repo, options = {})[m
[32m+[m[32m        refs = repo.git.refs(options, prefix)[m
[32m+[m[32m        refs.split("\n").map do |ref|[m
[32m+[m[32m          name, id = *ref.split(' ')[m
[32m+[m[32m          commit = Commit.create(repo, :id => id)[m
[32m+[m[32m          self.new(name, commit)[m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      protected[m
[32m+[m
[32m+[m[32m        def prefix[m
[32m+[m[32m          "refs/#{name.to_s.gsub(/^.*::/, '').downcase}s"[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    attr_reader :name[m
[32m+[m[32m    attr_reader :commit[m
[32m+[m
[32m+[m[32m    # Instantiate a new Head[m
[32m+[m[32m    #   +name+ is the name of the head[m
[32m+[m[32m    #   +commit+ is the Commit that the head points to[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Head (baked)[m
[32m+[m[32m    def initialize(name, commit)[m
[32m+[m[32m      @name = name[m
[32m+[m[32m      @commit = commit[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Pretty object inspection[m
[32m+[m[32m    def inspect[m
[32m+[m[32m      %Q{#<#{self.class.name} "#{@name}">}[m
[32m+[m[32m    end[m
[32m+[m[32m  end # Ref[m
[32m+[m
[32m+[m[32m  # A Head is a named reference to a Commit. Every Head instance contains a name[m
[32m+[m[32m  # and a Commit object.[m
[32m+[m[32m  #[m
[32m+[m[32m  #   r = Grit::Repo.new("/path/to/repo")[m
[32m+[m[32m  #   h = r.heads.first[m
[32m+[m[32m  #   h.name       # => "master"[m
[32m+[m[32m  #   h.commit     # => #<Grit::Commit "1c09f116cbc2cb4100fb6935bb162daa4723f455">[m
[32m+[m[32m  #   h.commit.id  # => "1c09f116cbc2cb4100fb6935bb162daa4723f455"[m
[32m+[m[32m  class Head < Ref[m
[32m+[m
[32m+[m[32m    # Get the HEAD revision of the repo.[m
[32m+[m[32m    #   +repo+ is the Repo[m
[32m+[m[32m    #   +options+ is a Hash of options[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Head (baked)[m
[32m+[m[32m    def self.current(repo, options = {})[m
[32m+[m[32m      head = repo.git.fs_read('HEAD').chomp[m
[32m+[m[32m      if /ref: refs\/heads\/(.*)/.match(head)[m
[32m+[m[32m        self.new($1, repo.git.rev_parse(options, 'HEAD'))[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m  end # Head[m
[32m+[m
[32m+[m[32m  class Remote < Ref; end[m
[32m+[m
[32m+[m[32mend # Grit[m
[1mdiff --git a/grit/lib/grit/repo.rb b/grit/lib/grit/repo.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..9eb3354[m
[1m--- /dev/null[m
[1m+++ b/grit/lib/grit/repo.rb[m
[36m@@ -0,0 +1,482 @@[m
[32m+[m[32mmodule Grit[m
[32m+[m
[32m+[m[32m  class Repo[m
[32m+[m[32m    DAEMON_EXPORT_FILE = 'git-daemon-export-ok'[m
[32m+[m
[32m+[m[32m    # The path of the git repo as a String[m
[32m+[m[32m    attr_accessor :path[m
[32m+[m[32m    attr_accessor :working_dir[m
[32m+[m[32m    attr_reader :bare[m
[32m+[m
[32m+[m[32m    # The git command line interface object[m
[32m+[m[32m    attr_accessor :git[m
[32m+[m
[32m+[m[32m    # Create a new Repo instance[m
[32m+[m[32m    #   +path+ is the path to either the root git directory or the bare git repo[m
[32m+[m[32m    #   +options+ :is_bare force to load a bare repo[m
[32m+[m[32m    #[m
[32m+[m[32m    # Examples[m
[32m+[m[32m    #   g = Repo.new("/Users/tom/dev/grit")[m
[32m+[m[32m    #   g = Repo.new("/Users/tom/public/grit.git")[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Repo[m
[32m+[m[32m    def initialize(path, options = {})[m
[32m+[m[32m      epath = File.expand_path(path)[m
[32m+[m
[32m+[m[32m      if File.exist?(File.join(epath, '.git'))[m
[32m+[m[32m        self.working_dir = epath[m
[32m+[m[32m        self.path = File.join(epath, '.git')[m
[32m+[m[32m        @bare = false[m
[32m+[m[32m      elsif File.exist?(epath) && (epath =~ /\.git$/ || options[:is_bare])[m
[32m+[m[32m        self.path = epath[m
[32m+[m[32m        @bare = true[m
[32m+[m[32m      elsif File.exist?(epath)[m
[32m+[m[32m        raise InvalidGitRepositoryError.new(epath)[m
[32m+[m[32m      else[m
[32m+[m[32m        raise NoSuchPathError.new(epath)[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      self.git = Git.new(self.path)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Does nothing yet...[m
[32m+[m[32m    def self.init(path)[m
[32m+[m[32m      # !! TODO !![m
[32m+[m[32m      # create directory[m
[32m+[m[32m      # generate initial git directory[m
[32m+[m[32m      # create new Grit::Repo on that dir, return it[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # The project's description. Taken verbatim from GIT_REPO/description[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns String[m
[32m+[m[32m    def description[m
[32m+[m[32m      self.git.fs_read('description').chomp[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def blame(file, commit = nil)[m
[32m+[m[32m      Blame.new(self, file, commit)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m
[32m+[m[32m    # An array of Head objects representing the branch heads in[m
[32m+[m[32m    # this repo[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Head[] (baked)[m
[32m+[m[32m    def heads[m
[32m+[m[32m      Head.find_all(self)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    alias_method :branches, :heads[m
[32m+[m
[32m+[m[32m    def get_head(head_name)[m
[32m+[m[32m      heads.find { |h| h.name == head_name }[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def is_head?(head_name)[m
[32m+[m[32m      get_head(head_name)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Object reprsenting the current repo head.[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Head (baked)[m
[32m+[m[32m    def head[m
[32m+[m[32m      Head.current(self)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m
[32m+[m[32m    # Commits current index[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns true/false if commit worked[m
[32m+[m[32m    def commit_index(message)[m
[32m+[m[32m      self.git.commit({}, '-m', message)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Commits all tracked and modified files[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns true/false if commit worked[m
[32m+[m[32m    def commit_all(message)[m
[32m+[m[32m      self.git.commit({}, '-a', '-m', message)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Adds files to the index[m
[32m+[m[32m    def add(*files)[m
[32m+[m[32m      self.git.add({}, *files.flatten)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Remove files from the index[m
[32m+[m[32m    def remove(*files)[m
[32m+[m[32m      self.git.rm({}, *files.flatten)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m
[32m+[m[32m    def blame_tree(commit, path = nil)[m
[32m+[m[32m      commit_array = self.git.blame_tree(commit, path)[m
[32m+[m
[32m+[m[32m      final_array = {}[m
[32m+[m[32m      commit_array.each do |file, sha|[m
[32m+[m[32m        final_array[file] = commit(sha)[m
[32m+[m[32m      end[m
[32m+[m[32m      final_array[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def status[m
[32m+[m[32m      Status.new(self)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m
[32m+[m[32m    # An array of Tag objects that are available in this repo[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Tag[] (baked)[m
[32m+[m[32m    def tags[m
[32m+[m[32m      Tag.find_all(self)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # An array of Remote objects representing the remote branches in[m
[32m+[m[32m    # this repo[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Remote[] (baked)[m
[32m+[m[32m    def remotes[m
[32m+[m[32m      Remote.find_all(self)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def remote_list[m
[32m+[m[32m      self.git.list_remotes[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def remote_add(name, url)[m
[32m+[m[32m      self.git.remote({}, 'add', name, url)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def remote_fetch(name)[m
[32m+[m[32m      self.git.fetch({}, name)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # takes an array of remote names and last pushed dates[m
[32m+[m[32m    # fetches from all of the remotes where the local fetch[m
[32m+[m[32m    # date is earlier than the passed date, then records the[m
[32m+[m[32m    # last fetched date[m
[32m+[m[32m    #[m
[32m+[m[32m    # { 'origin' => date,[m
[32m+[m[32m    #   'peter => date,[m
[32m+[m[32m    # }[m
[32m+[m[32m    def remotes_fetch_needed(remotes)[m
[32m+[m[32m      remotes.each do |remote, date|[m
[32m+[m[32m        # TODO: check against date[m
[32m+[m[32m        self.remote_fetch(remote)[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m
[32m+[m[32m    # An array of Ref objects representing the refs in[m
[32m+[m[32m    # this repo[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Ref[] (baked)[m
[32m+[m[32m    def refs[m
[32m+[m[32m      [ Head.find_all(self), Tag.find_all(self), Remote.find_all(self) ].flatten[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def commit_stats(start = 'master', max_count = 10, skip = 0)[m
[32m+[m[32m      options = {:max_count => max_count,[m
[32m+[m[32m                 :skip => skip}[m
[32m+[m
[32m+[m[32m      CommitStats.find_all(self, start, options)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # An array of Commit objects representing the history of a given ref/commit[m
[32m+[m[32m    #   +start+ is the branch/commit name (default 'master')[m
[32m+[m[32m    #   +max_count+ is the maximum number of commits to return (default 10, use +false+ for all)[m
[32m+[m[32m    #   +skip+ is the number of commits to skip (default 0)[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Commit[] (baked)[m
[32m+[m[32m    def commits(start = 'master', max_count = 10, skip = 0)[m
[32m+[m[32m      options = {:max_count => max_count,[m
[32m+[m[32m                 :skip => skip}[m
[32m+[m
[32m+[m[32m      Commit.find_all(self, start, options)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # The Commits objects that are reachable via +to+ but not via +from+[m
[32m+[m[32m    # Commits are returned in chronological order.[m
[32m+[m[32m    #   +from+ is the branch/commit name of the younger item[m
[32m+[m[32m    #   +to+ is the branch/commit name of the older item[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Commit[] (baked)[m
[32m+[m[32m    def commits_between(from, to)[m
[32m+[m[32m      Commit.find_all(self, "#{from}..#{to}").reverse[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # The Commits objects that are newer than the specified date.[m
[32m+[m[32m    # Commits are returned in chronological order.[m
[32m+[m[32m    #   +start+ is the branch/commit name (default 'master')[m
[32m+[m[32m    #   +since+ is a string represeting a date/time[m
[32m+[m[32m    #   +extra_options+ is a hash of extra options[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Commit[] (baked)[m
[32m+[m[32m    def commits_since(start = 'master', since = '1970-01-01', extra_options = {})[m
[32m+[m[32m      options = {:since => since}.merge(extra_options)[m
[32m+[m
[32m+[m[32m      Commit.find_all(self, start, options)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # The number of commits reachable by the given branch/commit[m
[32m+[m[32m    #   +start+ is the branch/commit name (default 'master')[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Integer[m
[32m+[m[32m    def commit_count(start = 'master')[m
[32m+[m[32m      Commit.count(self, start)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # The Commit object for the specified id[m
[32m+[m[32m    #   +id+ is the SHA1 identifier of the commit[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Commit (baked)[m
[32m+[m[32m    def commit(id)[m
[32m+[m[32m      options = {:max_count => 1}[m
[32m+[m
[32m+[m[32m      Commit.find_all(self, id, options).first[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Returns a list of commits that is in +other_repo+ but not in self[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Commit[][m
[32m+[m[32m    def commit_deltas_from(other_repo, ref = "master", other_ref = "master")[m
[32m+[m[32m      # TODO: we should be able to figure out the branch point, rather than[m
[32m+[m[32m      # rev-list'ing the whole thing[m
[32m+[m[32m      repo_refs       = self.git.rev_list({}, ref).strip.split("\n")[m
[32m+[m[32m      other_repo_refs = other_repo.git.rev_list({}, other_ref).strip.split("\n")[m
[32m+[m
[32m+[m[32m      (other_repo_refs - repo_refs).map do |ref|[m
[32m+[m[32m        Commit.find_all(other_repo, ref, {:max_count => 1}).first[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # The Tree object for the given treeish reference[m
[32m+[m[32m    #   +treeish+ is the reference (default 'master')[m
[32m+[m[32m    #   +paths+ is an optional Array of directory paths to restrict the tree (deafult [])[m
[32m+[m[32m    #[m
[32m+[m[32m    # Examples[m
[32m+[m[32m    #   repo.tree('master', ['lib/'])[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Tree (baked)[m
[32m+[m[32m    def tree(treeish = 'master', paths = [])[m
[32m+[m[32m      Tree.construct(self, treeish, paths)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # The Blob object for the given id[m
[32m+[m[32m    #   +id+ is the SHA1 id of the blob[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Blob (unbaked)[m
[32m+[m[32m    def blob(id)[m
[32m+[m[32m      Blob.create(self, :id => id)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # The commit log for a treeish[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Commit[][m
[32m+[m[32m    def log(commit = 'master', path = nil, options = {})[m
[32m+[m[32m      default_options = {:pretty => "raw"}[m
[32m+[m[32m      actual_options  = default_options.merge(options)[m
[32m+[m[32m      arg = path ? [commit, '--', path] : [commit][m
[32m+[m[32m      commits = self.git.log(actual_options, *arg)[m
[32m+[m[32m      Commit.list_from_string(self, commits)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # The diff from commit +a+ to commit +b+, optionally restricted to the given file(s)[m
[32m+[m[32m    #   +a+ is the base commit[m
[32m+[m[32m    #   +b+ is the other commit[m
[32m+[m[32m    #   +paths+ is an optional list of file paths on which to restrict the diff[m
[32m+[m[32m    def diff(a, b, *paths)[m
[32m+[m[32m      self.git.diff({}, a, b, '--', *paths)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # The commit diff for the given commit[m
[32m+[m[32m    #   +commit+ is the commit name/id[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Diff[][m
[32m+[m[32m    def commit_diff(commit)[m
[32m+[m[32m      Commit.diff(self, commit)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Initialize a bare git repository at the given path[m
[32m+[m[32m    #   +path+ is the full path to the repo (traditionally ends with /<name>.git)[m
[32m+[m[32m    #   +options+ is any additional options to the git init command[m
[32m+[m[32m    #[m
[32m+[m[32m    # Examples[m
[32m+[m[32m    #   Grit::Repo.init_bare('/var/git/myrepo.git')[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Repo (the newly created repo)[m
[32m+[m[32m    def self.init_bare(path, git_options = {}, repo_options = {})[m
[32m+[m[32m      git_options = {:bare => true}.merge(git_options)[m
[32m+[m[32m      git = Git.new(path)[m
[32m+[m[32m      git.fs_mkdir('..')[m
[32m+[m[32m      git.init(git_options)[m
[32m+[m[32m      self.new(path, repo_options)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Fork a bare git repository from this repo[m
[32m+[m[32m    #   +path+ is the full path of the new repo (traditionally ends with /<name>.git)[m
[32m+[m[32m    #   +options+ is any additional options to the git clone command (:bare and :shared are true by default)[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Repo (the newly forked repo)[m
[32m+[m[32m    def fork_bare(path, options = {})[m
[32m+[m[32m      default_options = {:bare => true, :shared => true}[m
[32m+[m[32m      real_options = default_options.merge(options)[m
[32m+[m[32m      Git.new(path).fs_mkdir('..')[m
[32m+[m[32m      self.git.clone(real_options, self.path, path)[m
[32m+[m[32m      Repo.new(path)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Fork a bare git repository from another repo[m
[32m+[m[32m    #   +path+ is the full path of the new repo (traditionally ends with /<name>.git)[m
[32m+[m[32m    #   +options+ is any additional options to the git clone command (:bare and :shared are true by default)[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Repo (the newly forked repo)[m
[32m+[m[32m    def fork_bare_from(path, options = {})[m
[32m+[m[32m      default_options = {:bare => true, :shared => true}[m
[32m+[m[32m      real_options = default_options.merge(options)[m
[32m+[m[32m      Git.new(self.path).fs_mkdir('..')[m
[32m+[m[32m      self.git.clone(real_options, path, self.path)[m
[32m+[m[32m      Repo.new(self.path)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Archive the given treeish[m
[32m+[m[32m    #   +treeish+ is the treeish name/id (default 'master')[m
[32m+[m[32m    #   +prefix+ is the optional prefix[m
[32m+[m[32m    #[m
[32m+[m[32m    # Examples[m
[32m+[m[32m    #   repo.archive_tar[m
[32m+[m[32m    #   # => <String containing tar archive>[m
[32m+[m[32m    #[m
[32m+[m[32m    #   repo.archive_tar('a87ff14')[m
[32m+[m[32m    #   # => <String containing tar archive for commit a87ff14>[m
[32m+[m[32m    #[m
[32m+[m[32m    #   repo.archive_tar('master', 'myproject/')[m
[32m+[m[32m    #   # => <String containing tar archive and prefixed with 'myproject/'>[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns String (containing tar archive)[m
[32m+[m[32m    def archive_tar(treeish = 'master', prefix = nil)[m
[32m+[m[32m      options = {}[m
[32m+[m[32m      options[:prefix] = prefix if prefix[m
[32m+[m[32m      self.git.archive(options, treeish)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Archive and gzip the given treeish[m
[32m+[m[32m    #   +treeish+ is the treeish name/id (default 'master')[m
[32m+[m[32m    #   +prefix+ is the optional prefix[m
[32m+[m[32m    #[m
[32m+[m[32m    # Examples[m
[32m+[m[32m    #   repo.archive_tar_gz[m
[32m+[m[32m    #   # => <String containing tar.gz archive>[m
[32m+[m[32m    #[m
[32m+[m[32m    #   repo.archive_tar_gz('a87ff14')[m
[32m+[m[32m    #   # => <String containing tar.gz archive for commit a87ff14>[m
[32m+[m[32m    #[m
[32m+[m[32m    #   repo.archive_tar_gz('master', 'myproject/')[m
[32m+[m[32m    #   # => <String containing tar.gz archive and prefixed with 'myproject/'>[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns String (containing tar.gz archive)[m
[32m+[m[32m    def archive_tar_gz(treeish = 'master', prefix = nil)[m
[32m+[m[32m      options = {}[m
[32m+[m[32m      options[:prefix] = prefix if prefix[m
[32m+[m[32m      self.git.archive(options, treeish, "| gzip -n")[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Write an archive directly to a file[m
[32m+[m[32m    #   +treeish+ is the treeish name/id (default 'master')[m
[32m+[m[32m    #   +prefix+ is the optional prefix (default nil)[m
[32m+[m[32m    #   +filename+ is the name of the file (default 'archive.tar.gz')[m
[32m+[m[32m    #   +format+ is the optional format (default nil)[m
[32m+[m[32m    #   +pipe+ is the command to run the output through (default 'gzip')[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns nothing[m
[32m+[m[32m    def archive_to_file(treeish = 'master', prefix = nil, filename = 'archive.tar.gz', format = nil, pipe = "gzip")[m
[32m+[m[32m      options = {}[m
[32m+[m[32m      options[:prefix] = prefix if prefix[m
[32m+[m[32m      options[:format] = format if format[m
[32m+[m[32m      self.git.archive(options, treeish, "| #{pipe} > #{filename}")[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Enable git-daemon serving of this repository by writing the[m
[32m+[m[32m    # git-daemon-export-ok file to its git directory[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns nothing[m
[32m+[m[32m    def enable_daemon_serve[m
[32m+[m[32m      self.git.fs_write(DAEMON_EXPORT_FILE, '')[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Disable git-daemon serving of this repository by ensuring there is no[m
[32m+[m[32m    # git-daemon-export-ok file in its git directory[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns nothing[m
[32m+[m[32m    def disable_daemon_serve[m
[32m+[m[32m      self.git.fs_delete(DAEMON_EXPORT_FILE)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def gc_auto[m
[32m+[m[32m      self.git.gc({:auto => true})[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # The list of alternates for this repo[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Array[String] (pathnames of alternates)[m
[32m+[m[32m    def alternates[m
[32m+[m[32m      alternates_path = "objects/info/alternates"[m
[32m+[m[32m      if self.git.fs_exist?(alternates_path)[m
[32m+[m[32m        self.git.fs_read(alternates_path).strip.split("\n")[m
[32m+[m[32m      else[m
[32m+[m[32m        [][m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Sets the alternates[m
[32m+[m[32m    #   +alts+ is the Array of String paths representing the alternates[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns nothing[m
[32m+[m[32m    def alternates=(alts)[m
[32m+[m[32m      alts.each do |alt|[m
[32m+[m[32m        unless File.exist?(alt)[m
[32m+[m[32m          raise "Could not set alternates. Alternate path #{alt} must exist"[m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      if alts.empty?[m
[32m+[m[32m        self.git.fs_write('objects/info/alternates', '')[m
[32m+[m[32m      else[m
[32m+[m[32m        self.git.fs_write('objects/info/alternates', alts.join("\n"))[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def config[m
[32m+[m[32m      @config ||= Config.new(self)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def index[m
[32m+[m[32m      Index.new(self)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def update_ref(head, commit_sha)[m
[32m+[m[32m      return nil if !commit_sha || (commit_sha.size != 40)[m
[32m+[m[32m      self.git.fs_write("refs/heads/#{head}", commit_sha)[m
[32m+[m[32m      commit_sha[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Rename the current repository directory.[m
[32m+[m[32m    #   +name+ is the new name[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns nothing[m
[32m+[m[32m    def rename(name)[m
[32m+[m[32m      if @bare[m
[32m+[m[32m        self.git.fs_move('/', "../#{name}")[m
[32m+[m[32m      else[m
[32m+[m[32m        self.git.fs_move('/', "../../#{name}")[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Pretty object inspection[m
[32m+[m[32m    def inspect[m
[32m+[m[32m      %Q{#<Grit::Repo "#{@path}">}[m
[32m+[m[32m    end[m
[32m+[m[32m  end # Repo[m
[32m+[m
[32m+[m[32mend # Grit[m

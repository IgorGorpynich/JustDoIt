[32m+[m[32m    assert_equal 8, commits.size[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_last_commits_array[m
[32m+[m[32m    setup_a[m
[32m+[m[32m    arr = @findex.last_commits(@commit, ['lib/grit/git.rb', 'lib/grit/actor.rb', 'lib/grit/commit.rb'])[m
[32m+[m[32m    assert_equal '74fd66519e983a0f29e16a342a6059dbffe36020', arr['lib/grit/git.rb'][m
[32m+[m[32m    assert_equal @commit, arr['lib/grit/commit.rb'][m
[32m+[m[32m    assert_equal nil, arr['lib/grit/actor.rb'][m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_last_commits_pattern[m

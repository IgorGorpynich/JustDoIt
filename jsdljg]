[32m+[m
[32m+[m[32m  def test_to_patch[m
[32m+[m[32m    @c = Commit.create(@r, :id => '80f136f500dfdb8c3e8abf4ae716f875f0a1b57f')[m
[32m+[m
[32m+[m[32m    patch = @c.to_patch[m
[32m+[m
[32m+[m[32m    assert patch.include?('From 80f136f500dfdb8c3e8abf4ae716f875f0a1b57f Mon Sep 17 00:00:00 2001')[m
[32m+[m[32m    assert patch.include?('From: tom <tom@taco.(none)>')[m
[32m+[m[32m    assert patch.include?('Date: Tue, 20 Nov 2007 17:27:42 -0800')[m
[32m+[m[32m    assert patch.include?('Subject: [PATCH] fix tests on other machines')[m
[32m+[m[32m    assert patch.include?('test/test_reality.rb |   30 +++++++++++++++---------------')[m
[32m+[m[32m    assert patch.include?('@@ -1,17 +1,17 @@')[m
[32m+[m[32m    assert patch.include?('+#     recurse(t)')[m
[32m+[m[32m    assert patch.include?("1.6.")[m
[32m+[m[32m  end[m

[32m+[m[32mrequire File.dirname(__FILE__) + '/helper'[m
[32m+[m[32mrequire 'pp'[m
[32m+[m
[32m+[m[32mclass TestBlameTree < Test::Unit::TestCase[m
[32m+[m
[32m+[m[32m  def setup[m
[32m+[m[32m    @git = Git.new(File.join(File.dirname(__FILE__), *%w[dot_git]))[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_blame_tree[m
[32m+[m[32m    commit = '2d3acf90f35989df8f262dc50beadc4ee3ae1560'[m
[32m+[m[32m    tree = @git.blame_tree(commit)[m
[32m+[m[32m    last_commit_sha = tree['History.txt'][m
[32m+[m[32m    assert_equal last_commit_sha, '7bcc0ee821cdd133d8a53e8e7173a334fef448aa'[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_blame_tree_path[m
[32m+[m[32m    commit = '2d3acf90f35989df8f262dc50beadc4ee3ae1560'[m
[32m+[m[32m    tree = @git.blame_tree(commit, 'lib')[m
[32m+[m[32m    last_commit_sha = tree['lib/grit.rb'][m
[32m+[m[32m    assert_equal last_commit_sha, '5a0943123f6872e75a9b1dd0b6519dd42a186fda'[m
[32m+[m[32m    last_commit_sha = tree['lib/grit'][m
[32m+[m[32m    assert_equal last_commit_sha, '2d3acf90f35989df8f262dc50beadc4ee3ae1560'[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_blame_tree_multi_path[m
[32m+[m[32m    commit = '2d3acf90f35989df8f262dc50beadc4ee3ae1560'[m
[32m+[m[32m    tree = @git.blame_tree(commit, 'lib/grit')[m
[32m+[m[32m    last_commit_sha = tree['lib/grit/diff.rb'][m
[32m+[m[32m    assert_equal last_commit_sha, '22825175e37f22c9418d756ca69b574d75602994'[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32mend[m

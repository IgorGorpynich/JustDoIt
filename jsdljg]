[1mdiff --git a/grit/lib/grit/actor.rb b/grit/lib/grit/actor.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..61b00d9[m
[1m--- /dev/null[m
[1m+++ b/grit/lib/grit/actor.rb[m
[36m@@ -0,0 +1,36 @@[m
[32m+[m[32mmodule Grit[m
[32m+[m
[32m+[m[32m  class Actor[m
[32m+[m[32m    attr_reader :name[m
[32m+[m[32m    attr_reader :email[m
[32m+[m
[32m+[m[32m    def initialize(name, email)[m
[32m+[m[32m      @name = name[m
[32m+[m[32m      @email = email[m
[32m+[m[32m    end[m
[32m+[m[32m    alias_method :to_s, :name[m
[32m+[m
[32m+[m[32m    # Create an Actor from a string.[m
[32m+[m[32m    #   +str+ is the string, which is expected to be in regular git format[m
[32m+[m[32m    #[m
[32m+[m[32m    # Format[m
[32m+[m[32m    #   John Doe <jdoe@example.com>[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Actor[m
[32m+[m[32m    def self.from_string(str)[m
[32m+[m[32m      case str[m
[32m+[m[32m        when /<.+>/[m
[32m+[m[32m          m, name, email = *str.match(/(.*) <(.+?)>/)[m
[32m+[m[32m          return self.new(name, email)[m
[32m+[m[32m        else[m
[32m+[m[32m          return self.new(str, nil)[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Pretty object inspection[m
[32m+[m[32m    def inspect[m
[32m+[m[32m      %Q{#<Grit::Actor "#{@name} <#{@email}>">}[m
[32m+[m[32m    end[m
[32m+[m[32m  end # Actor[m
[32m+[m
[32m+[m[32mend # Grit[m
[1mdiff --git a/grit/lib/grit/blame.rb b/grit/lib/grit/blame.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..2869ce6[m
[1m--- /dev/null[m
[1m+++ b/grit/lib/grit/blame.rb[m
[36m@@ -0,0 +1,61 @@[m
[32m+[m[32mmodule Grit[m
[32m+[m
[32m+[m[32m  class Blame[m
[32m+[m
[32m+[m[32m    attr_reader :lines[m
[32m+[m
[32m+[m[32m    def initialize(repo, file, commit)[m
[32m+[m[32m      @repo = repo[m
[32m+[m[32m      @file = file[m
[32m+[m[32m      @commit = commit[m
[32m+[m[32m      @lines = [][m
[32m+[m[32m      load_blame[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def load_blame[m
[32m+[m[32m      output = @repo.git.blame({'p' => true}, @commit, '--', @file)[m
[32m+[m[32m      process_raw_blame(output)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def process_raw_blame(output)[m
[32m+[m[32m      lines, final = [], [][m
[32m+[m[32m      info, commits = {}, {}[m
[32m+[m
[32m+[m[32m      # process the output[m
[32m+[m[32m      output.split("\n").each do |line|[m
[32m+[m[32m        if line[0, 1] == "\t"[m
[32m+[m[32m          lines << line[1, line.size][m
[32m+[m[32m        elsif m = /^(\w{40}) (\d+) (\d+)/.match(line)[m
[32m+[m[32m          if !commits[m[1]][m
[32m+[m[32m            commits[m[1]] = @repo.commit(m[1])[m
[32m+[m[32m          end[m
[32m+[m[32m          info[m[3].to_i] = [commits[m[1]], m[2].to_i][m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      # get it together[m
[32m+[m[32m      info.sort.each do |lineno, commit|[m
[32m+[m[32m        final << BlameLine.new(lineno, commit[1], commit[0], lines[lineno - 1])[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      @lines = final[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Pretty object inspection[m
[32m+[m[32m    def inspect[m
[32m+[m[32m      %Q{#<Grit::Blame "#{@file} <#{@commit}>">}[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    class BlameLine[m
[32m+[m[32m      attr_accessor :lineno, :oldlineno, :commit, :line[m
[32m+[m[32m      def initialize(lineno, oldlineno, commit, line)[m
[32m+[m[32m        @lineno = lineno[m
[32m+[m[32m        @oldlineno = oldlineno[m
[32m+[m[32m        @commit = commit[m
[32m+[m[32m        @line = line[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m  end # Blame[m
[32m+[m
[32m+[m[32mend # Grit[m
\ No newline at end of file[m
[1mdiff --git a/grit/lib/grit/blob.rb b/grit/lib/grit/blob.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..9593d56[m
[1m--- /dev/null[m
[1m+++ b/grit/lib/grit/blob.rb[m
[36m@@ -0,0 +1,126 @@[m
[32m+[m[32mmodule Grit[m
[32m+[m
[32m+[m[32m  class Blob[m
[32m+[m[32m    DEFAULT_MIME_TYPE = "text/plain"[m
[32m+[m
[32m+[m[32m    attr_reader :id[m
[32m+[m[32m    attr_reader :mode[m
[32m+[m[32m    attr_reader :name[m
[32m+[m
[32m+[m[32m    # Create an unbaked Blob containing just the specified attributes[m
[32m+[m[32m    #   +repo+ is the Repo[m
[32m+[m[32m    #   +atts+ is a Hash of instance variable data[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Blob (unbaked)[m
[32m+[m[32m    def self.create(repo, atts)[m
[32m+[m[32m      self.allocate.create_initialize(repo, atts)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Initializer for Blob.create[m
[32m+[m[32m    #   +repo+ is the Repo[m
[32m+[m[32m    #   +atts+ is a Hash of instance variable data[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Blob (unbaked)[m
[32m+[m[32m    def create_initialize(repo, atts)[m
[32m+[m[32m      @repo = repo[m
[32m+[m[32m      atts.each do |k, v|[m
[32m+[m[32m        instance_variable_set("@#{k}".to_sym, v)[m
[32m+[m[32m      end[m
[32m+[m[32m      self[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # The size of this blob in bytes[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Integer[m
[32m+[m[32m    def size[m
[32m+[m[32m      @size ||= @repo.git.cat_file({:s => true}, id).chomp.to_i[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # The binary contents of this blob.[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns String[m
[32m+[m[32m    def data[m
[32m+[m[32m      @data ||= @repo.git.cat_file({:p => true}, id)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # The mime type of this file (based on the filename)[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns String[m
[32m+[m[32m    def mime_type[m
[32m+[m[32m      guesses = MIME::Types.type_for(self.name) rescue [][m
[32m+[m[32m      guesses.first ? guesses.first.simplified : DEFAULT_MIME_TYPE[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # The blame information for the given file at the given commit[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Array: [Grit::Commit, Array: [<line>]][m
[32m+[m[32m    def self.blame(repo, commit, file)[m
[32m+[m[32m      data = repo.git.blame({:p => true}, commit, '--', file)[m
[32m+[m
[32m+[m[32m      commits = {}[m
[32m+[m[32m      blames = [][m
[32m+[m[32m      info = nil[m
[32m+[m
[32m+[m[32m      data.split("\n").each do |line|[m
[32m+[m[32m        parts = line.split(/\s+/, 2)[m
[32m+[m[32m        case parts.first[m
[32m+[m[32m          when /^[0-9A-Fa-f]{40}$/[m
[32m+[m[32m            case line[m
[32m+[m[32m              when /^([0-9A-Fa-f]{40}) (\d+) (\d+) (\d+)$/[m
[32m+[m[32m                _, id, origin_line, final_line, group_lines = *line.match(/^([0-9A-Fa-f]{40}) (\d+) (\d+) (\d+)$/)[m
[32m+[m[32m                info = {:id => id}[m
[32m+[m[32m                blames << [nil, []][m
[32m+[m[32m              when /^([0-9A-Fa-f]{40}) (\d+) (\d+)$/[m
[32m+[m[32m                _, id, origin_line, final_line = *line.match(/^([0-9A-Fa-f]{40}) (\d+) (\d+)$/)[m
[32m+[m[32m                info = {:id => id}[m
[32m+[m[32m            end[m
[32m+[m[32m          when /^(author|committer)/[m
[32m+[m[32m            case parts.first[m
[32m+[m[32m              when /^(.+)-mail$/[m
[32m+[m[32m                info["#{$1}_email".intern] = parts.last[m
[32m+[m[32m              when /^(.+)-time$/[m
[32m+[m[32m                info["#{$1}_date".intern] = Time.at(parts.last.to_i)[m
[32m+[m[32m              when /^(author|committer)$/[m
[32m+[m[32m                info[$1.intern] = parts.last[m
[32m+[m[32m            end[m
[32m+[m[32m          when /^filename/[m
[32m+[m[32m            info[:filename] = parts.last[m
[32m+[m[32m          when /^summary/[m
[32m+[m[32m            info[:summary] = parts.last[m
[32m+[m[32m          when ''[m
[32m+[m[32m            c = commits[info[:id]][m
[32m+[m[32m            unless c[m
[32m+[m[32m              c = Commit.create(repo, :id => info[:id],[m
[32m+[m[32m                                      :author => Actor.from_string(info[:author] + ' ' + info[:author_email]),[m
[32m+[m[32m                                      :authored_date => info[:author_date],[m
[32m+[m[32m                                      :committer => Actor.from_string(info[:committer] + ' ' + info[:committer_email]),[m
[32m+[m[32m                                      :committed_date => info[:committer_date],[m
[32m+[m[32m                                      :message => info[:summary])[m
[32m+[m[32m              commits[info[:id]] = c[m
[32m+[m[32m            end[m
[32m+[m[32m            _, text = *line.match(/^\t(.*)$/)[m
[32m+[m[32m            blames.last[0] = c[m
[32m+[m[32m            blames.last[1] << text[m
[32m+[m[32m            info = nil[m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      blames[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def basename[m
[32m+[m[32m      File.basename(name)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Pretty object inspection[m
[32m+[m[32m    def inspect[m
[32m+[m[32m      %Q{#<Grit::Blob "#{@id}">}[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Compares blobs by name[m
[32m+[m[32m    def <=>(other)[m
[32m+[m[32m      name <=> other.name[m
[32m+[m[32m    end[m
[32m+[m[32m  end # Blob[m
[32m+[m
[32m+[m[32mend # Grit[m
\ No newline at end of file[m
[1mdiff --git a/grit/lib/grit/commit.rb b/grit/lib/grit/commit.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..fe6f15c[m
[1m--- /dev/null[m
[1m+++ b/grit/lib/grit/commit.rb[m
[36m@@ -0,0 +1,247 @@[m
[32m+[m[32mmodule Grit[m
[32m+[m
[32m+[m[32m  class Commit[m
[32m+[m[32m    attr_reader :id[m
[32m+[m[32m    lazy_reader :parents[m
[32m+[m[32m    lazy_reader :tree[m
[32m+[m[32m    lazy_reader :author[m
[32m+[m[32m    lazy_reader :authored_date[m
[32m+[m[32m    lazy_reader :committer[m
[32m+[m[32m    lazy_reader :committed_date[m
[32m+[m[32m    lazy_reader :message[m
[32m+[m[32m    lazy_reader :short_message[m
[32m+[m[32m    lazy_reader :author_string[m
[32m+[m
[32m+[m[32m    # Instantiate a new Commit[m
[32m+[m[32m    #   +id+ is the id of the commit[m
[32m+[m[32m    #   +parents+ is an array of commit ids (will be converted into Commit instances)[m
[32m+[m[32m    #   +tree+ is the correspdonding tree id (will be converted into a Tree object)[m
[32m+[m[32m    #   +author+ is the author string[m
[32m+[m[32m    #   +authored_date+ is the authored Time[m
[32m+[m[32m    #   +committer+ is the committer string[m
[32m+[m[32m    #   +committed_date+ is the committed Time[m
[32m+[m[32m    #   +message+ is an array of commit message lines[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Commit (baked)[m
[32m+[m[32m    def initialize(repo, id, parents, tree, author, authored_date, committer, committed_date, message)[m
[32m+[m[32m      @repo = repo[m
[32m+[m[32m      @id = id[m
[32m+[m[32m      @parents = parents.map { |p| Commit.create(repo, :id => p) }[m
[32m+[m[32m      @tree = Tree.create(repo, :id => tree)[m
[32m+[m[32m      @author = author[m
[32m+[m[32m      @authored_date = authored_date[m
[32m+[m[32m      @committer = committer[m
[32m+[m[32m      @committed_date = committed_date[m
[32m+[m[32m      @message = message.join("\n")[m
[32m+[m[32m      @short_message = message.select { |x| !x.strip.empty? }[0] || ''[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def id_abbrev[m
[32m+[m[32m      @id_abbrev ||= @repo.git.rev_parse({}, self.id).chomp[0, 7][m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Create an unbaked Commit containing just the specified attributes[m
[32m+[m[32m    #   +repo+ is the Repo[m
[32m+[m[32m    #   +atts+ is a Hash of instance variable data[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Commit (unbaked)[m
[32m+[m[32m    def self.create(repo, atts)[m
[32m+[m[32m      self.allocate.create_initialize(repo, atts)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Initializer for Commit.create[m
[32m+[m[32m    #   +repo+ is the Repo[m
[32m+[m[32m    #   +atts+ is a Hash of instance variable data[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Commit (unbaked)[m
[32m+[m[32m    def create_initialize(repo, atts)[m
[32m+[m[32m      @repo = repo[m
[32m+[m[32m      atts.each do |k, v|[m
[32m+[m[32m        instance_variable_set("@#{k}", v)[m
[32m+[m[32m      end[m
[32m+[m[32m      self[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def lazy_source[m
[32m+[m[32m      self.class.find_all(@repo, @id, {:max_count => 1}).first[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Count the number of commits reachable from this ref[m
[32m+[m[32m    #   +repo+ is the Repo[m
[32m+[m[32m    #   +ref+ is the ref from which to begin (SHA1 or name)[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Integer[m
[32m+[m[32m    def self.count(repo, ref)[m
[32m+[m[32m      repo.git.rev_list({}, ref).size / 41[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Find all commits matching the given criteria.[m
[32m+[m[32m    #   +repo+ is the Repo[m
[32m+[m[32m    #   +ref+ is the ref from which to begin (SHA1 or name) or nil for --all[m
[32m+[m[32m    #   +options+ is a Hash of optional arguments to git[m
[32m+[m[32m    #     :max_count is the maximum number of commits to fetch[m
[32m+[m[32m    #     :skip is the number of commits to skip[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Commit[] (baked)[m
[32m+[m[32m    def self.find_all(repo, ref, options = {})[m
[32m+[m[32m      allowed_options = [:max_count, :skip, :since][m
[32m+[m
[32m+[m[32m      default_options = {:pretty => "raw"}[m
[32m+[m[32m      actual_options = default_options.merge(options)[m
[32m+[m
[32m+[m[32m      if ref[m
[32m+[m[32m        output = repo.git.rev_list(actual_options, ref)[m
[32m+[m[32m      else[m
[32m+[m[32m        output = repo.git.rev_list(actual_options.merge(:all => true))[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      self.list_from_string(repo, output)[m
[32m+[m[32m    rescue Grit::GitRuby::Repository::NoSuchShaFound[m
[32m+[m[32m      [][m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Parse out commit information into an array of baked Commit objects[m
[32m+[m[32m    #   +repo+ is the Repo[m
[32m+[m[32m    #   +text+ is the text output from the git command (raw format)[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Commit[] (baked)[m
[32m+[m[32m    #[m
[32m+[m[32m    # really should re-write this to be more accepting of non-standard commit messages[m
[32m+[m[32m    # - it broke when 'encoding' was introduced - not sure what else might show up[m
[32m+[m[32m    #[m
[32m+[m[32m    def self.list_from_string(repo, text)[m
[32m+[m[32m      lines = text.split("\n")[m
[32m+[m
[32m+[m[32m      commits = [][m
[32m+[m
[32m+[m[32m      while !lines.empty?[m
[32m+[m[32m        id = lines.shift.split.last[m
[32m+[m[32m        tree = lines.shift.split.last[m
[32m+[m
[32m+[m[32m        parents = [][m
[32m+[m[32m        parents << lines.shift.split.last while lines.first =~ /^parent/[m
[32m+[m
[32m+[m[32m        author, authored_date = self.actor(lines.shift)[m
[32m+[m[32m        committer, committed_date = self.actor(lines.shift)[m
[32m+[m
[32m+[m[32m        # not doing anything with this yet, but it's sometimes there[m
[32m+[m[32m        encoding = lines.shift.split.last if lines.first =~ /^encoding/[m
[32m+[m
[32m+[m[32m        lines.shift[m
[32m+[m
[32m+[m[32m        message_lines = [][m
[32m+[m[32m        message_lines << lines.shift[4..-1] while lines.first =~ /^ {4}/[m
[32m+[m
[32m+[m[32m        lines.shift while lines.first && lines.first.empty?[m
[32m+[m
[32m+[m[32m        commits << Commit.new(repo, id, parents, tree, author, authored_date, committer, committed_date, message_lines)[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      commits[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Show diffs between two trees:[m
[32m+[m[32m    #   +repo+ is the Repo[m
[32m+[m[32m    #   +a+ is a named commit[m
[32m+[m[32m    #   +b+ is an optional named commit.  Passing an array assumes you[m
[32m+[m[32m    #     wish to omit the second named commit and limit the diff to the[m
[32m+[m[32m    #     given paths.[m
[32m+[m[32m    #   +paths* is an array of paths to limit the diff.[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Diff[] (baked)[m
[32m+[m[32m    def self.diff(repo, a, b = nil, paths = [])[m
[32m+[m[32m      if b.is_a?(Array)[m
[32m+[m[32m        paths = b[m
[32m+[m[32m        b     = nil[m
[32m+[m[32m      end[m
[32m+[m[32m      paths.unshift("--") unless paths.empty?[m
[32m+[m[32m      paths.unshift(b)    unless b.nil?[m
[32m+[m[32m      paths.unshift(a)[m
[32m+[m[32m      text = repo.git.diff({:full_index => true}, *paths)[m
[32m+[m[32m      Diff.list_from_string(repo, text)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def show[m
[32m+[m[32m      if parents.size > 1[m
[32m+[m[32m        diff = @repo.git.native("diff #{parents[0].id}...#{parents[1].id}", {:full_index => true})[m
[32m+[m[32m      else[m
[32m+[m[32m        diff = @repo.git.show({:full_index => true, :pretty => 'raw'}, @id)[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      if diff =~ /diff --git a/[m
[32m+[m[32m        diff = diff.sub(/.+?(diff --git a)/m, '\1')[m
[32m+[m[32m      else[m
[32m+[m[32m        diff = ''[m
[32m+[m[32m      end[m
[32m+[m[32m      Diff.list_from_string(@repo, diff)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def diffs[m
[32m+[m[32m      if parents.empty?[m
[32m+[m[32m        show[m
[32m+[m[32m      else[m
[32m+[m[32m        self.class.diff(@repo, parents.first.id, @id)[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def stats[m
[32m+[m[32m      stats = @repo.commit_stats(self.sha, 1)[0][-1][m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Convert this Commit to a String which is just the SHA1 id[m
[32m+[m[32m    def to_s[m
[32m+[m[32m      @id[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def sha[m
[32m+[m[32m      @id[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def date[m
[32m+[m[32m      @committed_date[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def to_patch[m
[32m+[m[32m      @repo.git.format_patch({'1' => true, :stdout => true}, to_s)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Pretty object inspection[m
[32m+[m[32m    def inspect[m
[32m+[m[32m      %Q{#<Grit::Commit "#{@id}">}[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # private[m
[32m+[m
[32m+[m[32m    # Parse out the actor (author or committer) info[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns [String (actor name and email), Time (acted at time)][m
[32m+[m[32m    def self.actor(line)[m
[32m+[m[32m      m, actor, epoch = *line.match(/^.+? (.*) (\d+) .*$/)[m
[32m+[m[32m      [Actor.from_string(actor), Time.at(epoch.to_i)][m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def author_string[m
[32m+[m[32m      "%s <%s> %s %+05d" % [author.name, author.email, authored_date.to_i, 800][m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def to_hash[m
[32m+[m[32m      {[m
[32m+[m[32m        'id'       => id,[m
[32m+[m[32m        'parents'  => parents.map { |p| { 'id' => p.id } },[m
[32m+[m[32m        'tree'     => tree.id,[m
[32m+[m[32m        'message'  => message,[m
[32m+[m[32m        'author'   => {[m
[32m+[m[32m          'name'  => author.name,[m
[32m+[m[32m          'email' => author.email[m
[32m+[m[32m        },[m
[32m+[m[32m        'committer' => {[m
[32m+[m[32m          'name'  => committer.name,[m
[32m+[m[32m          'email' => committer.email[m
[32m+[m[32m        },[m
[32m+[m[32m        'authored_date'  => authored_date.xmlschema,[m
[32m+[m[32m        'committed_date' => committed_date.xmlschema,[m
[32m+[m[32m      }[m
[32m+[m[32m    end[m
[32m+[m[32m  end # Commit[m
[32m+[m
[32m+[m[32mend # Grit[m
[1mdiff --git a/grit/lib/grit/commit_stats.rb b/grit/lib/grit/commit_stats.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..f7213a0[m
[1m--- /dev/null[m
[1m+++ b/grit/lib/grit/commit_stats.rb[m
[36m@@ -0,0 +1,128 @@[m
[32m+[m[32mmodule Grit[m
[32m+[m
[32m+[m[32m  class CommitStats[m
[32m+[m
[32m+[m[32m    attr_reader :id, :files, :additions, :deletions, :total[m
[32m+[m
[32m+[m[32m    # Instantiate a new CommitStats[m
[32m+[m[32m    #   +id+ is the id of the commit[m
[32m+[m[32m    #   +files+ is an array of :[m
[32m+[m[32m    #     [ [filename, adds, deletes, total],[m
[32m+[m[32m    #       [filename, adds, deletes, total],[m
[32m+[m[32m    #       [filename, adds, deletes, total] ][m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::CommitStats (baked)[m
[32m+[m[32m    def initialize(repo, id, files)[m
[32m+[m[32m      @repo = repo[m
[32m+[m[32m      @id = id[m
[32m+[m[32m      @files = files[m
[32m+[m[32m      @additions  = files.inject(0) { |total, a| total += a[1] }[m
[32m+[m[32m      @deletions  = files.inject(0) { |total, a| total += a[2] }[m
[32m+[m[32m      @total  = files.inject(0) { |total, a| total += a[3] }[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Find all commit stats matching the given criteria.[m
[32m+[m[32m    #   +repo+ is the Repo[m
[32m+[m[32m    #   +ref+ is the ref from which to begin (SHA1 or name) or nil for --all[m
[32m+[m[32m    #   +options+ is a Hash of optional arguments to git[m
[32m+[m[32m    #     :max_count is the maximum number of commits to fetch[m
[32m+[m[32m    #     :skip is the number of commits to skip[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns assoc array [sha, Grit::Commit[] (baked)][m
[32m+[m[32m    def self.find_all(repo, ref, options = {})[m
[32m+[m[32m      allowed_options = [:max_count, :skip, :since][m
[32m+[m
[32m+[m[32m      default_options = {:numstat => true}[m
[32m+[m[32m      actual_options = default_options.merge(options)[m
[32m+[m
[32m+[m[32m      if ref[m
[32m+[m[32m        output = repo.git.log(actual_options, ref)[m
[32m+[m[32m      else[m
[32m+[m[32m        output = repo.git.log(actual_options.merge(:all => true))[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      self.list_from_string(repo, output)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Parse out commit information into an array of baked Commit objects[m
[32m+[m[32m    #   +repo+ is the Repo[m
[32m+[m[32m    #   +text+ is the text output from the git command (raw format)[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns assoc array [sha, Grit::Commit[] (baked)][m
[32m+[m[32m    def self.list_from_string(repo, text)[m
[32m+[m[32m      lines = text.split("\n")[m
[32m+[m
[32m+[m[32m      commits = [][m
[32m+[m
[32m+[m[32m      while !lines.empty?[m
[32m+[m[32m        id = lines.shift.split.last[m
[32m+[m
[32m+[m[32m        lines.shift[m
[32m+[m[32m        lines.shift[m
[32m+[m[32m        lines.shift[m
[32m+[m
[32m+[m[32m        message_lines = [][m
[32m+[m[32m        message_lines << lines.shift[4..-1] while lines.first =~ /^ {4}/ || lines.first == ''[m
[32m+[m
[32m+[m[32m        lines.shift while lines.first && lines.first.empty?[m
[32m+[m
[32m+[m[32m        files = [][m
[32m+[m[32m        while lines.first =~ /^([-\d]+)\s+([-\d]+)\s+(.+)/[m
[32m+[m[32m          (additions, deletions, filename) = lines.shift.split[m
[32m+[m[32m          additions = additions.to_i[m
[32m+[m[32m          deletions = deletions.to_i[m
[32m+[m[32m          total = additions + deletions[m
[32m+[m[32m          files << [filename, additions, deletions, total][m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        lines.shift while lines.first && lines.first.empty?[m
[32m+[m
[32m+[m[32m        commits << [id, CommitStats.new(repo, id, files)][m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      commits[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Pretty object inspection[m
[32m+[m[32m    def inspect[m
[32m+[m[32m      %Q{#<Grit::CommitStats "#{@id}">}[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Convert to an easy-to-traverse structure[m
[32m+[m[32m    def to_diffstat[m
[32m+[m[32m      files.map do |metadata|[m
[32m+[m[32m        DiffStat.new(*metadata)[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # private[m
[32m+[m
[32m+[m[32m    def to_hash[m
[32m+[m[32m      {[m
[32m+[m[32m        'id'        => id,[m
[32m+[m[32m        'files'     => files,[m
[32m+[m[32m        'additions' => additions,[m
[32m+[m[32m        'deletions' => deletions,[m
[32m+[m[32m        'total'     => total[m
[32m+[m[32m      }[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m  end # CommitStats[m
[32m+[m
[32m+[m[32m  class DiffStat[m
[32m+[m[32m    attr_reader :filename, :additions, :deletions[m
[32m+[m
[32m+[m[32m    def initialize(filename, additions, deletions, total=nil)[m
[32m+[m[32m      @filename, @additions, @deletions = filename, additions, deletions[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def net[m
[32m+[m[32m      additions - deletions[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def inspect[m
[32m+[m[32m      "#{filename}: +#{additions} -#{deletions}"[m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32mend # Grit[m
[1mdiff --git a/grit/lib/grit/config.rb b/grit/lib/grit/config.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..332a7da[m
[1m--- /dev/null[m
[1m+++ b/grit/lib/grit/config.rb[m
[36m@@ -0,0 +1,44 @@[m
[32m+[m[32mmodule Grit[m
[32m+[m
[32m+[m[32m  class Config[m
[32m+[m[32m    def initialize(repo)[m
[32m+[m[32m      @repo = repo[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def []=(key, value)[m
[32m+[m[32m      @repo.git.config({}, key, value)[m
[32m+[m[32m      @data = nil[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def [](key)[m
[32m+[m[32m      data[key][m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def fetch(key, default = nil)[m
[32m+[m[32m      data[key] || default || raise(IndexError.new("key not found"))[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def keys[m
[32m+[m[32m      data.keys[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    protected[m
[32m+[m[32m      def data[m
[32m+[m[32m        @data ||= load_config[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      def load_config[m
[32m+[m[32m        hash = {}[m
[32m+[m[32m        config_lines.map do |line|[m
[32m+[m[32m          key, value = line.split(/=/, 2)[m
[32m+[m[32m          hash[key] = value[m
[32m+[m[32m        end[m
[32m+[m[32m        hash[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      def config_lines[m
[32m+[m[32m        @repo.git.config(:list => true).split(/\n/)[m
[32m+[m[32m      end[m
[32m+[m[32m  end # Config[m
[32m+[m
[32m+[m[32mend # Grit[m
\ No newline at end of file[m
[1mdiff --git a/grit/lib/grit/diff.rb b/grit/lib/grit/diff.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..7290f7a[m
[1m--- /dev/null[m
[1m+++ b/grit/lib/grit/diff.rb[m
[36m@@ -0,0 +1,70 @@[m
[32m+[m[32mmodule Grit[m
[32m+[m
[32m+[m[32m  class Diff[m
[32m+[m[32m    attr_reader :a_path, :b_path[m
[32m+[m[32m    attr_reader :a_blob, :b_blob[m
[32m+[m[32m    attr_reader :a_mode, :b_mode[m
[32m+[m[32m    attr_reader :new_file, :deleted_file[m
[32m+[m[32m    attr_reader :diff[m
[32m+[m
[32m+[m[32m    def initialize(repo, a_path, b_path, a_blob, b_blob, a_mode, b_mode, new_file, deleted_file, diff)[m
[32m+[m[32m      @repo = repo[m
[32m+[m[32m      @a_path = a_path[m
[32m+[m[32m      @b_path = b_path[m
[32m+[m[32m      @a_blob = a_blob =~ /^0{40}$/ ? nil : Blob.create(repo, :id => a_blob)[m
[32m+[m[32m      @b_blob = b_blob =~ /^0{40}$/ ? nil : Blob.create(repo, :id => b_blob)[m
[32m+[m[32m      @a_mode = a_mode[m
[32m+[m[32m      @b_mode = b_mode[m
[32m+[m[32m      @new_file = new_file || @a_blob.nil?[m
[32m+[m[32m      @deleted_file = deleted_file || @b_blob.nil?[m
[32m+[m[32m      @diff = diff[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def self.list_from_string(repo, text)[m
[32m+[m[32m      lines = text.split("\n")[m
[32m+[m
[32m+[m[32m      diffs = [][m
[32m+[m
[32m+[m[32m      while !lines.empty?[m
[32m+[m[32m        m, a_path, b_path = *lines.shift.match(%r{^diff --git a/(.+?) b/(.+)$})[m
[32m+[m
[32m+[m[32m        if lines.first =~ /^old mode/[m
[32m+[m[32m          m, a_mode = *lines.shift.match(/^old mode (\d+)/)[m
[32m+[m[32m          m, b_mode = *lines.shift.match(/^new mode (\d+)/)[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        if lines.empty? || lines.first =~ /^diff --git/[m
[32m+[m[32m          diffs << Diff.new(repo, a_path, b_path, nil, nil, a_mode, b_mode, false, false, nil)[m
[32m+[m[32m          next[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        new_file = false[m
[32m+[m[32m        deleted_file = false[m
[32m+[m
[32m+[m[32m        if lines.first =~ /^new file/[m
[32m+[m[32m          m, b_mode = lines.shift.match(/^new file mode (.+)$/)[m
[32m+[m[32m          a_mode = nil[m
[32m+[m[32m          new_file = true[m
[32m+[m[32m        elsif lines.first =~ /^deleted file/[m
[32m+[m[32m          m, a_mode = lines.shift.match(/^deleted file mode (.+)$/)[m
[32m+[m[32m          b_mode = nil[m
[32m+[m[32m          deleted_file = true[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        m, a_blob, b_blob, b_mode = *lines.shift.match(%r{^index ([0-9A-Fa-f]+)\.\.([0-9A-Fa-f]+) ?(.+)?$})[m
[32m+[m[32m        b_mode.strip! if b_mode[m
[32m+[m
[32m+[m[32m        diff_lines = [][m
[32m+[m[32m        while lines.first && lines.first !~ /^diff/[m
[32m+[m[32m          diff_lines << lines.shift[m
[32m+[m[32m        end[m
[32m+[m[32m        diff = diff_lines.join("\n")[m
[32m+[m
[32m+[m[32m        diffs << Diff.new(repo, a_path, b_path, a_blob, b_blob, a_mode, b_mode, new_file, deleted_file, diff)[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      diffs[m
[32m+[m[32m    end[m
[32m+[m[32m  end # Diff[m
[32m+[m
[32m+[m[32mend # Grit[m
[1mdiff --git a/grit/lib/grit/errors.rb b/grit/lib/grit/errors.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..2947a34[m
[1m--- /dev/null[m
[1m+++ b/grit/lib/grit/errors.rb[m
[36m@@ -0,0 +1,7 @@[m
[32m+[m[32mmodule Grit[m
[32m+[m[32m  class InvalidGitRepositoryError < StandardError[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  class NoSuchPathError < StandardError[m
[32m+[m[32m  end[m
[32m+[m[32mend[m
\ No newline at end of file[m
[1mdiff --git a/grit/lib/grit/git-ruby.rb b/grit/lib/grit/git-ruby.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..1cb27f0[m
[1m--- /dev/null[m
[1m+++ b/grit/lib/grit/git-ruby.rb[m
[36m@@ -0,0 +1,267 @@[m
[32m+[m[32mrequire 'grit/git-ruby/repository'[m
[32m+[m[32mrequire 'grit/git-ruby/file_index'[m
[32m+[m
[32m+[m[32mmodule Grit[m
[32m+[m
[32m+[m[32m  # the functions in this module intercept the calls to git binary[m
[32m+[m[32m  # made buy the grit objects and attempts to run them in pure ruby[m
[32m+[m[32m  # if it will be faster, or if the git binary is not available (!!TODO!!)[m
[32m+[m[32m  module GitRuby[m
[32m+[m
[32m+[m[32m    attr_accessor :ruby_git_repo, :git_file_index[m
[32m+[m
[32m+[m[32m    def init(options)[m
[32m+[m[32m      if options.size == 0[m
[32m+[m[32m        Grit::GitRuby::Repository.init(@git_dir)[m
[32m+[m[32m      else[m
[32m+[m[32m        method_missing('init', options)[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def cat_file(options, ref)[m
[32m+[m[32m      if options[:t][m
[32m+[m[32m        file_type(ref)[m
[32m+[m[32m      elsif options[:s][m
[32m+[m[32m        file_size(ref)[m
[32m+[m[32m      elsif options[:p][m
[32m+[m[32m        try_run { ruby_git.cat_file(ref) }[m
[32m+[m[32m      end[m
[32m+[m[32m    rescue Grit::GitRuby::Repository::NoSuchShaFound[m
[32m+[m[32m      ''[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # lib/grit/tree.rb:16:      output = repo.git.ls_tree({}, treeish, *paths)[m
[32m+[m[32m    def ls_tree(options, treeish, *paths)[m
[32m+[m[32m      sha = rev_parse({}, treeish)[m
[32m+[m[32m      ruby_git.ls_tree(sha, paths.flatten, options.delete(:r))[m
[32m+[m[32m    rescue Grit::GitRuby::Repository::NoSuchShaFound[m
[32m+[m[32m      ''[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # git diff --full-index 'ec037431382e83c3e95d4f2b3d145afbac8ea55d' 'f1ec1aea10986159456846b8a05615b87828d6c6'[m
[32m+[m[32m    def diff(options, sha1, sha2 = nil)[m
[32m+[m[32m      try_run { ruby_git.diff(sha1, sha2, options) }[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def rev_list(options, ref = 'master')[m
[32m+[m[32m      options.delete(:skip) if options[:skip].to_i == 0[m
[32m+[m[32m      allowed_options = [:max_count, :since, :until, :pretty]  # this is all I can do right now[m
[32m+[m[32m      if ((options.keys - allowed_options).size > 0)[m
[32m+[m[32m        return method_missing('rev-list', options, ref)[m
[32m+[m[32m      elsif (options.size == 0)[m
[32m+[m[32m        # pure rev-list[m
[32m+[m[32m        begin[m
[32m+[m[32m          return file_index.commits_from(rev_parse({}, ref)).join("\n") + "\n"[m
[32m+[m[32m        rescue[m
[32m+[m[32m          return method_missing('rev-list', options, ref)[m
[32m+[m[32m        end[m
[32m+[m[32m      else[m
[32m+[m[32m        aref = rev_parse({}, ref)[m
[32m+[m[32m        if aref.is_a? Array[m
[32m+[m[32m          return method_missing('rev-list', options, ref)[m
[32m+[m[32m        else[m
[32m+[m[32m          return try_run { ruby_git.rev_list(aref, options) }[m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def rev_parse(options, string)[m
[32m+[m[32m      raise RuntimeError, "invalid string: #{string}" unless string.is_a?(String)[m
[32m+[m
[32m+[m[32m      if string =~ /\.\./[m
[32m+[m[32m        (sha1, sha2) = string.split('..')[m
[32m+[m[32m        return [rev_parse({}, sha1), rev_parse({}, sha2)][m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      if /^[0-9a-f]{40}$/.match(string)  # passing in a sha - just no-op it[m
[32m+[m[32m        return string.chomp[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      head = File.join(@git_dir, 'refs', 'heads', string)[m
[32m+[m[32m      return File.read(head).chomp if File.file?(head)[m
[32m+[m
[32m+[m[32m      head = File.join(@git_dir, 'refs', 'remotes', string)[m
[32m+[m[32m      return File.read(head).chomp if File.file?(head)[m
[32m+[m
[32m+[m[32m      head = File.join(@git_dir, 'refs', 'tags', string)[m
[32m+[m[32m      return File.read(head).chomp if File.file?(head)[m
[32m+[m
[32m+[m[32m      ## check packed-refs file, too[m
[32m+[m[32m      packref = File.join(@git_dir, 'packed-refs')[m
[32m+[m[32m      if File.file?(packref)[m
[32m+[m[32m        File.readlines(packref).each do |line|[m
[32m+[m[32m          if m = /^(\w{40}) refs\/.+?\/(.*?)$/.match(line)[m
[32m+[m[32m            next if !Regexp.new(Regexp.escape(string) + '$').match(m[3])[m
[32m+[m[32m            return m[1].chomp[m
[32m+[m[32m          end[m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      ## !! more - partials and such !![m
[32m+[m
[32m+[m[32m      # revert to calling git - grr[m
[32m+[m[32m      return method_missing('rev-parse', options, string).chomp[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def refs(options, prefix)[m
[32m+[m[32m      refs = [][m
[32m+[m[32m      already = {}[m
[32m+[m[32m      Dir.chdir(@git_dir) do[m
[32m+[m[32m        files = Dir.glob(prefix + '/**/*')[m
[32m+[m[32m        files.each do |ref|[m
[32m+[m[32m          next if !File.file?(ref)[m
[32m+[m[32m          id = File.read(ref).chomp[m
[32m+[m[32m          name = ref.sub("#{prefix}/", '')[m
[32m+[m[32m          if !already[name][m
[32m+[m[32m            refs << "#{name} #{id}"[m
[32m+[m[32m            already[name] = true[m
[32m+[m[32m          end[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        if File.file?('packed-refs')[m
[32m+[m[32m          File.readlines('packed-refs').each do |line|[m
[32m+[m[32m            if m = /^(\w{40}) (.*?)$/.match(line)[m
[32m+[m[32m              next if !Regexp.new('^' + prefix).match(m[2])[m
[32m+[m[32m              name = m[2].sub("#{prefix}/", '')[m
[32m+[m[32m              if !already[name][m
[32m+[m[32m                refs << "#{name} #{m[1]}"[m
[32m+[m[32m                already[name] = true[m
[32m+[m[32m              end[m
[32m+[m[32m            end[m
[32m+[m[32m          end[m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      refs.join("\n")[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def tags(options, prefix)[m
[32m+[m[32m      refs = [][m
[32m+[m[32m      already = {}[m
[32m+[m
[32m+[m[32m      Dir.chdir(repo.path) do[m
[32m+[m[32m        files = Dir.glob(prefix + '/**/*')[m
[32m+[m
[32m+[m[32m        files.each do |ref|[m
[32m+[m[32m          next if !File.file?(ref)[m
[32m+[m
[32m+[m[32m          id = File.read(ref).chomp[m
[32m+[m[32m          name = ref.sub("#{prefix}/", '')[m
[32m+[m
[32m+[m[32m          if !already[name][m
[32m+[m[32m            refs << "#{name} #{id}"[m
[32m+[m[32m            already[name] = true[m
[32m+[m[32m          end[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        if File.file?('packed-refs')[m
[32m+[m[32m          lines = File.readlines('packed-refs')[m
[32m+[m[32m          lines.each_with_index do |line, i|[m
[32m+[m[32m            if m = /^(\w{40}) (.*?)$/.match(line)[m
[32m+[m[32m              next if !Regexp.new('^' + prefix).match(m[2])[m
[32m+[m[32m              name = m[2].sub("#{prefix}/", '')[m
[32m+[m
[32m+[m[32m              # Annotated tags in packed-refs include a reference[m
[32m+[m[32m              # to the commit object on the following line.[m
[32m+[m[32m              next_line = lines[i + 1][m
[32m+[m
[32m+[m[32m              id =[m
[32m+[m[32m              if next_line && next_line[0] == ?^[m
[32m+[m[32m                next_line[1..-1].chomp[m
[32m+[m[32m              else[m
[32m+[m[32m                m[1][m
[32m+[m[32m              end[m
[32m+[m
[32m+[m[32m              if !already[name][m
[32m+[m[32m                refs << "#{name} #{id}"[m
[32m+[m[32m                already[name] = true[m
[32m+[m[32m              end[m
[32m+[m[32m            end[m
[32m+[m[32m          end[m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      refs.join("\n")[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def file_size(ref)[m
[32m+[m[32m      try_run { ruby_git.cat_file_size(ref).to_s }[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def file_type(ref)[m
[32m+[m[32m      try_run { ruby_git.cat_file_type(ref).to_s }[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def blame_tree(commit, path = nil)[m
[32m+[m[32m      begin[m
[32m+[m[32m        path = [path].join('/').to_s + '/' if (path && path != '')[m
[32m+[m[32m        path = '' if !path.is_a? String[m
[32m+[m[32m        commits = file_index.last_commits(rev_parse({}, commit), looking_for(commit, path))[m
[32m+[m[32m        clean_paths(commits)[m
[32m+[m[32m      rescue FileIndex::IndexFileNotFound[m
[32m+[m[32m        {}[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def file_index[m
[32m+[m[32m      @git_file_index ||= FileIndex.new(@git_dir)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def ruby_git[m
[32m+[m[32m      @ruby_git_repo ||= Repository.new(@git_dir)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    private[m
[32m+[m
[32m+[m[32m      def try_run[m
[32m+[m[32m        ret = ''[m
[32m+[m[32m        Timeout.timeout(self.class.git_timeout) do[m
[32m+[m[32m          ret = yield[m
[32m+[m[32m        end[m
[32m+[m[32m        @bytes_read += ret.size[m
[32m+[m
[32m+[m[32m        #if @bytes_read > 5242880 # 5.megabytes[m
[32m+[m[32m        #  bytes = @bytes_read[m
[32m+[m[32m        #  @bytes_read = 0[m
[32m+[m[32m        #  raise Grit::Git::GitTimeout.new(command, bytes)[m
[32m+[m[32m        #end[m
[32m+[m
[32m+[m[32m        ret[m
[32m+[m[32m      rescue Timeout::Error => e[m
[32m+[m[32m        bytes = @bytes_read[m
[32m+[m[32m        @bytes_read = 0[m
[32m+[m[32m        raise Grit::Git::GitTimeout.new(command, bytes)[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      def looking_for(commit, path = nil)[m
[32m+[m[32m        tree_sha = ruby_git.get_subtree(rev_parse({}, commit), path)[m
[32m+[m
[32m+[m[32m        looking_for = [][m
[32m+[m[32m        ruby_git.get_object_by_sha1(tree_sha).entry.each do |e|[m
[32m+[m[32m          if path && !(path == '' || path == '.' || path == './')[m
[32m+[m[32m            file = File.join(path, e.name)[m
[32m+[m[32m          else[m
[32m+[m[32m            file = e.name[m
[32m+[m[32m          end[m
[32m+[m[32m          file += '/' if e.type == :directory[m
[32m+[m[32m          looking_for << file[m
[32m+[m[32m        end[m
[32m+[m[32m        looking_for[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      def clean_paths(commit_array)[m
[32m+[m[32m        new_commits = {}[m
[32m+[m[32m        commit_array.each do |file, sha|[m
[32m+[m[32m          file = file.chop if file[file.size - 1 , 1] == '/'[m
[32m+[m[32m          new_commits[file] = sha[m
[32m+[m[32m        end[m
[32m+[m[32m        new_commits[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m    # TODO[m
[32m+[m[32m    # git grep -n 'foo' 'master'[m
[32m+[m[32m    # git log --pretty='raw' --max-count='1' 'master' -- 'LICENSE'[m
[32m+[m[32m    # git log --pretty='raw' --max-count='1' 'master' -- 'test'[m
[32m+[m
[32m+[m[32m  end[m
[32m+[m[32mend[m
[1mdiff --git a/grit/lib/grit/git-ruby/commit_db.rb b/grit/lib/grit/git-ruby/commit_db.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..c141c8b[m
[1m--- /dev/null[m
[1m+++ b/grit/lib/grit/git-ruby/commit_db.rb[m
[36m@@ -0,0 +1,52 @@[m
[32m+[m[32mbegin[m
[32m+[m[32m  require 'sequel'[m
[32m+[m
[32m+[m[32m  module Grit[m
[32m+[m
[32m+[m[32m    class CommitDb[m
[32m+[m
[32m+[m[32m      SCHEMA_VERSION = 1[m
[32m+[m
[32m+[m[32m      attr_accessor :db, :git[m
[32m+[m
[32m+[m[32m      def initialize(git_obj, index_location = nil)[m
[32m+[m[32m        @git = git_obj[m
[32m+[m[32m        db_file = File.join(index_location || @git.git_dir, 'commit_db')[m
[32m+[m[32m        if !File.exists?(db_file)[m
[32m+[m[32m          @db = Sequel.open "sqlite:///#{db_file}"[m
[32m+[m[32m          setup_tables[m
[32m+[m[32m        else[m
[32m+[m[32m          @db = Sequel.open "sqlite:///#{db_file}"[m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      def rev_list(branch, options)[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      def update_db(branch = nil)[m
[32m+[m[32m        # find all refs/heads, for each[m
[32m+[m[32m        # add branch if not there[m
[32m+[m[32m        # go though all commits in branch[m
[32m+[m[32m          # add new commit_branches a[m
[32m+[m[32m          # and commit_nodes for each new one[m
[32m+[m[32m          # stop if reach commit that already has branch and node links[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      def setup_tables[m
[32m+[m[32m        @db << "create table meta (meta_key text, meta_value text)"[m
[32m+[m[32m        @db[:meta] << {:meta_key => 'schema', :meta_value => SCHEMA_VERSION}[m
[32m+[m
[32m+[m[32m        @db << "create table commits (id integer, sha text, author_date integer)"[m
[32m+[m[32m        @db << "create table nodes (id integer, path text, type text)"[m
[32m+[m[32m        @db << "create table branches (id integer, ref text, commit_id integer)"[m
[32m+[m
[32m+[m[32m        @db << "create table commit_branches (commit_id integer, branch_id integer)"[m
[32m+[m[32m        @db << "create table commit_nodes (commit_id integer, node_id integer, node_sha string)"[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32mrescue LoadError[m
[32m+[m[32m  # no commit db[m
[32m+[m[32mend[m
[1mdiff --git a/grit/lib/grit/git-ruby/file_index.rb b/grit/lib/grit/git-ruby/file_index.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..286e736[m
[1m--- /dev/null[m
[1m+++ b/grit/lib/grit/git-ruby/file_index.rb[m
[36m@@ -0,0 +1,193 @@[m
[32m+[m[32m# this implements a file-based 'file index', an simple index of[m
[32m+[m[32m# all of the reachable commits in a repo, along with the parents[m
[32m+[m[32m# and which files were modified during each commit[m
[32m+[m[32m#[m
[32m+[m[32m# this class looks for a file named '[.git]/file-index', generated via:[m
[32m+[m[32m#[m
[32m+[m[32m# git log --pretty=oneline --name-only --parents --reverse --all > file-index[m
[32m+[m[32m#[m
[32m+[m[32m# for this to work properly, you'll want to add the following as a post-receive hook[m
[32m+[m[32m# to keep the index up to date[m
[32m+[m[32m#[m
[32m+[m[32m# git log --pretty=oneline --name-only --parents --reverse [old-rev]..[new-rev] >> file-index[m
[32m+[m[32m#[m
[32m+[m[32mmodule Grit[m
[32m+[m[32m  module GitRuby[m
[32m+[m
[32m+[m[32m  class FileIndex[m
[32m+[m
[32m+[m[32m    class IndexFileNotFound < StandardError[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    class UnsupportedRef < StandardError[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    class << self[m
[32m+[m[32m      attr_accessor :max_file_size[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    self.max_file_size = 10_000_000 # ~10M[m
[32m+[m
[32m+[m[32m    attr_reader :files[m
[32m+[m
[32m+[m[32m    # initializes index given repo_path[m
[32m+[m[32m    def initialize(repo_path)[m
[32m+[m[32m      @index_file = File.join(repo_path, 'file-index')[m
[32m+[m[32m      if File.file?(@index_file) && (File.size(@index_file) < Grit::GitRuby::FileIndex.max_file_size)[m
[32m+[m[32m        read_index[m
[32m+[m[32m      else[m
[32m+[m[32m        raise IndexFileNotFound[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # returns count of all commits[m
[32m+[m[32m    def count_all[m
[32m+[m[32m      @sha_count[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # returns count of all commits reachable from SHA[m
[32m+[m[32m    # note: originally did this recursively, but ruby gets pissed about that on[m
[32m+[m[32m    # really big repos where the stack level gets 'too deep' (thats what she said)[m
[32m+[m[32m    def count(commit_sha)[m
[32m+[m[32m      commits_from(commit_sha).size[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # builds a list of all commits reachable from a single commit[m
[32m+[m[32m    def commits_from(commit_sha)[m
[32m+[m[32m      raise UnsupportedRef if commit_sha.is_a? Array[m
[32m+[m
[32m+[m[32m      already = {}[m
[32m+[m[32m      final = [][m
[32m+[m[32m      left_to_do = [commit_sha][m
[32m+[m
[32m+[m[32m      while commit_sha = left_to_do.shift[m
[32m+[m[32m        next if already[commit_sha][m
[32m+[m
[32m+[m[32m        final << commit_sha[m
[32m+[m[32m        already[commit_sha] = true[m
[32m+[m
[32m+[m[32m        commit = @commit_index[commit_sha][m
[32m+[m[32m        commit[:parents].each do |sha|[m
[32m+[m[32m          left_to_do << sha[m
[32m+[m[32m        end if commit[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      sort_commits(final)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def sort_commits(sha_array)[m
[32m+[m[32m      sha_array.sort { |a, b| @commit_order[b].to_i <=> @commit_order[a].to_i }[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # returns files changed at commit sha[m
[32m+[m[32m    def files(commit_sha)[m
[32m+[m[32m      @commit_index[commit_sha][:files] rescue nil[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # returns all commits for a file[m
[32m+[m[32m    def commits_for(file)[m
[32m+[m[32m      @all_files[file][m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # returns the shas of the last commits for all[m
[32m+[m[32m    # the files in [] from commit_sha[m
[32m+[m[32m    # files_matcher can be a regexp or an array[m
[32m+[m[32m    def last_commits(commit_sha, files_matcher)[m
[32m+[m[32m      acceptable = commits_from(commit_sha)[m
[32m+[m
[32m+[m[32m      matches = {}[m
[32m+[m
[32m+[m[32m      if files_matcher.is_a? Regexp[m
[32m+[m[32m        files = @all_files.keys.select { |file| file =~ files_matcher }[m
[32m+[m[32m        files_matcher = files[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      if files_matcher.is_a? Array[m
[32m+[m[32m        # find the last commit for each file in the array[m
[32m+[m[32m        files_matcher.each do |f|[m
[32m+[m[32m          @all_files[f].each do |try|[m
[32m+[m[32m            if acceptable.include?(try)[m
[32m+[m[32m              matches[f] = try[m
[32m+[m[32m              break[m
[32m+[m[32m            end[m
[32m+[m[32m          end if @all_files[f][m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      matches[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    private[m
[32m+[m
[32m+[m[32m      # read and parse the file-index data[m
[32m+[m[32m      def read_index[m
[32m+[m[32m        f = File.new(@index_file)[m
[32m+[m[32m        @sha_count = 0[m
[32m+[m[32m        @commit_index = {}[m
[32m+[m[32m        @commit_order = {}[m
[32m+[m[32m        @all_files = {}[m
[32m+[m[32m        while line = f.gets[m
[32m+[m[32m          if /^(\w{40})/.match(line)[m
[32m+[m[32m            shas = line.scan(/(\w{40})/)[m
[32m+[m[32m            current_sha = shas.shift.first[m
[32m+[m[32m            parents = shas.map { |sha| sha.first }[m
[32m+[m[32m            @commit_index[current_sha] = {:files => [], :parents => parents }[m
[32m+[m[32m            @commit_order[current_sha] = @sha_count[m
[32m+[m[32m            @sha_count += 1[m
[32m+[m[32m          else[m
[32m+[m[32m            file_name = line.chomp[m
[32m+[m[32m            tree = ''[m
[32m+[m[32m            File.dirname(file_name).split('/').each do |part|[m
[32m+[m[32m              next if part == '.'[m
[32m+[m[32m              tree += part + '/'[m
[32m+[m[32m              @all_files[tree] ||= [][m
[32m+[m[32m              @all_files[tree].unshift(current_sha)[m
[32m+[m[32m            end[m
[32m+[m[32m            @all_files[file_name] ||= [][m
[32m+[m[32m            @all_files[file_name].unshift(current_sha)[m
[32m+[m[32m            @commit_index[current_sha][:files] << file_name[m
[32m+[m[32m          end[m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  end[m
[32m+[m[32mend[m
[32m+[m
[32m+[m
[32m+[m[32m# benchmark testing on big-ass repos[m
[32m+[m
[32m+[m[32mif __FILE__ == $0[m
[32m+[m
[32m+[m[32m  #repo = '/Users/schacon/projects/git/.git'[m
[32m+[m[32m  #commit = 'd8933f013a66cc1deadf83a9c24eccb6fee78a35'[m
[32m+[m[32m  #file_list = ["builtin-merge-recursive.c", "git-send-email-script", "git-parse-remote.sh", "builtin-add.c", "merge-base.c", "read-cache.c", "fsck.h", "diff.c", "refs.c", "diffcore-rename.c", "epoch.c", "pack-intersect.c", "fast-import.c", "git-applypatch.sh", "git.spec.in", "rpush.c", "git-clone-script", "utf8.c", "git-external-diff-script", "builtin-init-db.c", "pack-redundant.c", "builtin-diff-index.c", "index.c", "update-index.c", "fetch-clone.c", "pager.c", "diff.h", "unpack-trees.c", "git-browse-help.sh", "git-rename-script", "refs.h", "get-tar-commit-id.c", "git-push.sh", "README", "delta.c", "mailsplit.c", "gitweb.cgi", "var.c", "epoch.h", "gsimm.c", "archive.c", "sideband.c", "utf8.h", "local-fetch.c", "git-request-pull-script", "builtin-send-pack.c", "blob.c", "builtin-ls-remote.c", "pretty.c", "git-diff.sh", "diffcore-break.c", "unpack-trees.h", "git-mv.perl", "interpolate.c", "builtin-diff-files.c", "delta.h", "commit-tree.c", "git-diff-script", "decorate.c", "builtin-name-rev.c", "tree-walk.c", "git-revert-script", "git-sh-setup.sh", "transport.c", "gsimm.h", "archive.h", "count-delta.c", "sideband.h", "git-merge.sh", "git-gui.sh", "git-core.spec.in", "cvs2git.c", "blob.h", "git.sh", "http-push.c", "builtin-commit-tree.c", "diff-helper.c", "builtin-push.c", "interpolate.h", "decorate.h", "git-citool", "dotest", "builtin-verify-tag.c", "git-mergetool.sh", "tree-walk.h", "log-tree.c", "name-rev.c", "applypatch", "cat-file.c", "test-delta.c", "server-info.c", "count-delta.h", "write-tree.c", "local-pull.c", "transport.h", "git-rm.sh", "unpack-objects.c", "xdiff-interface.c", "git-repack-script", "commit.c", "hash-object.c", "git-merge-recursive.py", "git-clone-dumb-http", "thread-utils.c", "git-send-email.perl", "git-whatchanged.sh", "log-tree.h", "builtin-annotate.c", "show-index.c", "pkt-line.c", "ident.c", "git-rebase-script", "name-hash.c", "git-archimport.perl", "xdiff-interface.h", "commit.h", "diff-lib.c", "wt-status.c", "base85.c", "
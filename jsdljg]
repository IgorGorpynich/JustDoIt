[32m+[m[32m  # to_hash[m
[32m+[m
[32m+[m[32m  def test_to_hash[m
[32m+[m[32m    old_tz, ENV["TZ"] = ENV["TZ"], "US/Pacific"[m
[32m+[m[32m    @c = Commit.create(@r, :id => '4c8124ffcf4039d292442eeccabdeca5af5c5017')[m
[32m+[m[32m    date = Time.parse('Wed Oct 10 03:06:12 -0400 2007')[m
[32m+[m[32m    expected = {[m
[32m+[m[32m      'parents' => ['id' => "634396b2f541a9f2d58b00be1a07f0c358b999b3"],[m
[32m+[m[32m      'committed_date' => date.xmlschema,[m
[32m+[m[32m      'tree' => "672eca9b7f9e09c22dcb128c283e8c3c8d7697a4",[m
[32m+[m[32m      'authored_date' => date.xmlschema,[m
[32m+[m[32m      'committer' => {'email' => "tom@mojombo.com", 'name' => "Tom Preston-Werner"},[m
[32m+[m[32m      'message' => "implement Grit#heads",[m
[32m+[m[32m      'author' => {'email' => "tom@mojombo.com", 'name' => "Tom Preston-Werner"},[m
[32m+[m[32m      'id' => "4c8124ffcf4039d292442eeccabdeca5af5c5017"[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    assert_equal expected, @c.to_hash[m
[32m+[m[32m  ensure[m
[32m+[m[32m    ENV["TZ"] = old_tz[m
[32m+[m[32m  end[m
[32m+[m[32mend[m

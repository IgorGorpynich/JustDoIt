[32m+[m[32mrequire File.dirname(__FILE__) + '/helper'[m
[32m+[m
[32m+[m[32mclass TestTree < Test::Unit::TestCase[m
[32m+[m[32m  def setup[m
[32m+[m[32m    @r = Repo.new(GRIT_REPO)[m
[32m+[m[32m    @t = Tree.allocate[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  # contents[m
[32m+[m[32m  def test_nosuch_tree[m
[32m+[m[32m    t = @r.tree('blahblah')[m
[32m+[m[32m    assert t.contents.is_a?(Array)[m
[32m+[m[32m    assert t.is_a?(Tree)[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_contents_should_cache[m
[32m+[m[32m    Git.any_instance.expects(:ls_tree).returns([m
[32m+[m[32m      fixture('ls_tree_a'),[m
[32m+[m[32m      fixture('ls_tree_b')[m
[32m+[m[32m    ).times(2)[m
[32m+[m[32m    tree = @r.tree('master')[m
[32m+[m
[32m+[m[32m    child = tree.contents.last[m
[32m+[m
[32m+[m[32m    child.contents[m
[32m+[m[32m    child.contents[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  # content_from_string[m
[32m+[m
[32m+[m[32m  def test_content_from_string_tree_should_return_tree[m
[32m+[m[32m    text = fixture('ls_tree_a').split("\n").last[m
[32m+[m
[32m+[m[32m    tree = @t.content_from_string(nil, text)[m
[32m+[m
[32m+[m[32m    assert_equal Tree, tree.class[m
[32m+[m[32m    assert_equal "650fa3f0c17f1edb4ae53d8dcca4ac59d86e6c44", tree.id[m
[32m+[m[32m    assert_equal "040000", tree.mode[m
[32m+[m[32m    assert_equal "test", tree.name[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_content_from_string_tree_should_return_blob[m
[32m+[m[32m    text = fixture('ls_tree_b').split("\n").first[m
[32m+[m
[32m+[m[32m    tree = @t.content_from_string(nil, text)[m
[32m+[m
[32m+[m[32m    assert_equal Blob, tree.class[m
[32m+[m[32m    assert_equal "aa94e396335d2957ca92606f909e53e7beaf3fbb", tree.id[m
[32m+[m[32m    assert_equal "100644", tree.mode[m
[32m+[m[32m    assert_equal "grit.rb", tree.name[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_content_from_string_tree_should_return_submodule[m
[32m+[m[32m    text = fixture('ls_tree_submodule').split("\n").first[m
[32m+[m
[32m+[m[32m    sm = @t.content_from_string(nil, text)[m
[32m+[m
[32m+[m[32m    assert_kind_of Submodule, sm[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_content_from_string_invalid_type_should_raise[m
[32m+[m[32m    assert_raise(RuntimeError) do[m
[32m+[m[32m      @t.content_from_string(nil, "040000 bogus 650fa3f0c17f1edb4ae53d8dcca4ac59d86e6c44	test")[m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  # /[m
[32m+[m
[32m+[m[32m  def test_slash[m
[32m+[m[32m    Git.any_instance.expects(:ls_tree).returns([m
[32m+[m[32m      fixture('ls_tree_a')[m
[32m+[m[32m    )[m
[32m+[m[32m    tree = @r.tree('master')[m
[32m+[m
[32m+[m[32m    assert_equal 'aa06ba24b4e3f463b3c4a85469d0fb9e5b421cf8', (tree/'lib').id[m
[32m+[m[32m    assert_equal '8b1e02c0fb554eed2ce2ef737a68bb369d7527df', (tree/'README.txt').id[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_slash_with_commits[m
[32m+[m[32m    Git.any_instance.expects(:ls_tree).returns([m
[32m+[m[32m      fixture('ls_tree_commit')[m
[32m+[m[32m    )[m
[32m+[m[32m    tree = @r.tree('master')[m
[32m+[m
[32m+[m[32m    assert_equal 'd35b34c6e931b9da8f6941007a92c9c9a9b0141a', (tree/'bar').id[m
[32m+[m[32m    assert_equal '2afb47bcedf21663580d5e6d2f406f08f3f65f19', (tree/'foo').id[m
[32m+[m[32m    assert_equal 'f623ee576a09ca491c4a27e48c0dfe04be5f4a2e', (tree/'baz').id[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  # inspect[m
[32m+[m
[32m+[m[32m  def test_inspect[m
[32m+[m[32m    @t = Tree.create(@r, :id => 'abc')[m
[32m+[m[32m    assert_equal %Q{#<Grit::Tree "abc">}, @t.inspect[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_basename[m
[32m+[m[32m    @t = Tree.create(@r, :name => 'foo/bar')[m
[32m+[m[32m    assert_equal "bar", @t.basename[m
[32m+[m[32m  end[m

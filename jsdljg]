[32m+[m[32mrequire File.dirname(__FILE__) + '/helper'[m
[32m+[m[32mrequire 'tempfile'[m
[32m+[m
[32m+[m[32mclass TestRubyGit < Test::Unit::TestCase[m
[32m+[m
[32m+[m[32m  def setup[m
[32m+[m[32m    @git = Git.new(File.join(File.dirname(__FILE__), *%w[dot_git]))[m
[32m+[m[32m    @commit_sha = '5e3ee1198672257164ce3fe31dea3e40848e68d5'[m
[32m+[m[32m    @tree_sha = 'cd7422af5a2e0fff3e94d6fb1a8fff03b2841881'[m
[32m+[m[32m    @blob_sha = '4232d073306f01cf0b895864e5a5cfad7dd76fce'[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_init_gitdir[m
[32m+[m[32m    tf = Tempfile.new('gitdir')[m
[32m+[m[32m    temppath = tf.path[m
[32m+[m[32m    tf.unlink[m
[32m+[m
[32m+[m[32m    git = Git.new(temppath)[m
[32m+[m[32m    git.init({})[m
[32m+[m[32m    assert File.exists?(File.join(temppath, 'config'))[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_log_merge[m
[32m+[m[32m    c1 = '420eac97a826bfac8724b6b0eef35c20922124b7'[m
[32m+[m[32m    c2 = '30e367cef2203eba2b341dc9050993b06fd1e108'[m
[32m+[m[32m    out = @git.rev_list({:pretty => 'raw', :max_count => 10}, 'master')[m
[32m+[m[32m    assert_match "commit #{c1}", out[m
[32m+[m[32m    assert_match "commit #{c2}", out[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_log_max_count[m
[32m+[m[32m    out = @git.rev_list({:max_count => 10}, 'master')[m
[32m+[m[32m    assert_equal 10, out.split("\n").size[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_diff[m
[32m+[m[32m    commit1 = '2d3acf90f35989df8f262dc50beadc4ee3ae1560'[m
[32m+[m[32m    commit2 = '420eac97a826bfac8724b6b0eef35c20922124b7'[m
[32m+[m[32m    out = @git.diff({}, commit1, commit2)[m
[32m+[m[32m    assert_match 'index 6afcf64..9e78ddf 100644', out[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_diff_single[m
[32m+[m[32m    commit1 = '2d3acf90f35989df8f262dc50beadc4ee3ae1560'[m
[32m+[m[32m    out = @git.diff({}, commit1, nil)[m
[32m+[m[32m    assert_match 'index ad42ff5..aa50f09 100644', out[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_diff_full[m
[32m+[m[32m    commit1 = '2d3acf90f35989df8f262dc50beadc4ee3ae1560'[m
[32m+[m[32m    commit2 = '420eac97a826bfac8724b6b0eef35c20922124b7'[m
[32m+[m[32m    out = @git.diff({:full_index => true}, commit1, commit2)[m
[32m+[m[32m    assert_match 'index 6afcf64c80da8253fa47228eb09bc0eea217e5d1..9e78ddfaabf79f8314cc9a53a2f59775aee06bd7', out[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_diff_add[m
[32m+[m[32m    commit1 = 'c9cf68fc61bd2634e90a4f6a12d88744e6297c4e'[m
[32m+[m[32m    commit2 = '7a8d32cb18a0ba2ff8bf86cadacc3fd2816da219'[m
[32m+[m[32m    out = @git.diff({}, commit1, commit2)[m
[32m+[m[32m    assert_match "--- /dev/null\n+++ b/test/test_tag.rb", out[m
[32m+[m[32m    assert_match "diff --git a/test/test_tag.rb b/test/test_tag.rb", out[m
[32m+[m[32m    assert_match 'index 0000000..2e3b0cb', out[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_diff_remove[m
[32m+[m[32m    commit1 = 'c9cf68fc61bd2634e90a4f6a12d88744e6297c4e'[m
[32m+[m[32m    commit2 = '7a8d32cb18a0ba2ff8bf86cadacc3fd2816da219'[m
[32m+[m[32m    out = @git.diff({}, commit1, commit2)[m
[32m+[m[32m    assert_match "--- a/test/fixtures/diff_2\n+++ /dev/null", out[m
[32m+[m[32m    assert_match "diff --git a/test/fixtures/diff_2 b/test/fixtures/diff_2", out[m
[32m+[m[32m    assert_match 'index 0000000..2e3b0cb', out[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m
[32m+[m[32m  def test_cat_file_contents_commit[m

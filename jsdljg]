[32m+[m[32m-        @_content_type = type[m
[32m+[m[32m+      list[m
[32m+[m[32m+    end[m
[32m+[m[32m+[m[41m    [m
[32m+[m[32m+    public[m
[32m+[m[32m+    def self.params_to_query_string(value, prefix = nil)[m
[32m+[m[32m+      case value[m
[32m+[m[32m+      when Array[m
[32m+[m[32m+        value.map { |v|[m
[32m+[m[32m+          params_to_query_string(v, "#{prefix}[]")[m
[32m+[m[32m+        } * "&"[m
[32m+[m[32m+      when Hash[m
[32m+[m[32m+        value.map { |k, v|[m
[32m+[m[32m+          params_to_query_string(v, prefix ? "#{prefix}[#{Merb::Request.escape(k)}]" : Merb::Request.escape(k))[m
[32m+[m[32m+        } * "&"[m
[32m+[m[32m+      else[m
[32m+[m[32m+        "#{prefix}=#{Merb::Request.escape(value)}"[m
[32m+[m[32m       end[m
[32m+[m[32m+    end[m[41m      [m
[32m+[m[41m       [m
[32m+[m[32m-    end[m
[32m+[m[32m+  end[m
[32m+[m[32m+[m
[32m+[m[32m+  class AcceptType[m
[32m+[m[32m+[m
[32m+[m[32m+    attr_reader :media_range, :quality, :index, :type, :sub_type[m
[32m+[m[41m     [m
[32m+[m[32m+    def initialize(entry,index)[m
[32m+[m[32m+      @index = index[m
[32m+[m[32m+      @media_range, quality = entry.split(/;\s*q=/).map{|a| a.strip }[m
[32m+[m[32m+      @type, @sub_type = @media_range.split(/\//)[m
[32m+[m[32m+      quality ||= 0.0 if @media_range == '*/*'[m
[32m+[m[32m+      @quality = ((quality || 1.0).to_f * 100).to_i[m
[32m+[m[32m+    end[m
[32m+[m[32m+[m[41m  [m
[32m+[m[32m+    def <=>(entry)[m
[32m+[m[32m+      c = entry.quality <=> quality[m
[32m+[m[32m+      c = index <=> entry.index if c == 0[m
[32m+[m[32m+      c[m
[32m+[m[32m+    end[m
[32m+[m[32m+[m[41m  [m
[32m+[m[32m+    def eql?(entry)[m
[32m+[m[32m+      synonyms.include?(entry.media_range)[m
[32m+[m[32m+    end[m
[32m+[m[32m+[m[41m  [m
[32m+[m[32m+    def ==(entry); eql?(entry); end[m
[32m+[m[32m+[m[41m  [m
[32m+[m[32m+    def hash; super_range.hash; end[m
[32m+[m[32m+[m[41m  [m
[32m+[m[32m+    def synonyms[m
[32m+[m[32m+      @syns ||= Merb.available_mime_types.values.map do |e|[m[41m [m
[32m+[m[32m+        e[:request_headers] if e[:request_headers].include?(@media_range)[m
[32m+[m[32m+      end.compact.flatten[m
[32m+[m[32m+    end[m
[32m+[m[32m+[m[41m  [m
[32m+[m[32m+    def super_range[m
[32m+[m[32m+      synonyms.first || @media_range[m
[32m+[m[32m+    end[m
[32m+[m[32m+[m[41m  [m
[32m+[m[32m+    def to_sym[m
[32m+[m[32m+      Merb.available_mime_types.select{|k,v|[m[41m [m
[32m+[m[32m+        v[:request_headers] == synonyms || v[:request_headers][0] == synonyms[0]}.flatten.first[m
[32m+[m[32m+    end[m
[32m+[m[32m+[m[41m  [m
[32m+[m[32m+    def to_s[m
[32m+[m[32m+      @media_range[m
[32m+[m[32m+    end[m
[32m+[m[32m+[m[41m  [m
[32m+[m[32m   end[m
[32m+[m[32m+[m
[32m+[m[41m     [m
[32m+[m[32m end[m

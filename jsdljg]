[32m+[m[32m        )[m
[32m+[m
[32m+[m[32m        host = "#{config['username']}@rubyforge.org"[m
[32m+[m[32m        remote_dir = "/var/www/gforge-projects/grit/"[m
[32m+[m[32m        local_dir = 'rdoc'[m
[32m+[m
[32m+[m[32m        Rake::SshDirPublisher.new(host, remote_dir, local_dir).upload[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m[32mrescue LoadError[m
[32m+[m[32m  puts "Rake SshDirPublisher is unavailable or your rubyforge environment is not configured."[m
[32m+[m[32mend[m

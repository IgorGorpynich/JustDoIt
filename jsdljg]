[32m+[m[32m    assert_equal out, nil[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_ls_tree_treeish[m
[32m+[m[32m    out = @git.ls_tree({}, 'testing')[m
[32m+[m[32m    assert_equal out, fixture('cat_file_tree_ruby').chomp[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_ls_tree_paths[m
[32m+[m[32m    paths = ['History.txt', 'lib'][m
[32m+[m[32m    out = @git.ls_tree({}, @tree_sha, paths)[m
[32m+[m[32m    assert_equal out, fixture('ls_tree_paths_ruby').chomp[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_ls_tree_paths_multi_single[m
[32m+[m[32m    paths = ['lib/grit.rb'][m
[32m+[m[32m    out = @git.ls_tree({}, @tree_sha, paths)[m
[32m+[m[32m    assert_equal out, "100644 blob 6afcf64c80da8253fa47228eb09bc0eea217e5d1\tlib/grit.rb"[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_ls_tree_recursive[m
[32m+[m[32m    # this is the tree associated with @commit_sha, which we use in[m
[32m+[m[32m    # the next test[m
[32m+[m[32m    tree_sha = '77fc9894c0904279fde93adc9c0ba231515ce68a'[m
[32m+[m
[32m+[m[32m    out = @git.ls_tree({:r => true}, tree_sha)[m

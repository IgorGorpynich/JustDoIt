[1mdiff --git a/grit/lib/grit/status.rb b/grit/lib/grit/status.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..1e07a1a[m
[1m--- /dev/null[m
[1m+++ b/grit/lib/grit/status.rb[m
[36m@@ -0,0 +1,151 @@[m
[32m+[m[32mmodule Grit[m
[32m+[m
[32m+[m[32m  class Status[m
[32m+[m[32m    include Enumerable[m
[32m+[m
[32m+[m[32m    @base = nil[m
[32m+[m[32m    @files = nil[m
[32m+[m
[32m+[m[32m    def initialize(base)[m
[32m+[m[32m      @base = base[m
[32m+[m[32m      construct_status[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def changed[m
[32m+[m[32m      @files.select { |k, f| f.type == 'M' }[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def added[m
[32m+[m[32m      @files.select { |k, f| f.type == 'A' }[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def deleted[m
[32m+[m[32m      @files.select { |k, f| f.type == 'D' }[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def untracked[m
[32m+[m[32m      @files.select { |k, f| f.untracked }[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def pretty[m
[32m+[m[32m      out = ''[m
[32m+[m[32m      self.each do |file|[m
[32m+[m[32m        out << file.path[m
[32m+[m[32m        out << "\n\tsha(r) " + file.sha_repo.to_s + ' ' + file.mode_repo.to_s[m
[32m+[m[32m        out << "\n\tsha(i) " + file.sha_index.to_s + ' ' + file.mode_index.to_s[m
[32m+[m[32m        out << "\n\ttype   " + file.type.to_s[m
[32m+[m[32m        out << "\n\tstage  " + file.stage.to_s[m
[32m+[m[32m        out << "\n\tuntrac " + file.untracked.to_s[m
[32m+[m[32m        out << "\n"[m
[32m+[m[32m      end[m
[32m+[m[32m      out << "\n"[m
[32m+[m[32m      out[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # enumerable method[m
[32m+[m
[32m+[m[32m    def [](file)[m
[32m+[m[32m      @files[file][m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def each[m
[32m+[m[32m      @files.each do |k, file|[m
[32m+[m[32m        yield file[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    class StatusFile[m
[32m+[m[32m      attr_accessor :path, :type, :stage, :untracked[m
[32m+[m[32m      attr_accessor :mode_index, :mode_repo[m
[32m+[m[32m      attr_accessor :sha_index, :sha_repo[m
[32m+[m
[32m+[m[32m      @base = nil[m
[32m+[m
[32m+[m[32m      def initialize(base, hash)[m
[32m+[m[32m        @base = base[m
[32m+[m[32m        @path = hash[:path][m
[32m+[m[32m        @type = hash[:type][m
[32m+[m[32m        @stage = hash[:stage][m
[32m+[m[32m        @mode_index = hash[:mode_index][m
[32m+[m[32m        @mode_repo = hash[:mode_repo][m
[32m+[m[32m        @sha_index = hash[:sha_index][m
[32m+[m[32m        @sha_repo = hash[:sha_repo][m
[32m+[m[32m        @untracked = hash[:untracked][m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      def blob(type = :index)[m
[32m+[m[32m        if type == :repo[m
[32m+[m[32m          @base.object(@sha_repo)[m
[32m+[m[32m        else[m
[32m+[m[32m          @base.object(@sha_index) rescue @base.object(@sha_repo)[m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    private[m
[32m+[m
[32m+[m[32m      def construct_status[m
[32m+[m[32m        @files = ls_files[m
[32m+[m
[32m+[m[32m        Dir.chdir(@base.working_dir) do[m
[32m+[m[32m          # find untracked in working dir[m
[32m+[m[32m          Dir.glob('**/*') do |file|[m
[32m+[m[32m            if !@files[file][m
[32m+[m[32m              @files[file] = {:path => file, :untracked => true} if !File.directory?(file)[m
[32m+[m[32m            end[m
[32m+[m[32m          end[m
[32m+[m
[32m+[m[32m          # find modified in tree[m
[32m+[m[32m         diff_files.each do |path, data|[m
[32m+[m[32m            @files[path] ? @files[path].merge!(data) : @files[path] = data[m
[32m+[m[32m          end[m
[32m+[m
[32m+[m[32m          # find added but not committed - new files[m
[32m+[m[32m          diff_index('HEAD').each do |path, data|[m
[32m+[m[32m            @files[path] ? @files[path].merge!(data) : @files[path] = data[m
[32m+[m[32m          end[m
[32m+[m
[32m+[m[32m          @files.each do |k, file_hash|[m
[32m+[m[32m            @files[k] = StatusFile.new(@base, file_hash)[m
[32m+[m[32m          end[m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      # compares the index and the working directory[m
[32m+[m[32m      def diff_files[m
[32m+[m[32m        hsh = {}[m
[32m+[m[32m        @base.git.diff_files.split("\n").each do |line|[m
[32m+[m[32m          (info, file) = line.split("\t")[m
[32m+[m[32m          (mode_src, mode_dest, sha_src, sha_dest, type) = info.split[m
[32m+[m[32m          hsh[file] = {:path => file, :mode_file => mode_src.to_s[1, 7], :mode_index => mode_dest,[m
[32m+[m[32m                        :sha_file => sha_src, :sha_index => sha_dest, :type => type}[m
[32m+[m[32m        end[m
[32m+[m[32m        hsh[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      # compares the index and the repository[m
[32m+[m[32m      def diff_index(treeish)[m
[32m+[m[32m        hsh = {}[m
[32m+[m[32m        @base.git.diff_index({}, treeish).split("\n").each do |line|[m
[32m+[m[32m          (info, file) = line.split("\t")[m
[32m+[m[32m          (mode_src, mode_dest, sha_src, sha_dest, type) = info.split[m
[32m+[m[32m          hsh[file] = {:path => file, :mode_repo => mode_src.to_s[1, 7], :mode_index => mode_dest,[m
[32m+[m[32m                        :sha_repo => sha_src, :sha_index => sha_dest, :type => type}[m
[32m+[m[32m        end[m
[32m+[m[32m        hsh[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      def ls_files[m
[32m+[m[32m        hsh = {}[m
[32m+[m[32m        lines = @base.git.ls_files({:stage => true})[m
[32m+[m[32m        lines.split("\n").each do |line|[m
[32m+[m[32m          (info, file) = line.split("\t")[m
[32m+[m[32m          (mode, sha, stage) = info.split[m
[32m+[m[32m          hsh[file] = {:path => file, :mode_index => mode, :sha_index => sha, :stage => stage}[m
[32m+[m[32m        end[m
[32m+[m[32m        hsh[m
[32m+[m[32m      end[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32mend[m
\ No newline at end of file[m
[1mdiff --git a/grit/lib/grit/submodule.rb b/grit/lib/grit/submodule.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..c4f0250[m
[1m--- /dev/null[m
[1m+++ b/grit/lib/grit/submodule.rb[m
[36m@@ -0,0 +1,88 @@[m
[32m+[m[32mmodule Grit[m
[32m+[m
[32m+[m[32m  class Submodule[m
[32m+[m[32m    attr_reader :id[m
[32m+[m[32m    attr_reader :mode[m
[32m+[m[32m    attr_reader :name[m
[32m+[m
[32m+[m[32m    # Create a Submodule containing just the specified attributes[m
[32m+[m[32m    #   +repo+ is the Repo[m
[32m+[m[32m    #   +atts+ is a Hash of instance variable data[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Submodule (unbaked)[m
[32m+[m[32m    def self.create(repo, atts)[m
[32m+[m[32m      self.allocate.create_initialize(repo, atts)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Initializer for Submodule.create[m
[32m+[m[32m    #   +repo+ is the Repo[m
[32m+[m[32m    #   +atts+ is a Hash of instance variable data[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Submodule[m
[32m+[m[32m    def create_initialize(repo, atts)[m
[32m+[m[32m      @repo = repo[m
[32m+[m[32m      atts.each do |k, v|[m
[32m+[m[32m        instance_variable_set("@#{k}".to_sym, v)[m
[32m+[m[32m      end[m
[32m+[m[32m      self[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # The url of this submodule[m
[32m+[m[32m    #   +ref+ is the committish that should be used to look up the url[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns String[m
[32m+[m[32m    def url(ref)[m
[32m+[m[32m      config = self.class.config(@repo, ref)[m
[32m+[m
[32m+[m[32m      lookup = config.keys.inject({}) do |acc, key|[m
[32m+[m[32m        id = config[key]['id'][m
[32m+[m[32m        acc[id] = config[key]['url'][m
[32m+[m[32m        acc[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      lookup[@id][m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # The configuration information for the given +repo+[m
[32m+[m[32m    #   +repo+ is the Repo[m
[32m+[m[32m    #   +ref+ is the committish (defaults to 'master')[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns a Hash of { <path:String> => { 'url' => <url:String>, 'id' => <id:String> } }[m
[32m+[m[32m    # Returns {} if no .gitmodules file was found[m
[32m+[m[32m    def self.config(repo, ref = "master")[m
[32m+[m[32m      commit = repo.commit(ref)[m
[32m+[m[32m      blob = commit.tree/'.gitmodules'[m
[32m+[m[32m      return {} unless blob[m
[32m+[m
[32m+[m[32m      lines = blob.data.gsub(/\r\n?/, "\n" ).split("\n")[m
[32m+[m
[32m+[m[32m      config = {}[m
[32m+[m[32m      current = nil[m
[32m+[m
[32m+[m[32m      lines.each do |line|[m
[32m+[m[32m        if line =~ /^\[submodule "(.+)"\]$/[m
[32m+[m[32m          current = $1[m
[32m+[m[32m          config[current] = {}[m
[32m+[m[32m          config[current]['id'] = (commit.tree/current).id[m
[32m+[m[32m        elsif line =~ /^\t(\w+) = (.+)$/[m
[32m+[m[32m          config[current][$1] = $2[m
[32m+[m[32m          config[current]['id'] = (commit.tree/$2).id if $1 == 'path'[m
[32m+[m[32m        else[m
[32m+[m[32m          # ignore[m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      config[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def basename[m
[32m+[m[32m      File.basename(name)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Pretty object inspection[m
[32m+[m[32m    def inspect[m
[32m+[m[32m      %Q{#<Grit::Submodule "#{@id}">}[m
[32m+[m[32m    end[m
[32m+[m[32m  end # Submodule[m
[32m+[m
[32m+[m[32mend # Grit[m
\ No newline at end of file[m
[1mdiff --git a/grit/lib/grit/tag.rb b/grit/lib/grit/tag.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..66e6a6b[m
[1m--- /dev/null[m
[1m+++ b/grit/lib/grit/tag.rb[m
[36m@@ -0,0 +1,16 @@[m
[32m+[m[32mmodule Grit[m
[32m+[m
[32m+[m[32m  class Tag < Ref[m
[32m+[m[32m    def self.find_all(repo, options = {})[m
[32m+[m[32m      refs = repo.git.refs(options, prefix)[m
[32m+[m[32m      refs.split("\n").map do |ref|[m
[32m+[m[32m        name, id = *ref.split(' ')[m
[32m+[m[32m        cid = repo.git.commit_from_sha(id)[m
[32m+[m[32m        raise "Unknown object type." if cid == ''[m
[32m+[m[32m        commit = Commit.create(repo, :id => cid)[m
[32m+[m[32m        self.new(name, commit)[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32mend[m
[1mdiff --git a/grit/lib/grit/tree.rb b/grit/lib/grit/tree.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..803612f[m
[1m--- /dev/null[m
[1m+++ b/grit/lib/grit/tree.rb[m
[36m@@ -0,0 +1,123 @@[m
[32m+[m[32mmodule Grit[m
[32m+[m
[32m+[m[32m  class Tree[m
[32m+[m[32m    lazy_reader :contents[m
[32m+[m[32m    attr_reader :id[m
[32m+[m[32m    attr_reader :mode[m
[32m+[m[32m    attr_reader :name[m
[32m+[m
[32m+[m[32m    # Construct the contents of the tree[m
[32m+[m[32m    #   +repo+ is the Repo[m
[32m+[m[32m    #   +treeish+ is the reference[m
[32m+[m[32m    #   +paths+ is an optional Array of directory paths to restrict the tree[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Tree (baked)[m
[32m+[m[32m    def self.construct(repo, treeish, paths = [])[m
[32m+[m[32m      output = repo.git.ls_tree({}, treeish, *paths)[m
[32m+[m[32m      self.allocate.construct_initialize(repo, treeish, output)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def construct_initialize(repo, id, text)[m
[32m+[m[32m      @repo = repo[m
[32m+[m[32m      @id = id[m
[32m+[m[32m      @contents = [][m
[32m+[m
[32m+[m[32m      text.split("\n").each do |line|[m
[32m+[m[32m        @contents << content_from_string(repo, line)[m
[32m+[m[32m      end[m
[32m+[m[32m      @contents.compact![m
[32m+[m
[32m+[m[32m      self[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def lazy_source[m
[32m+[m[32m      Tree.construct(@repo, @id, [])[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Create an unbaked Tree containing just the specified attributes[m
[32m+[m[32m    #   +repo+ is the Repo[m
[32m+[m[32m    #   +atts+ is a Hash of instance variable data[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Tree (unbaked)[m
[32m+[m[32m    def self.create(repo, atts)[m
[32m+[m[32m      self.allocate.create_initialize(repo, atts)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Initializer for Tree.create[m
[32m+[m[32m    #   +repo+ is the Repo[m
[32m+[m[32m    #   +atts+ is a Hash of instance variable data[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Tree (unbaked)[m
[32m+[m[32m    def create_initialize(repo, atts)[m
[32m+[m[32m      @repo = repo[m
[32m+[m
[32m+[m[32m      atts.each do |k, v|[m
[32m+[m[32m        instance_variable_set("@#{k}", v)[m
[32m+[m[32m      end[m
[32m+[m[32m      self[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Parse a content item and create the appropriate object[m
[32m+[m[32m    #   +repo+ is the Repo[m
[32m+[m[32m    #   +text+ is the single line containing the items data in `git ls-tree` format[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Blob or Grit::Tree[m
[32m+[m[32m    def content_from_string(repo, text)[m
[32m+[m[32m      mode, type, id, name = text.split(" ", 4)[m
[32m+[m[32m      case type[m
[32m+[m[32m        when "tree"[m
[32m+[m[32m          Tree.create(repo, :id => id, :mode => mode, :name => name)[m
[32m+[m[32m        when "blob"[m
[32m+[m[32m          Blob.create(repo, :id => id, :mode => mode, :name => name)[m
[32m+[m[32m        when "link"[m
[32m+[m[32m          Blob.create(repo, :id => id, :mode => mode, :name => name)[m
[32m+[m[32m        when "commit"[m
[32m+[m[32m          Submodule.create(repo, :id => id, :mode => mode, :name => name)[m
[32m+[m[32m        else[m
[32m+[m[32m          raise "Invalid type: #{type}"[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Find the named object in this tree's contents[m
[32m+[m[32m    #[m
[32m+[m[32m    # Examples[m
[32m+[m[32m    #   Repo.new('/path/to/grit').tree/'lib'[m
[32m+[m[32m    #   # => #<Grit::Tree "6cc23ee138be09ff8c28b07162720018b244e95e">[m
[32m+[m[32m    #   Repo.new('/path/to/grit').tree/'README.txt'[m
[32m+[m[32m    #   # => #<Grit::Blob "8b1e02c0fb554eed2ce2ef737a68bb369d7527df">[m
[32m+[m[32m    #[m
[32m+[m[32m    # Returns Grit::Blob or Grit::Tree or nil if not found[m
[32m+[m[32m    def /(file)[m
[32m+[m[32m      if file =~ /\//[m
[32m+[m[32m        file.split("/").inject(self) { |acc, x| acc/x } rescue nil[m
[32m+[m[32m      else[m
[32m+[m[32m        self.contents.find { |c| c.name == file }[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def basename[m
[32m+[m[32m      File.basename(name)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Pretty object inspection[m
[32m+[m[32m    def inspect[m
[32m+[m[32m      %Q{#<Grit::Tree "#{@id}">}[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Find only Tree objects from contents[m
[32m+[m[32m    def trees[m
[32m+[m[32m      contents.select {|v| v.kind_of? Tree}[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Find only Blob objects from contents[m
[32m+[m[32m    def blobs[m
[32m+[m[32m      contents.select {|v| v.kind_of? Blob}[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    # Compares trees by name[m
[32m+[m[32m    def <=>(other)[m
[32m+[m[32m      name <=> other.name[m
[32m+[m[32m    end[m
[32m+[m[32m  end # Tree[m
[32m+[m
[32m+[m[32mend # Grit[m
[1mdiff --git a/grit/lib/open3_detach.rb b/grit/lib/open3_detach.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..fd8020a[m
[1m--- /dev/null[m
[1m+++ b/grit/lib/open3_detach.rb[m
[36m@@ -0,0 +1,46 @@[m
[32m+[m[32mmodule Open3[m
[32m+[m[32m  extend self[m
[32m+[m
[32m+[m[32m  def popen3(*cmd)[m
[32m+[m[32m    pw = IO::pipe   # pipe[0] for read, pipe[1] for write[m
[32m+[m[32m    pr = IO::pipe[m
[32m+[m[32m    pe = IO::pipe[m
[32m+[m
[32m+[m[32m    pid = fork{[m
[32m+[m[32m      # child[m
[32m+[m[32m      fork{[m
[32m+[m[32m        # grandchild[m
[32m+[m[32m        pw[1].close[m
[32m+[m[32m        STDIN.reopen(pw[0])[m
[32m+[m[32m        pw[0].close[m
[32m+[m
[32m+[m[32m        pr[0].close[m
[32m+[m[32m        STDOUT.reopen(pr[1])[m
[32m+[m[32m        pr[1].close[m
[32m+[m
[32m+[m[32m        pe[0].close[m
[32m+[m[32m        STDERR.reopen(pe[1])[m
[32m+[m[32m        pe[1].close[m
[32m+[m
[32m+[m[32m        exec(*cmd)[m
[32m+[m[32m      }[m
[32m+[m[32m      exit!(0)[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    pw[0].close[m
[32m+[m[32m    pr[1].close[m
[32m+[m[32m    pe[1].close[m
[32m+[m[32m    Process.waitpid(pid)[m
[32m+[m[32m    pi = [pw[1], pr[0], pe[0]][m
[32m+[m[32m    pw[1].sync = true[m
[32m+[m[32m    if defined? yield[m
[32m+[m[32m      begin[m
[32m+[m[32m        return yield(*pi)[m
[32m+[m[32m      ensure[m
[32m+[m[32m        Process.detach(pid) if pid[m
[32m+[m[32m        pi.each { |p| p.close unless p.closed? }[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m[32m    pi[m
[32m+[m[32m  end[m
[32m+[m[32mend[m

[32m+[m[32m  # commit_deltas_from[m
[32m+[m
[32m+[m[32m  def test_commit_deltas_from_nothing_new[m
[32m+[m[32m    other_repo = Repo.new(GRIT_REPO)[m
[32m+[m[32m    @r.git.expects(:rev_list).with({}, "master").returns(fixture("rev_list_delta_b"))[m
[32m+[m[32m    other_repo.git.expects(:rev_list).with({}, "master").returns(fixture("rev_list_delta_a"))[m
[32m+[m
[32m+[m[32m    delta_blobs = @r.commit_deltas_from(other_repo)[m
[32m+[m[32m    assert_equal 0, delta_blobs.size[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_commit_deltas_from_when_other_has_new[m
[32m+[m[32m    other_repo = Repo.new(GRIT_REPO)[m
[32m+[m[32m    @r.git.expects(:rev_list).with({}, "master").returns(fixture("rev_list_delta_a"))[m
[32m+[m[32m    other_repo.git.expects(:rev_list).with({}, "master").returns(fixture("rev_list_delta_b"))[m
[32m+[m[32m    %w[[m
[32m+[m[32m      4c8124ffcf4039d292442eeccabdeca5af5c5017[m
[32m+[m[32m      634396b2f541a9f2d58b00be1a07f0c358b999b3[m
[32m+[m[32m      ab25fd8483882c3bda8a458ad2965d2248654335[m
[32m+[m[32m    ].each do |ref|[m
[32m+[m[32m      Commit.expects(:find_all).with(other_repo, ref, :max_count => 1).returns([stub()])[m
[32m+[m[32m    end[m
[32m+[m[32m    delta_blobs = @r.commit_deltas_from(other_repo)[m
[32m+[m[32m    assert_equal 3, delta_blobs.size[m
[32m+[m[32m  end[m
[32m+[m

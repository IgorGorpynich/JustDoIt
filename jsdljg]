[32m+[m[32m    # Set +debug+ to true to log all git calls and responses[m
[32m+[m[32m    attr_accessor :debug[m
[32m+[m[32m    attr_accessor :use_git_ruby[m
[32m+[m[32m    # The standard +logger+ for debugging git calls - this defaults to a plain STDOUT logger[m
[32m+[m[32m    attr_accessor :logger[m
[32m+[m[32m    def log(str)[m

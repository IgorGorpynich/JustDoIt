[32m+[m[32mrequire File.dirname(__FILE__) + '/helper'[m
[32m+[m[32mrequire 'pp'[m
[32m+[m
[32m+[m[32mclass TestRubyGitAlt < Test::Unit::TestCase[m
[32m+[m
[32m+[m[32m  def setup[m
[32m+[m[32m    @git1 = Grit::Repo.new(File.join(File.dirname(__FILE__), *%w[dot_git]), :is_bare => true)[m
[32m+[m[32m    @git2 = Grit::Repo.new(File.join(File.dirname(__FILE__), *%w[dot_git_clone]), :is_bare => true)[m
[32m+[m[32m    @git3 = Grit::Repo.new(File.join(File.dirname(__FILE__), *%w[dot_git_clone2]), :is_bare => true)[m
[32m+[m[32m    @commit_sha = 'ca8a30f5a7f0f163bbe3b6f0abf18a6c83b0687a'[m
[32m+[m[32m    @tree_sha = 'cd7422af5a2e0fff3e94d6fb1a8fff03b2841881'[m
[32m+[m[32m    @blob_sha = '4232d073306f01cf0b895864e5a5cfad7dd76fce'[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_basic[m
[32m+[m[32m    sha_hex = [@commit_sha].pack("H*")[m
[32m+[m[32m    assert @git1.git.ruby_git.in_loose?(sha_hex)[m
[32m+[m[32m    assert @git2.git.ruby_git.in_loose?(sha_hex)[m
[32m+[m[32m    assert @git1.git.ruby_git.object_exists?(@commit_sha)[m
[32m+[m[32m    assert @git2.git.ruby_git.object_exists?(@commit_sha)[m
[32m+[m[32m    assert_equal 10, @git1.commits.size[m
[32m+[m[32m    assert_equal 10, @git2.commits.size[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_clone_of_clone[m
[32m+[m[32m    sha_hex = [@commit_sha].pack("H*")[m
[32m+[m[32m    assert @git2.git.ruby_git.in_loose?(sha_hex)[m
[32m+[m[32m    assert @git3.git.ruby_git.in_loose?(sha_hex)[m
[32m+[m[32m    assert @git2.git.ruby_git.object_exists?(@commit_sha)[m
[32m+[m[32m    assert @git3.git.ruby_git.object_exists?(@commit_sha)[m
[32m+[m[32m    assert_equal 10, @git2.commits.size[m
[32m+[m[32m    assert_equal 10, @git3.commits.size[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_tree_path[m
[32m+[m[32m    file = @git2.tree('master', ['test/test_head.rb']).contents.first.name[m
[32m+[m[32m    assert_equal file, 'test/test_head.rb'[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32mend[m

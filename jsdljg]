[32m+[m[32m+    # request. Usually used to limit formats to a single action. See also[m
[32m+[m[32m+    # the controller-level only_provides that affects all actions[m
[32m+[m[32m+    # in a controller.[m[41m      [m
[32m+[m[32m+    #[m[41m [m
[32m+[m[32m+    # ==== Parameters[m
[32m+[m[32m+    # *formats<Symbol>:: A list of formats to use as the per-action list[m
[32m+[m[32m+    #                    of provided formats[m
[32m+[m[32m+    #[m
[32m+[m[32m+    # ==== Returns[m
[32m+[m[32m+    # Array:: List of formats passed in[m
[32m+[m[32m+    #[m
[32m+[m[32m+    #---[m
[32m+[m[32m+    # @public[m
[32m+[m[32m+    def only_provides(*formats)[m
[32m+[m[32m+      self._provided_formats = *formats[m
[32m+[m[32m+    end[m
[32m+[m[32m+[m[41m    [m
[32m+[m[32m+    # Removes formats from the list of provided formats for this particular[m[41m [m
[32m+[m[32m+    # request. Usually used to remove formats from a single action.  See[m
[32m+[m[32m+    # also the controller-level does_not_provide that affects all actions in a[m
[32m+[m[32m+    # controller.[m
[32m+[m[32m+    #[m
[32m+[m[32m+    # ==== Parameters[m
[32m+[m[32m+    # *formats<Symbol>:: Registered mime-type[m
[32m+[m[32m+    #[m[41m [m
[32m+[m[32m+    # ==== Returns[m
[32m+[m[32m+    # Array:: List of formats that remain after removing the ones not to provide[m
[32m+[m[32m+    #[m
[32m+[m[32m+    #---[m
[32m+[m[32m+    # @public[m[41m      [m
[32m+[m[32m+    def does_not_provide(*formats)[m
[32m+[m[32m+      formats.flatten![m
[32m+[m[32m+      self._provided_formats -= formats[m
[32m+[m[32m+    end[m
[32m+[m[32m+[m[41m    [m
[32m+[m[32m+    # Do the content negotiation:[m
[32m+[m[32m+    # 1. if params[:format] is there, and provided, use it[m
[32m+[m[32m+    # 2. Parse the Accept header[m
[32m+[m[32m+    # 3. If it's */*, use the first provided format[m
[32m+[m[32m+    # 4. Look for one that is provided, in order of request[m
[32m+[m[32m+    # 5. Raise 406 if none found[m
[32m+[m[32m+    def _perform_content_negotiation # :nodoc:[m
[32m+[m[32m+      raise Merb::ControllerExceptions::NotAcceptable if _provided_formats.empty?[m
[32m+[m[32m+      if fmt = params[:format] && _provided_formats.include?(fmt.to_sym)[m
[32m+[m[32m+        return fmt.to_sym[m
[32m+[m[32m       end[m
[32m+[m[32m+      accepts = Responder.parse(request.accept).map {|t| t.to_sym}[m
[32m+[m[32m+      return _provided_formats.first if accepts.include?(:all)[m
[32m+[m[32m+      (accepts & _provided_formats).first || (raise Merb::ControllerExceptions::NotAcceptable)[m
[32m+[m[32m+    end[m
[32m+[m[41m [m
[32m+[m[32m-      # Returns the output format for this request, based on the[m[41m [m
[32m+[m[32m-      # provided formats, <tt>params[:format]</tt> and the client's HTTP[m
[32m+[m[32m-      # Accept header.[m
[32m+[m[32m-      #[m
[32m+[m[32m-      # The first time this is called, it triggers content negotiation[m
[32m+[m[32m-      # and caches the value.  Once you call +content_type+ you can[m
[32m+[m[32m-      # not set or change the list of provided formats.[m
[32m+[m[32m-      #[m
[32m+[m[32m-      # Called automatically by +render+, so you should only call it if[m
[32m+[m[32m-      # you need the value, not to trigger content negotiation.[m[41m [m
[32m+[m[32m-      #[m[41m [m
[32m+[m[32m-      # ==== Parameters[m
[32m+[m[32m-      # fmt<String?>::[m[41m [m
[32m+[m[32m-      #   An optional format to use instead of performing content negotiation.[m
[32m+[m[32m-      #   This can be used to pass in the values of opts[:format] from the[m[41m [m
[32m+[m[32m-      #   render function to short-circuit content-negotiation when it's not[m
[32m+[m[32m-      #   necessary. This optional parameter should not be considered part[m
[32m+[m[32m-      #   of the public API.[m
[32m+[m[32m-      #[m
[32m+[m[32m-      # ==== Returns[m
[32m+[m[32m-      # Symbol:: The content-type that will be used for this controller.[m
[32m+[m[32m-      #[m
[32m+[m[32m-      #---[m
[32m+[m[32m-      # @public[m
[32m+[m[32m-      def content_type(fmt = nil)[m
[32m+[m[32m-        self.content_type = (fmt || _perform_content_negotiation) unless @_content_type[m
[32m+[m[32m-        @_content_type[m
[32m+[m[32m+    # Returns the output format for this request, based on the[m[41m [m
[32m+[m[32m+    # provided formats, <tt>params[:format]</tt> and the client's HTTP[m
[32m+[m[32m+    # Accept header.[m
[32m+[m[32m+    #[m
[32m+[m[32m+    # The first time this is called, it triggers content negotiation[m
[32m+[m[32m+    # and caches the value.  Once you call +content_type+ you can[m
[32m+[m[32m+    # not set or change the list of provided formats.[m
[32m+[m[32m+    #[m
[32m+[m[32m+    # Called automatically by +render+, so you should only call it if[m
[32m+[m[32m+    # you need the value, not to trigger content negotiation.[m[41m [m
[32m+[m[32m+    #[m[41m [m
[32m+[m[32m+    # ==== Parameters[m
[32m+[m[32m+    # fmt<String?>::[m[41m [m
[32m+[m[32m+    #   An optional format to use instead of performing content negotiation.[m
[32m+[m[32m+    #   This can be used to pass in the values of opts[:format] from the[m[41m [m
[32m+[m[32m+    #   render function to short-circuit content-negotiation when it's not[m
[32m+[m[32m+    #   necessary. This optional parameter should not be considered part[m
[32m+[m[32m+    #   of the public API.[m
[32m+[m[32m+    #[m
[32m+[m[32m+    # ==== Returns[m
[32m+[m[32m+    # Symbol:: The content-type that will be used for this controller.[m
[32m+[m[32m+    #[m
[32m+[m[32m+    #---[m
[32m+[m[32m+    # @public[m
[32m+[m[32m+    def content_type(fmt = nil)[m
[32m+[m[32m+      @_content_type = (fmt || _perform_content_negotiation) unless @_content_type[m
[32m+[m[32m+      @_content_type[m
[32m+[m[32m+    end[m
[32m+[m[32m+[m[41m    [m
[32m+[m[32m+    # Sets the content type of the current response to a value based on[m[41m [m
[32m+[m[32m+    # a passed in key. The Content-Type header will be set to the first[m
[32m+[m[32m+    # registered header for the mime-type.[m
[32m+[m[32m+    #[m
[32m+[m[32m+    # ==== Parameters[m
[32m+[m[32m+    # type<Symbol>:: A type that is in the list of registered mime-types.[m
[32m+[m[32m+    #[m
[32m+[m[32m+    # ==== Raises[m
[32m+[m[32m+    # ArgumentError:: "type" is not in the list of registered mime-types.[m
[32m+[m[32m+    #[m
[32m+[m[32m+    # ==== Returns[m
[32m+[m[32m+    # Symbol:: The content-type that was passed in.[m
[32m+[m[32m+    #[m
[32m+[m[32m+    #---[m
[32m+[m[32m+    # @semipublic[m
[32m+[m[32m+    def content_type=(type)[m
[32m+[m[32m+      unless Merb.available_mime_types.has_key?(type)[m
[32m+[m[32m+        raise Merb::ControllerExceptions::NotAcceptable.new("Unknown content_type for response: #{type}")[m[41m [m
[32m+[m[32m+      end[m[41m        [m
[32m+[m[32m+      headers['Content-Type'] = Merb.available_mime_types[type].first[m[41m        [m
[32m+[m[32m+      @_content_type = type[m
[32m+[m[32m+    end[m
[32m+[m[32m+[m[41m    [m
[32m+[m[32m+  end[m
[32m+[m[32m+[m
[32m+[m[32m+  class Responder[m
[32m+[m[32m+[m[41m  [m
[32m+[m[32m+    protected[m
[32m+[m[32m+    def self.parse(accept_header)[m
[32m+[m[32m+      # parse the raw accept header into a unique, sorted array of AcceptType objects[m
[32m+[m[32m+      list = accept_header.to_s.split(/,/).enum_for(:each_with_index).map do |entry,index|[m
[32m+[m[32m+        AcceptType.new(entry,index += 1)[m
[32m+[m[32m+      end.sort.uniq[m
[32m+[m[32m+      # firefox (and possibly other browsers) send broken default accept headers.[m
[32m+[m[32m+      # fix them up by sorting alternate xml forms (namely application/xhtml+xml)[m
[32m+[m[32m+      # ahead of pure xml types (application/xml,text/xml).[m
[32m+[m[32m+      if app_xml = list.detect{|e| e.super_range == 'application/xml'}[m
[32m+[m[32m+        list.select{|e| e.to_s =~ /\+xml/}.each { |acc_type|[m
[32m+[m[32m+          list[list.index(acc_type)],list[list.index(app_xml)] =[m[41m [m
[32m+[m[32m+            list[list.index(app_xml)],list[list.index(acc_type)] }[m
[32m+[m[32m       end[m
[32m+[m[32m-[m[41m      [m
[32m+[m[32m-      # Sets the content type of the current response to a value based on[m[41m [m
[32m+[m[32m-      # a passed in key. The Content-Type header will be set to the first[m
[32m+[m[32m-      # registered header for the mime-type.[m
[32m+[m[32m-      #[m
[32m+[m[32m-      # ==== Parameters[m
[32m+[m[32m-      # type<Symbol>:: A type that is in the list of registered mime-types.[m
[32m+[m[32m-      #[m
[32m+[m[32m-      # ==== Raises[m
[32m+[m[32m-      # ArgumentError:: "type" is not in the list of registered mime-types.[m

[32m+[m[32mr = g.add_remote(name, Git::Base)  # Git::Remote[m
[32m+[m
[32m+[m[32mg.remotes  # array of Git::Remotes[m
[32m+[m[32mg.remote(name).fetch[m
[32m+[m[32mg.remote(name).remove[m
[32m+[m[32mg.remote(name).merge[m
[32m+[m[32mg.remote(name).merge(branch)[m
[32m+[m
[32m+[m[32mg.fetch[m
[32m+[m[32mg.fetch(g.remotes.first)[m
[32m+[m
[32m+[m[32mg.pull[m
[32m+[m[32mg.pull(Git::Repo, Git::Branch) # fetch and a merge[m
[32m+[m
[32m+[m[32mg.add_tag('tag_name') # returns Git::Tag[m
[32m+[m
[32m+[m[32mg.repack[m
[32m+[m
[32m+[m[32mg.push[m
[32m+[m[32mg.push(g.remote('name'))[m
[32m+[m[41m   [m
[32m+[m[32mg.reset # defaults to HEAD[m
[32m+[m[32mg.reset_hard(Git::Commit)[m
[32m+[m
[32m+[m[32mg.branch('new_branch') # creates new or fetches existing[m
[32m+[m[32mg.branch('new_branch').checkout[m
[32m+[m[32mg.branch('new_branch').delete[m
[32m+[m[32mg.branch('existing_branch').checkout[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mrequire 'mojombo-grit'[m
[32m+[m
[32m+[m[32minclude Grit[m
[32m+[m[32mGrit.debug[m
[32m+[m[32mGrit.use_pure_ruby[m
[32m+[m
[32m+[m[32mrepo = Repo.new("/Users/tom/dev/grit")[m
[32m+[m
[32m+[m[32m= Commit Log[m
[32m+[m
[32m+[m[32mrepo.commits('mybranch')[m
[32m+[m[32mrepo.commits('40d3057d09a7a4d61059bca9dca5ae698de58cbe')[m
[32m+[m[32mrepo.commits('v0.1')[m
[32m+[m
[32m+[m[32mrepo.log('mybranch', 100, 20)[m
[32m+[m
[32m+[m[32mhead = repo.commits.first[m
[32m+[m[32mhead.id[m
[32m+[m[32m# => "e80bbd2ce67651aa18e57fb0b43618ad4baf7750"[m
[32m+[m[32mhead.parents[m

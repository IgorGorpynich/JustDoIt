[32m+[m[32m  Jeweler::Tasks.new do |s|[m
[32m+[m[32m    s.name = "grit"[m
[32m+[m[32m    s.rubyforge_project = "grit"[m
[32m+[m[32m    s.summary = "Grit is a Ruby library for extracting information from a git repository in an object oriented manner."[m
[32m+[m[32m    s.email = "tom@mojombo.com"[m
[32m+[m[32m    s.homepage = "http://github.com/mojombo/grit"[m
[32m+[m[32m    s.description = "Grit is a Ruby library for extracting information from a git repository in an object oriented manner."[m
[32m+[m[32m    s.authors = ["Tom Preston-Werner", "Scott Chacon"][m
[32m+[m[32m    s.files = FileList["[A-Z]*.*", "lib/**/*"][m
[32m+[m[32m    s.add_dependency('mime-types', '>= 1.15')[m
[32m+[m[32m    s.add_dependency('diff-lcs', '>= 1.1.2')[m
[32m+[m[32m  end[m

[1mdiff --git a/grit/lib/grit/git-ruby/git_object.rb b/grit/lib/grit/git-ruby/git_object.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..833b035[m
[1m--- /dev/null[m
[1m+++ b/grit/lib/grit/git-ruby/git_object.rb[m
[36m@@ -0,0 +1,350 @@[m
[32m+[m[32m#[m
[32m+[m[32m# converted from the gitrb project[m
[32m+[m[32m#[m
[32m+[m[32m# authors:[m
[32m+[m[32m#    Matthias Lederhofer <matled@gmx.net>[m
[32m+[m[32m#    Simon 'corecode' Schubert <corecode@fs.ei.tum.de>[m
[32m+[m[32m#    Scott Chacon <schacon@gmail.com>[m
[32m+[m[32m#[m
[32m+[m[32m# provides native ruby access to git objects and pack files[m
[32m+[m[32m#[m
[32m+[m
[32m+[m[32m# These classes translate the raw binary data kept in the sha encoded files[m
[32m+[m[32m# into parsed data that can then be used in another fashion[m
[32m+[m[32mrequire 'stringio'[m
[32m+[m
[32m+[m[32mmodule Grit[m
[32m+[m[32m  module GitRuby[m
[32m+[m
[32m+[m[32m  # class for author/committer/tagger lines[m
[32m+[m[32m  class UserInfo[m
[32m+[m[32m    attr_accessor :name, :email, :date, :offset[m
[32m+[m
[32m+[m[32m    def initialize(str)[m
[32m+[m[32m      @email = ''[m
[32m+[m[32m      @date = Time.now[m
[32m+[m[32m      @offset = 0[m
[32m+[m
[32m+[m[32m      m = /^(.*?) <(.*)> (\d+) ([+-])0*(\d+?)$/.match(str)[m
[32m+[m[32m      if !m[m
[32m+[m[32m        case str[m
[32m+[m[32m        when /<.+>/[m
[32m+[m[32m          m, @name, @email = *str.match(/(.*) <(.+?)>/)[m
[32m+[m[32m        else[m
[32m+[m[32m          @name = str[m
[32m+[m[32m        end[m
[32m+[m[32m      else[m
[32m+[m[32m        @name = m[1][m
[32m+[m[32m        @email = m[2][m
[32m+[m[32m        @date = Time.at(Integer(m[3]))[m
[32m+[m[32m        @offset = (m[4] == "-" ? -1 : 1)*Integer(m[5])[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def to_s[m
[32m+[m[32m      "%s <%s> %s %+05d" % [@name, @email, @date.to_i, @offset][m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  # base class for all git objects (blob, tree, commit, tag)[m
[32m+[m[32m  class GitObject[m
[32m+[m[32m    attr_accessor :repository[m
[32m+[m
[32m+[m[32m    def GitObject.from_raw(rawobject, repository = nil)[m
[32m+[m[32m      case rawobject.type[m
[32m+[m[32m      when :blob[m
[32m+[m[32m        return Blob.from_raw(rawobject, repository)[m
[32m+[m[32m      when :tree[m
[32m+[m[32m        return Tree.from_raw(rawobject, repository)[m
[32m+[m[32m      when :commit[m
[32m+[m[32m        return Commit.from_raw(rawobject, repository)[m
[32m+[m[32m      when :tag[m
[32m+[m[32m        return Tag.from_raw(rawobject, repository)[m
[32m+[m[32m      else[m
[32m+[m[32m        raise RuntimeError, "got invalid object-type"[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def initialize[m
[32m+[m[32m      raise NotImplemented, "abstract class"[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def type[m
[32m+[m[32m      raise NotImplemented, "abstract class"[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def raw_content[m
[32m+[m[32m      raise NotImplemented, "abstract class"[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def sha1[m
[32m+[m[32m      Digest::SHA1.hexdigest("%s %d\0" % \[m
[32m+[m[32m                             [self.type, self.raw_content.length] + \[m
[32m+[m[32m                             self.raw_content)[m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  class Blob < GitObject[m
[32m+[m[32m    attr_accessor :content[m
[32m+[m
[32m+[m[32m    def self.from_raw(rawobject, repository)[m
[32m+[m[32m      new(rawobject.content)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def initialize(content, repository=nil)[m
[32m+[m[32m      @content = content[m
[32m+[m[32m      @repository = repository[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def type[m
[32m+[m[32m      :blob[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def raw_content[m
[32m+[m[32m      @content[m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  class DirectoryEntry[m
[32m+[m[32m    S_IFMT  = 00170000[m
[32m+[m[32m    S_IFLNK =  0120000[m
[32m+[m[32m    S_IFREG =  0100000[m
[32m+[m[32m    S_IFDIR =  0040000[m
[32m+[m[32m    S_IFGITLINK = 0160000[m
[32m+[m[32m    attr_accessor :mode, :name, :sha1[m
[32m+[m[32m    def initialize(mode, filename, sha1o)[m
[32m+[m[32m      @mode = 0[m
[32m+[m[32m      mode.each_byte do |i|[m
[32m+[m[32m        @mode = (@mode << 3) | (i-'0'.getord(0))[m
[32m+[m[32m      end[m
[32m+[m[32m      @name = filename[m
[32m+[m[32m      @sha1 = sha1o[m
[32m+[m[32m      if ![S_IFLNK, S_IFDIR, S_IFREG, S_IFGITLINK].include?(@mode & S_IFMT)[m
[32m+[m[32m        raise RuntimeError, "unknown type for directory entry"[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def type[m
[32m+[m[32m      case @mode & S_IFMT[m
[32m+[m[32m      when S_IFGITLINK[m
[32m+[m[32m        @type = :submodule[m
[32m+[m[32m      when S_IFLNK[m
[32m+[m[32m        @type = :link[m
[32m+[m[32m      when S_IFDIR[m
[32m+[m[32m        @type = :directory[m
[32m+[m[32m      when S_IFREG[m
[32m+[m[32m        @type = :file[m
[32m+[m[32m      else[m
[32m+[m[32m        raise RuntimeError, "unknown type for directory entry"[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def type=(type)[m
[32m+[m[32m      case @type[m
[32m+[m[32m      when :link[m
[32m+[m[32m        @mode = (@mode & ~S_IFMT) | S_IFLNK[m
[32m+[m[32m      when :directory[m
[32m+[m[32m        @mode = (@mode & ~S_IFMT) | S_IFDIR[m
[32m+[m[32m      when :file[m
[32m+[m[32m        @mode = (@mode & ~S_IFMT) | S_IFREG[m
[32m+[m[32m      when :submodule[m
[32m+[m[32m        @mode = (@mode & ~S_IFMT) | S_IFGITLINK[m
[32m+[m[32m      else[m
[32m+[m[32m        raise RuntimeError, "invalid type"[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def format_type[m
[32m+[m[32m      case type[m
[32m+[m[32m      when :link[m
[32m+[m[32m        'link'[m
[32m+[m[32m      when :directory[m
[32m+[m[32m        'tree'[m
[32m+[m[32m      when :file[m
[32m+[m[32m        'blob'[m
[32m+[m[32m      when :submodule[m
[32m+[m[32m        'commit'[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def format_mode[m
[32m+[m[32m      "%06o" % @mode[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def raw[m
[32m+[m[32m      "%o %s\0%s" % [@mode, @name, [@sha1].pack("H*")][m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m
[32m+[m[32m  def self.read_bytes_until(io, char)[m
[32m+[m[32m    string = ''[m
[32m+[m[32m    if RUBY_VERSION > '1.9'[m
[32m+[m[32m      while ((next_char = io.getc) != char) && !io.eof[m
[32m+[m[32m        string += next_char[m
[32m+[m[32m      end[m
[32m+[m[32m    else[m
[32m+[m[32m      while ((next_char = io.getc.chr) != char) && !io.eof[m
[32m+[m[32m        string += next_char[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m[32m    string[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m
[32m+[m[32m  class Tree < GitObject[m
[32m+[m[32m    attr_accessor :entry[m
[32m+[m
[32m+[m[32m    def self.from_raw(rawobject, repository=nil)[m
[32m+[m[32m      raw = StringIO.new(rawobject.content)[m
[32m+[m
[32m+[m[32m      entries = [][m
[32m+[m[32m      while !raw.eof?[m
[32m+[m[32m        mode      = Grit::GitRuby.read_bytes_until(raw, ' ')[m
[32m+[m[32m        file_name = Grit::GitRuby.read_bytes_until(raw, "\0")[m
[32m+[m[32m        raw_sha   = raw.read(20)[m
[32m+[m[32m        sha = raw_sha.unpack("H*").first[m
[32m+[m
[32m+[m[32m        entries << DirectoryEntry.new(mode, file_name, sha)[m
[32m+[m[32m      end[m
[32m+[m[32m      new(entries, repository)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def initialize(entries=[], repository = nil)[m
[32m+[m[32m      @entry = entries[m
[32m+[m[32m      @repository = repository[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def type[m
[32m+[m[32m      :tree[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def raw_content[m
[32m+[m[32m      # TODO: sort correctly[m
[32m+[m[32m      #@entry.sort { |a,b| a.name <=> b.name }.[m
[32m+[m[32m      @entry.collect { |e| [[e.format_mode, e.format_type, e.sha1].join(' '), e.name].join("\t") }.join("\n")[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def actual_raw[m
[32m+[m[32m      #@entry.collect { |e| e.raw.join(' '), e.name].join("\t") }.join("\n")[m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  class Commit < GitObject[m
[32m+[m[32m    attr_accessor :author, :committer, :tree, :parent, :message, :headers[m
[32m+[m
[32m+[m[32m    def self.from_raw(rawobject, repository=nil)[m
[32m+[m[32m      parent = [][m
[32m+[m[32m      tree = author = committer = nil[m
[32m+[m
[32m+[m[32m      headers, message = rawobject.content.split(/\n\n/, 2)[m
[32m+[m[32m      all_headers = headers.split(/\n/).map { |header| header.split(/ /, 2) }[m
[32m+[m[32m      all_headers.each do |key, value|[m
[32m+[m[32m        case key[m
[32m+[m[32m        when "tree"[m
[32m+[m[32m          tree = value[m
[32m+[m[32m        when "parent"[m
[32m+[m[32m          parent.push(value)[m
[32m+[m[32m        when "author"[m
[32m+[m[32m          author = UserInfo.new(value)[m
[32m+[m[32m        when "committer"[m
[32m+[m[32m          committer = UserInfo.new(value)[m
[32m+[m[32m        else[m
[32m+[m[32m          warn "unknown header '%s' in commit %s" % \[m
[32m+[m[32m            [key, rawobject.sha1.unpack("H*")[0]][m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m[32m      if not tree && author && committer[m
[32m+[m[32m        raise RuntimeError, "incomplete raw commit object"[m
[32m+[m[32m      end[m
[32m+[m[32m      new(tree, parent, author, committer, message, headers, repository)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def initialize(tree, parent, author, committer, message, headers, repository=nil)[m
[32m+[m[32m      @tree = tree[m
[32m+[m[32m      @author = author[m
[32m+[m[32m      @parent = parent[m
[32m+[m[32m      @committer = committer[m
[32m+[m[32m      @message = message[m
[32m+[m[32m      @headers = headers[m
[32m+[m[32m      @repository = repository[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def type[m
[32m+[m[32m      :commit[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def raw_content[m
[32m+[m[32m      "tree %s\n%sauthor %s\ncommitter %s\n\n" % [[m
[32m+[m[32m        @tree,[m
[32m+[m[32m        @parent.collect { |i| "parent %s\n" % i }.join,[m
[32m+[m[32m        @author, @committer] + @message[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def raw_log(sha)[m
[32m+[m[32m      output = "commit #{sha}\n"[m
[32m+[m[32m      output += @headers + "\n\n"[m
[32m+[m[32m      output += @message.split("\n").map { |l| '    ' + l }.join("\n") + "\n\n"[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  class Tag < GitObject[m
[32m+[m[32m    attr_accessor :object, :type, :tag, :tagger, :message[m
[32m+[m
[32m+[m[32m    def self.from_raw(rawobject, repository=nil)[m
[32m+[m
[32m+[m[32m      headers, message = rawobject.content.split(/\n\n/, 2)[m
[32m+[m[32m      headers = headers.split(/\n/).map { |header| header.split(' ', 2) }[m
[32m+[m
[32m+[m[32m      object = ''[m
[32m+[m[32m      type = ''[m
[32m+[m[32m      tag = ''[m
[32m+[m[32m      tagger = ''[m
[32m+[m
[32m+[m[32m      headers.each do |key, value|[m
[32m+[m[32m        case key[m
[32m+[m[32m        when "object"[m
[32m+[m[32m          object = value[m
[32m+[m[32m        when "type"[m
[32m+[m[32m          if !["blob", "tree", "commit", "tag"].include?(value)[m
[32m+[m[32m            raise RuntimeError, "invalid type in tag"[m
[32m+[m[32m          end[m
[32m+[m[32m          type = value.to_sym[m
[32m+[m[32m        when "tag"[m
[32m+[m[32m          tag = value[m
[32m+[m[32m        when "tagger"[m
[32m+[m[32m          tagger = UserInfo.new(value)[m
[32m+[m[32m        else[m
[32m+[m[32m          warn "unknown header '%s' in tag" % \[m
[32m+[m[32m            [key, rawobject.sha1.unpack("H*")[0]][m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      if not object && type && tag && tagger[m
[32m+[m[32m        raise RuntimeError, "incomplete raw tag object"[m
[32m+[m[32m      end[m
[32m+[m[32m      new(object, type, tag, tagger, message, repository)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def initialize(object, type, tag, tagger, message, repository=nil)[m
[32m+[m[32m      @object = object[m
[32m+[m[32m      @type = type[m
[32m+[m[32m      @tag = tag[m
[32m+[m[32m      @tagger = tagger[m
[32m+[m[32m      @repository = repository[m
[32m+[m[32m      @message = message[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def raw_content[m
[32m+[m[32m      "object %s\ntype %s\ntag %s\ntagger %s\n\n" % \[m
[32m+[m[32m        [@object, @type, @tag, @tagger] + @message[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def type[m
[32m+[m[32m      :tag[m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  end[m
[32m+[m[32mend[m
[1mdiff --git a/grit/lib/grit/git-ruby/internal/file_window.rb b/grit/lib/grit/git-ruby/internal/file_window.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..c3d8a2d[m
[1m--- /dev/null[m
[1m+++ b/grit/lib/grit/git-ruby/internal/file_window.rb[m
[36m@@ -0,0 +1,58 @@[m
[32m+[m[32m#[m
[32m+[m[32m# converted from the gitrb project[m
[32m+[m[32m#[m
[32m+[m[32m# authors:[m
[32m+[m[32m#    Matthias Lederhofer <matled@gmx.net>[m
[32m+[m[32m#    Simon 'corecode' Schubert <corecode@fs.ei.tum.de>[m
[32m+[m[32m#    Scott Chacon <schacon@gmail.com>[m
[32m+[m[32m#[m
[32m+[m[32m# provides native ruby access to git objects and pack files[m
[32m+[m[32m#[m
[32m+[m
[32m+[m[32mmodule Grit[m
[32m+[m[32m  module GitRuby[m
[32m+[m[32m    module Internal[m
[32m+[m[32m      class FileWindow[m
[32m+[m[32m        def initialize(file, version = 1)[m
[32m+[m[32m          @file = file[m
[32m+[m[32m          @offset = nil[m
[32m+[m[32m          if version == 2[m
[32m+[m[32m            @global_offset = 8[m
[32m+[m[32m          else[m
[32m+[m[32m            @global_offset = 0[m
[32m+[m[32m          end[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        def unmap[m
[32m+[m[32m          @file = nil[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        def [](*idx)[m
[32m+[m[32m          idx = idx[0] if idx.length == 1[m
[32m+[m[32m          case idx[m
[32m+[m[32m          when Range[m
[32m+[m[32m            offset = idx.first[m
[32m+[m[32m            len = idx.last - idx.first + idx.exclude_end? ? 0 : 1[m
[32m+[m[32m          when Fixnum[m
[32m+[m[32m            offset = idx[m
[32m+[m[32m            len = nil[m
[32m+[m[32m          when Array[m
[32m+[m[32m            offset, len = idx[m
[32m+[m[32m          else[m
[32m+[m[32m            raise RuntimeError, "invalid index param: #{idx.class}"[m
[32m+[m[32m          end[m
[32m+[m[32m          if @offset != offset[m
[32m+[m[32m            @file.seek(offset + @global_offset)[m
[32m+[m[32m          end[m
[32m+[m[32m          @offset = offset + len ? len : 1[m
[32m+[m[32m          if not len[m
[32m+[m[32m            @file.read(1).getord(0)[m
[32m+[m[32m          else[m
[32m+[m[32m            @file.read(len)[m
[32m+[m[32m          end[m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m[32mend[m
[32m+[m
[1mdiff --git a/grit/lib/grit/git-ruby/internal/loose.rb b/grit/lib/grit/git-ruby/internal/loose.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..88ddc9a[m
[1m--- /dev/null[m
[1m+++ b/grit/lib/grit/git-ruby/internal/loose.rb[m
[36m@@ -0,0 +1,137 @@[m
[32m+[m[32m#[m
[32m+[m[32m# converted from the gitrb project[m
[32m+[m[32m#[m
[32m+[m[32m# authors:[m
[32m+[m[32m#    Matthias Lederhofer <matled@gmx.net>[m
[32m+[m[32m#    Simon 'corecode' Schubert <corecode@fs.ei.tum.de>[m
[32m+[m[32m#    Scott Chacon <schacon@gmail.com>[m
[32m+[m[32m#[m
[32m+[m[32m# provides native ruby access to git objects and pack files[m
[32m+[m[32m#[m
[32m+[m
[32m+[m[32mrequire 'zlib'[m
[32m+[m[32mrequire 'digest/sha1'[m
[32m+[m[32mrequire 'grit/git-ruby/internal/raw_object'[m
[32m+[m
[32m+[m[32mmodule Grit[m
[32m+[m[32m  module GitRuby[m
[32m+[m[32m    module Internal[m
[32m+[m[32m      class LooseObjectError < StandardError[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      class LooseStorage[m
[32m+[m[32m        def initialize(directory)[m
[32m+[m[32m          @directory = directory[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        def [](sha1)[m
[32m+[m[32m          sha1 = sha1.unpack("H*")[0][m
[32m+[m[32m          begin[m
[32m+[m[32m            return nil unless sha1[0...2] && sha1[2..39][m
[32m+[m[32m            path = @directory + '/' + sha1[0...2] + '/' + sha1[2..39][m
[32m+[m[32m            get_raw_object(File.read(path))[m
[32m+[m[32m          rescue Errno::ENOENT[m
[32m+[m[32m            nil[m
[32m+[m[32m          end[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        def get_raw_object(buf)[m
[32m+[m[32m          if buf.length < 2[m
[32m+[m[32m            raise LooseObjectError, "object file too small"[m
[32m+[m[32m          end[m
[32m+[m
[32m+[m[32m          if legacy_loose_object?(buf)[m
[32m+[m[32m            content = Zlib::Inflate.inflate(buf)[m
[32m+[m[32m            header, content = content.split(/\0/, 2)[m
[32m+[m[32m            if !header || !content[m
[32m+[m[32m              raise LooseObjectError, "invalid object header"[m
[32m+[m[32m            end[m
[32m+[m[32m            type, size = header.split(/ /, 2)[m
[32m+[m[32m            if !%w(blob tree commit tag).include?(type) || size !~ /^\d+$/[m
[32m+[m[32m              raise LooseObjectError, "invalid object header"[m
[32m+[m[32m            end[m
[32m+[m[32m            type = type.to_sym[m
[32m+[m[32m            size = size.to_i[m
[32m+[m[32m          else[m
[32m+[m[32m            type, size, used = unpack_object_header_gently(buf)[m
[32m+[m[32m            content = Zlib::Inflate.inflate(buf[used..-1])[m
[32m+[m[32m          end[m
[32m+[m[32m          raise LooseObjectError, "size mismatch" if content.length != size[m
[32m+[m[32m          return RawObject.new(type, content)[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        # currently, I'm using the legacy format because it's easier to do[m
[32m+[m[32m        # this function takes content and a type and writes out the loose object and returns a sha[m
[32m+[m[32m        def put_raw_object(content, type)[m
[32m+[m[32m          size = content.length.to_s[m
[32m+[m[32m          LooseStorage.verify_header(type, size)[m
[32m+[m
[32m+[m[32m          header = "#{type} #{size}\0"[m
[32m+[m[32m          store = header + content[m
[32m+[m
[32m+[m[32m          sha1 = Digest::SHA1.hexdigest(store)[m
[32m+[m[32m          path = @directory+'/'+sha1[0...2]+'/'+sha1[2..40][m
[32m+[m
[32m+[m[32m          if !File.exists?(path)[m
[32m+[m[32m            content = Zlib::Deflate.deflate(store)[m
[32m+[m
[32m+[m[32m            FileUtils.mkdir_p(@directory+'/'+sha1[0...2])[m
[32m+[m[32m            File.open(path, 'wb') do |f|[m
[32m+[m[32m              f.write content[m
[32m+[m[32m            end[m
[32m+[m[32m          end[m
[32m+[m[32m          return sha1[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        # simply figure out the sha[m
[32m+[m[32m        def self.calculate_sha(content, type)[m
[32m+[m[32m          size = content.length.to_s[m
[32m+[m[32m          verify_header(type, size)[m
[32m+[m[32m          header = "#{type} #{size}\0"[m
[32m+[m[32m          store = header + content[m
[32m+[m
[32m+[m[32m          Digest::SHA1.hexdigest(store)[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        def self.verify_header(type, size)[m
[32m+[m[32m          if !%w(blob tree commit tag).include?(type) || size !~ /^\d+$/[m
[32m+[m[32m            raise LooseObjectError, "invalid object header"[m
[32m+[m[32m          end[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        # private[m
[32m+[m[32m        def unpack_object_header_gently(buf)[m
[32m+[m[32m          used = 0[m
[32m+[m[32m          c = buf.getord(used)[m
[32m+[m[32m          used += 1[m
[32m+[m
[32m+[m[32m          type = (c >> 4) & 7;[m
[32m+[m[32m          size = c & 15;[m
[32m+[m[32m          shift = 4;[m
[32m+[m[32m          while c & 0x80 != 0[m
[32m+[m[32m            if buf.length <= used[m
[32m+[m[32m              raise LooseObjectError, "object file too short"[m
[32m+[m[32m            end[m
[32m+[m[32m            c = buf.getord(used)[m
[32m+[m[32m            used += 1[m
[32m+[m
[32m+[m[32m            size += (c & 0x7f) << shift[m
[32m+[m[32m            shift += 7[m
[32m+[m[32m          end[m
[32m+[m[32m          type = OBJ_TYPES[type][m
[32m+[m[32m          if ![:blob, :tree, :commit, :tag].include?(type)[m
[32m+[m[32m            raise LooseObjectError, "invalid loose object type"[m
[32m+[m[32m          end[m
[32m+[m[32m          return [type, size, used][m
[32m+[m[32m        end[m
[32m+[m[32m        private :unpack_object_header_gently[m
[32m+[m
[32m+[m[32m        def legacy_loose_object?(buf)[m
[32m+[m[32m          word = (buf.getord(0) << 8) + buf.getord(1)[m
[32m+[m[32m          buf.getord(0) == 0x78 && word % 31 == 0[m
[32m+[m[32m        end[m
[32m+[m[32m        private :legacy_loose_object?[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m[32mend[m
[1mdiff --git a/grit/lib/grit/git-ruby/internal/pack.rb b/grit/lib/grit/git-ruby/internal/pack.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..a8b416a[m
[1m--- /dev/null[m
[1m+++ b/grit/lib/grit/git-ruby/internal/pack.rb[m
[36m@@ -0,0 +1,384 @@[m
[32m+[m[32m#[m
[32m+[m[32m# converted from the gitrb project[m
[32m+[m[32m#[m
[32m+[m[32m# authors:[m
[32m+[m[32m#    Matthias Lederhofer <matled@gmx.net>[m
[32m+[m[32m#    Simon 'corecode' Schubert <corecode@fs.ei.tum.de>[m
[32m+[m[32m#    Scott Chacon <schacon@gmail.com>[m
[32m+[m[32m#[m
[32m+[m[32m# provides native ruby access to git objects and pack files[m
[32m+[m[32m#[m
[32m+[m
[32m+[m[32mrequire 'zlib'[m
[32m+[m[32mrequire 'grit/git-ruby/internal/raw_object'[m
[32m+[m[32mrequire 'grit/git-ruby/internal/file_window'[m
[32m+[m
[32m+[m[32mPACK_SIGNATURE = "PACK"[m
[32m+[m[32mPACK_IDX_SIGNATURE = "\377tOc"[m
[32m+[m
[32m+[m[32mmodule Grit[m
[32m+[m[32m  module GitRuby[m
[32m+[m[32m    module Internal[m
[32m+[m[32m      class PackFormatError < StandardError[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      class PackStorage[m
[32m+[m[32m        OBJ_OFS_DELTA = 6[m
[32m+[m[32m        OBJ_REF_DELTA = 7[m
[32m+[m
[32m+[m[32m        FanOutCount = 256[m
[32m+[m[32m        SHA1Size = 20[m
[32m+[m[32m        IdxOffsetSize = 4[m
[32m+[m[32m        OffsetSize = 4[m
[32m+[m[32m        CrcSize = 4[m
[32m+[m[32m        OffsetStart = FanOutCount * IdxOffsetSize[m
[32m+[m[32m        SHA1Start = OffsetStart + OffsetSize[m
[32m+[m[32m        EntrySize = OffsetSize + SHA1Size[m
[32m+[m[32m        EntrySizeV2 = SHA1Size + CrcSize + OffsetSize[m
[32m+[m
[32m+[m[32m        def initialize(file)[m
[32m+[m[32m          if file =~ /\.idx$/[m
[32m+[m[32m            file = file[0...-3] + 'pack'[m
[32m+[m[32m          end[m
[32m+[m[32m          @name = file[m
[32m+[m[32m          @cache = {}[m
[32m+[m[32m          init_pack[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        def with_idx(index_file = nil)[m
[32m+[m[32m          if !index_file[m
[32m+[m[32m            index_file = @name[m
[32m+[m[32m            idxfile = File.open(@name[0...-4]+'idx', 'rb')[m
[32m+[m[32m          else[m
[32m+[m[32m            idxfile = File.open(index_file, 'rb')[m
[32m+[m[32m          end[m
[32m+[m
[32m+[m[32m          # read header[m
[32m+[m[32m          sig = idxfile.read(4)[m
[32m+[m[32m          ver = idxfile.read(4).unpack("N")[0][m
[32m+[m
[32m+[m[32m          if sig == PACK_IDX_SIGNATURE[m
[32m+[m[32m            if(ver != 2)[m
[32m+[m[32m              raise PackFormatError, "pack #@name has unknown pack file version #{ver}"[m
[32m+[m[32m            end[m
[32m+[m[32m            @version = 2[m
[32m+[m[32m          else[m
[32m+[m[32m            @version = 1[m
[32m+[m[32m          end[m
[32m+[m
[32m+[m[32m          idx = FileWindow.new(idxfile, @version)[m
[32m+[m[32m          yield idx[m
[32m+[m[32m          idx.unmap[m
[32m+[m[32m        ensure[m
[32m+[m[32m          idxfile.close[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        def with_packfile[m
[32m+[m[32m          packfile = File.open(@name, 'rb')[m
[32m+[m[32m          yield packfile[m
[32m+[m[32m        ensure[m
[32m+[m[32m          packfile.close[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        def cache_objects[m
[32m+[m[32m          @cache = {}[m
[32m+[m[32m          with_packfile do |packfile|[m
[32m+[m[32m            each_entry do |sha, offset|[m
[32m+[m[32m              data, type = unpack_object(packfile, offset, {:caching => true})[m
[32m+[m[32m              if data[m
[32m+[m[32m                @cache[sha] = RawObject.new(OBJ_TYPES[type], data)[m
[32m+[m[32m              end[m
[32m+[m[32m            end[m
[32m+[m[32m          end[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        def name[m
[32m+[m[32m          @name[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        def close[m
[32m+[m[32m          # shouldnt be anything open now[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        # given an index file, list out the shas that it's packfile contains[m
[32m+[m[32m        def get_shas[m
[32m+[m[32m          shas = [][m
[32m+[m[32m          each_sha1 { |sha| shas << sha.unpack("H*")[0] }[m
[32m+[m[32m          shas[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        def [](sha1)[m
[32m+[m[32m          if obj = @cache[sha1][m
[32m+[m[32m            return obj[m
[32m+[m[32m          end[m
[32m+[m
[32m+[m[32m          offset = find_object(sha1)[m
[32m+[m[32m          return nil if !offset[m
[32m+[m[32m          @cache[sha1] = obj = parse_object(offset)[m
[32m+[m[32m          return obj[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        def init_pack[m
[32m+[m[32m          with_idx do |idx|[m
[32m+[m[32m            @offsets = [0][m
[32m+[m[32m            FanOutCount.times do |i|[m
[32m+[m[32m              pos = idx[i * IdxOffsetSize,IdxOffsetSize].unpack('N')[0][m
[32m+[m[32m              if pos < @offsets[i][m
[32m+[m[32m                raise PackFormatError, "pack #@name has discontinuous index #{i}"[m
[32m+[m[32m              end[m
[32m+[m[32m              @offsets << pos[m
[32m+[m[32m            end[m
[32m+[m[32m            @size = @offsets[-1][m
[32m+[m[32m          end[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        def each_entry[m
[32m+[m[32m          with_idx do |idx|[m
[32m+[m[32m            if @version == 2[m
[32m+[m[32m              data = read_data_v2(idx)[m
[32m+[m[32m              data.each do |sha1, crc, offset|[m
[32m+[m[32m                yield sha1, offset[m
[32m+[m[32m              end[m
[32m+[m[32m            else[m
[32m+[m[32m              pos = OffsetStart[m
[32m+[m[32m              @size.times do[m
[32m+[m[32m                offset = idx[pos,OffsetSize].unpack('N')[0][m
[32m+[m[32m                sha1 = idx[pos+OffsetSize,SHA1Size][m
[32m+[m[32m                pos += EntrySize[m
[32m+[m[32m                yield sha1, offset[m
[32m+[m[32m              end[m
[32m+[m[32m            end[m
[32m+[m[32m          end[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        def read_data_v2(idx)[m
[32m+[m[32m          data = [][m
[32m+[m[32m          pos = OffsetStart[m
[32m+[m[32m          @size.times do |i|[m
[32m+[m[32m            data[i] = [idx[pos,SHA1Size], 0, 0][m
[32m+[m[32m            pos += SHA1Size[m
[32m+[m[32m          end[m
[32m+[m[32m          @size.times do |i|[m
[32m+[m[32m            crc = idx[pos,CrcSize][m
[32m+[m[32m            data[i][1] = crc[m
[32m+[m[32m            pos += CrcSize[m
[32m+[m[32m          end[m
[32m+[m[32m          @size.times do |i|[m
[32m+[m[32m            offset = idx[pos,OffsetSize].unpack('N')[0][m
[32m+[m[32m            data[i][2] = offset[m
[32m+[m[32m            pos += OffsetSize[m
[32m+[m[32m          end[m
[32m+[m[32m          data[m
[32m+[m[32m        end[m
[32m+[m[32m        private :read_data_v2[m
[32m+[m
[32m+[m[32m        def each_sha1[m
[32m+[m[32m          with_idx do |idx|[m
[32m+[m[32m            if @version == 2[m
[32m+[m[32m              data = read_data_v2(idx)[m
[32m+[m[32m              data.each do |sha1, crc, offset|[m
[32m+[m[32m                yield sha1[m
[32m+[m[32m              end[m
[32m+[m[32m            else[m
[32m+[m[32m              pos = SHA1Start[m
[32m+[m[32m              @size.times do[m
[32m+[m[32m                sha1 = idx[pos,SHA1Size][m
[32m+[m[32m                pos += EntrySize[m
[32m+[m[32m                yield sha1[m
[32m+[m[32m              end[m
[32m+[m[32m            end[m
[32m+[m[32m          end[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        def find_object_in_index(idx, sha1)[m
[32m+[m[32m          slot = sha1.getord(0)[m
[32m+[m[32m          return nil if !slot[m
[32m+[m[32m          first, last = @offsets[slot,2][m
[32m+[m[32m          while first < last[m
[32m+[m[32m            mid = (first + last) / 2[m
[32m+[m[32m            if @version == 2[m
[32m+[m[32m              midsha1 = idx[OffsetStart + (mid * SHA1Size), SHA1Size][m
[32m+[m[32m              cmp = midsha1 <=> sha1[m
[32m+[m
[32m+[m[32m              if cmp < 0[m
[32m+[m[32m                first = mid + 1[m
[32m+[m[32m              elsif cmp > 0[m
[32m+[m[32m                last = mid[m
[32m+[m[32m              else[m
[32m+[m[32m                pos = OffsetStart + (@size * (SHA1Size + CrcSize)) + (mid * OffsetSize)[m
[32m+[m[32m                offset = idx[pos, OffsetSize].unpack('N')[0][m
[32m+[m[32m                return offset[m
[32m+[m[32m              end[m
[32m+[m[32m            else[m
[32m+[m[32m              midsha1 = idx[SHA1Start + mid * EntrySize,SHA1Size][m
[32m+[m[32m              cmp = midsha1 <=> sha1[m
[32m+[m
[32m+[m[32m              if cmp < 0[m
[32m+[m[32m                first = mid + 1[m
[32m+[m[32m              elsif cmp > 0[m
[32m+[m[32m                last = mid[m
[32m+[m[32m              else[m
[32m+[m[32m                pos = OffsetStart + mid * EntrySize[m
[32m+[m[32m                offset = idx[pos,OffsetSize].unpack('N')[0][m
[32m+[m[32m                return offset[m
[32m+[m[32m              end[m
[32m+[m[32m            end[m
[32m+[m[32m          end[m
[32m+[m[32m          nil[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        def find_object(sha1)[m
[32m+[m[32m          obj = nil[m
[32m+[m[32m          with_idx do |idx|[m
[32m+[m[32m            obj = find_object_in_index(idx, sha1)[m
[32m+[m[32m          end[m
[32m+[m[32m          obj[m
[32m+[m[32m        end[m
[32m+[m[32m        private :find_object[m
[32m+[m
[32m+[m[32m        def parse_object(offset)[m
[32m+[m[32m          obj = nil[m
[32m+[m[32m          with_packfile do |packfile|[m
[32m+[m[32m            data, type = unpack_object(packfile, offset)[m
[32m+[m[32m            obj = RawObject.new(OBJ_TYPES[type], data)[m
[32m+[m[32m          end[m
[32m+[m[32m          obj[m
[32m+[m[32m        end[m
[32m+[m[32m        protected :parse_object[m
[32m+[m
[32m+[m[32m        def unpack_object(packfile, offset, options = {})[m
[32m+[m[32m          obj_offset = offset[m
[32m+[m[32m          packfile.seek(offset)[m
[32m+[m
[32m+[m[32m          c = packfile.read(1).getord(0)[m
[32m+[m[32m          size = c & 0xf[m
[32m+[m[32m          type = (c >> 4) & 7[m
[32m+[m[32m          shift = 4[m
[32m+[m[32m          offset += 1[m
[32m+[m[32m          while c & 0x80 != 0[m
[32m+[m[32m            c = packfile.read(1).getord(0)[m
[32m+[m[32m            size |= ((c & 0x7f) << shift)[m
[32m+[m[32m            shift += 7[m
[32m+[m[32m            offset += 1[m
[32m+[m[32m          end[m
[32m+[m
[32m+[m[32m          return [false, false] if !(type == OBJ_COMMIT || type == OBJ_TREE) && options[:caching][m
[32m+[m
[32m+[m[32m          case type[m
[32m+[m[32m          when OBJ_OFS_DELTA, OBJ_REF_DELTA[m
[32m+[m[32m            data, type = unpack_deltified(packfile, type, offset, obj_offset, size, options)[m
[32m+[m[32m            #puts type[m
[32m+[m[32m          when OBJ_COMMIT, OBJ_TREE, OBJ_BLOB, OBJ_TAG[m
[32m+[m[32m            data = unpack_compressed(offset, size)[m
[32m+[m[32m          else[m
[32m+[m[32m            raise PackFormatError, "invalid type #{type}"[m
[32m+[m[32m          end[m
[32m+[m[32m          [data, type][m
[32m+[m[32m        end[m
[32m+[m[32m        private :unpack_object[m
[32m+[m
[32m+[m[32m        def unpack_deltified(packfile, type, offset, obj_offset, size, options = {})[m
[32m+[m[32m          packfile.seek(offset)[m
[32m+[m[32m          data = packfile.read(SHA1Size)[m
[32m+[m
[32m+[m[32m          if type == OBJ_OFS_DELTA[m
[32m+[m[32m            i = 0[m
[32m+[m[32m            c = data.getord(i)[m
[32m+[m[32m            base_offset = c & 0x7f[m
[32m+[m[32m            while c & 0x80 != 0[m
[32m+[m[32m              c = data.getord(i += 1)[m
[32m+[m[32m              base_offset += 1[m
[32m+[m[32m              base_offset <<= 7[m
[32m+[m[32m              base_offset |= c & 0x7f[m
[32m+[m[32m            end[m
[32m+[m[32m            base_offset = obj_offset - base_offset[m
[32m+[m[32m            offset += i + 1[m
[32m+[m[32m          else[m
[32m+[m[32m            base_offset = find_object(data)[m
[32m+[m[32m            offset += SHA1Size[m
[32m+[m[32m          end[m
[32m+[m
[32m+[m[32m          base, type = unpack_object(packfile, base_offset)[m
[32m+[m
[32m+[m[32m          return [false, false] if !(type == OBJ_COMMIT || type == OBJ_TREE) && options[:caching][m
[32m+[m
[32m+[m[32m          delta = unpack_compressed(offset, size)[m
[32m+[m[32m          [patch_delta(base, delta), type][m
[32m+[m[32m        end[m
[32m+[m[32m        private :unpack_deltified[m
[32m+[m
[32m+[m[32m        def unpack_compressed(offset, destsize)[m
[32m+[m[32m          outdata = ""[m
[32m+[m[32m          with_packfile do |packfile|[m
[32m+[m[32m            packfile.seek(offset)[m
[32m+[m[32m            zstr = Zlib::Inflate.new[m
[32m+[m[32m            while outdata.size < destsize[m
[32m+[m[32m              indata = packfile.read(4096)[m
[32m+[m[32m              if indata.size == 0[m
[32m+[m[32m                raise PackFormatError, 'error reading pack data'[m
[32m+[m[32m              end[m
[32m+[m[32m              outdata += zstr.inflate(indata)[m
[32m+[m[32m            end[m
[32m+[m[32m            if outdata.size > destsize[m
[32m+[m[32m              raise PackFormatError, 'error reading pack data'[m
[32m+[m[32m            end[m
[32m+[m[32m            zstr.close[m
[32m+[m[32m          end[m
[32m+[m[32m          outdata[m
[32m+[m[32m        end[m
[32m+[m[32m        private :unpack_compressed[m
[32m+[m
[32m+[m[32m        def patch_delta(base, delta)[m
[32m+[m[32m          src_size, pos = patch_delta_header_size(delta, 0)[m
[32m+[m[32m          if src_size != base.size[m
[32m+[m[32m            raise PackFormatError, 'invalid delta data'[m
[32m+[m[32m          end[m
[32m+[m
[32m+[m[32m          dest_size, pos = patch_delta_header_size(delta, pos)[m
[32m+[m[32m          dest = ""[m
[32m+[m[32m          while pos < delta.size[m
[32m+[m[32m            c = delta.getord(pos)[m
[32m+[m[32m            pos += 1[m
[32m+[m[32m            if c & 0x80 != 0[m
[32m+[m[32m              pos -= 1[m
[32m+[m[32m              cp_off = cp_size = 0[m
[32m+[m[32m              cp_off = delta.getord(pos += 1) if c & 0x01 != 0[m
[32m+[m[32m              cp_off |= delta.getord(pos += 1) << 8 if c & 0x02 != 0[m
[32m+[m[32m              cp_off |= delta.getord(pos += 1) << 16 if c & 0x04 != 0[m
[32m+[m[32m              cp_off |= delta.getord(pos += 1) << 24 if c & 0x08 != 0[m
[32m+[m[32m              cp_size = delta.getord(pos += 1) if c & 0x10 != 0[m
[32m+[m[32m              cp_size |= delta.getord(pos += 1) << 8 if c & 0x20 != 0[m
[32m+[m[32m              cp_size |= delta.getord(pos += 1) << 16 if c & 0x40 != 0[m
[32m+[m[32m              cp_size = 0x10000 if cp_size == 0[m
[32m+[m[32m              pos += 1[m
[32m+[m[32m              dest += base[cp_off,cp_size][m
[32m+[m[32m            elsif c != 0[m
[32m+[m[32m              dest += delta[pos,c][m
[32m+[m[32m              pos += c[m
[32m+[m[32m            else[m
[32m+[m[32m              raise PackFormatError, 'invalid delta data'[m
[32m+[m[32m            end[m
[32m+[m[32m          end[m
[32m+[m[32m          dest[m
[32m+[m[32m        end[m
[32m+[m[32m        private :patch_delta[m
[32m+[m
[32m+[m[32m        def patch_delta_header_size(delta, pos)[m
[32m+[m[32m          size = 0[m
[32m+[m[32m          shift = 0[m
[32m+[m[32m          begin[m
[32m+[m[32m            c = delta.getord(pos)[m
[32m+[m[32m            if c == nil[m
[32m+[m[32m              raise PackFormatError, 'invalid delta header'[m
[32m+[m[32m            end[m
[32m+[m[32m            pos += 1[m
[32m+[m[32m            size |= (c & 0x7f) << shift[m
[32m+[m[32m            shift += 7[m
[32m+[m[32m          end while c & 0x80 != 0[m
[32m+[m[32m          [size, pos][m
[32m+[m[32m        end[m
[32m+[m[32m        private :patch_delta_header_size[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m[32mend[m
[1mdiff --git a/grit/lib/grit/git-ruby/internal/raw_object.rb b/grit/lib/grit/git-ruby/internal/raw_object.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..5cee2ce[m
[1m--- /dev/null[m
[1m+++ b/grit/lib/grit/git-ruby/internal/raw_object.rb[m
[36m@@ -0,0 +1,37 @@[m
[32m+[m[32m#[m
[32m+[m[32m# converted from the gitrb project[m
[32m+[m[32m#[m
[32m+[m[32m# authors:[m
[32m+[m[32m#    Matthias Lederhofer <matled@gmx.net>[m
[32m+[m[32m#    Simon 'corecode' Schubert <corecode@fs.ei.tum.de>[m
[32m+[m[32m#[m
[32m+[m[32m# provides native ruby access to git objects and pack files[m
[32m+[m[32m#[m
[32m+[m
[32m+[m[32mrequire 'digest/sha1'[m
[32m+[m
[32m+[m[32mmodule Grit[m
[32m+[m[32m  module GitRuby[m
[32m+[m[32m    module Internal[m
[32m+[m[32m      OBJ_NONE = 0[m
[32m+[m[32m      OBJ_COMMIT = 1[m
[32m+[m[32m      OBJ_TREE = 2[m
[32m+[m[32m      OBJ_BLOB = 3[m
[32m+[m[32m      OBJ_TAG = 4[m
[32m+[m
[32m+[m[32m      OBJ_TYPES = [nil, :commit, :tree, :blob, :tag].freeze[m
[32m+[m
[32m+[m[32m      class RawObject[m
[32m+[m[32m        attr_accessor :type, :content[m
[32m+[m[32m        def initialize(type, content)[m
[32m+[m[32m          @type = type[m
[32m+[m[32m          @content = content[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        def sha1[m
[32m+[m[32m          Digest::SHA1.digest("%s %d\0" % [@type, @content.length] + @content)[m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m[32mend[m
[1mdiff --git a/grit/lib/grit/git-ruby/object.rb b/grit/lib/grit/git-ruby/object.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..078b739[m
[1m--- /dev/null[m
[1m+++ b/grit/lib/grit/git-ruby/object.rb[m
[36m@@ -0,0 +1,325 @@[m
[32m+[m[32m#[m
[32m+[m[32m# converted from the gitrb project[m
[32m+[m[32m#[m
[32m+[m[32m# authors:[m
[32m+[m[32m#    Matthias Lederhofer <matled@gmx.net>[m
[32m+[m[32m#    Simon 'corecode' Schubert <corecode@fs.ei.tum.de>[m
[32m+[m[32m#    Scott Chacon <schacon@gmail.com>[m
[32m+[m[32m#[m
[32m+[m[32m# provides native ruby access to git objects and pack files[m
[32m+[m[32m#[m
[32m+[m
[32m+[m[32m# These classes translate the raw binary data kept in the sha encoded files[m
[32m+[m[32m# into parsed data that can then be used in another fashion[m
[32m+[m[32mrequire 'stringio'[m
[32m+[m
[32m+[m[32mmodule Grit[m
[32m+[m[32m  module GitRuby[m
[32m+[m
[32m+[m[32m  # class for author/committer/tagger lines[m
[32m+[m[32m  class UserInfo[m
[32m+[m[32m    attr_accessor :name, :email, :date, :offset[m
[32m+[m
[32m+[m[32m    def initialize(str)[m
[32m+[m[32m      m = /^(.*?) <(.*)> (\d+) ([+-])0*(\d+?)$/.match(str)[m
[32m+[m[32m      if !m[m
[32m+[m[32m        raise RuntimeError, "invalid header '%s' in commit" % str[m
[32m+[m[32m      end[m
[32m+[m[32m      @name = m[1][m
[32m+[m[32m      @email = m[2][m
[32m+[m[32m      @date = Time.at(Integer(m[3]))[m
[32m+[m[32m      @offset = (m[4] == "-" ? -1 : 1)*Integer(m[5])[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def to_s[m
[32m+[m[32m      "%s <%s> %s %+05d" % [@name, @email, @date.to_i, @offset][m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  # base class for all git objects (blob, tree, commit, tag)[m
[32m+[m[32m  class Object[m
[32m+[m[32m    attr_accessor :repository[m
[32m+[m
[32m+[m[32m    def Object.from_raw(rawobject, repository = nil)[m
[32m+[m[32m      case rawobject.type[m
[32m+[m[32m      when :blob[m
[32m+[m[32m        return Blob.from_raw(rawobject, repository)[m
[32m+[m[32m      when :tree[m
[32m+[m[32m        return Tree.from_raw(rawobject, repository)[m
[32m+[m[32m      when :commit[m
[32m+[m[32m        return Commit.from_raw(rawobject, repository)[m
[32m+[m[32m      when :tag[m
[32m+[m[32m        return Tag.from_raw(rawobject, repository)[m
[32m+[m[32m      else[m
[32m+[m[32m        raise RuntimeError, "got invalid object-type"[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def initialize[m
[32m+[m[32m      raise NotImplemented, "abstract class"[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def type[m
[32m+[m[32m      raise NotImplemented, "abstract class"[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def raw_content[m
[32m+[m[32m      raise NotImplemented, "abstract class"[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def sha1[m
[32m+[m[32m      Digest::SHA1.hexdigest("%s %d\0" % \[m
[32m+[m[32m                             [self.type, self.raw_content.length] + \[m
[32m+[m[32m                             self.raw_content)[m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  class Blob < Object[m
[32m+[m[32m    attr_accessor :content[m
[32m+[m
[32m+[m[32m    def self.from_raw(rawobject, repository)[m
[32m+[m[32m      new(rawobject.content)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def initialize(content, repository=nil)[m
[32m+[m[32m      @content = content[m
[32m+[m[32m      @repository = repository[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def type[m
[32m+[m[32m      :blob[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def raw_content[m
[32m+[m[32m      @content[m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  class DirectoryEntry[m
[32m+[m[32m    S_IFMT  = 00170000[m
[32m+[m[32m    S_IFLNK =  0120000[m
[32m+[m[32m    S_IFREG =  0100000[m
[32m+[m[32m    S_IFDIR =  0040000[m
[32m+[m
[32m+[m[32m    attr_accessor :mode, :name, :sha1[m
[32m+[m[32m    def initialize(mode, filename, sha1o)[m
[32m+[m[32m      @mode = 0[m
[32m+[m[32m      mode.each_byte do |i|[m
[32m+[m[32m        @mode = (@mode << 3) | (i-'0'[0])[m
[32m+[m[32m      end[m
[32m+[m[32m      @name = filename[m
[32m+[m[32m      @sha1 = sha1o[m
[32m+[m[32m      if ![S_IFLNK, S_IFDIR, S_IFREG].include?(@mode & S_IFMT)[m
[32m+[m[32m        raise RuntimeError, "unknown type for directory entry"[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def type[m
[32m+[m[32m      case @mode & S_IFMT[m
[32m+[m[32m      when S_IFLNK[m
[32m+[m[32m        @type = :link[m
[32m+[m[32m      when S_IFDIR[m
[32m+[m[32m        @type = :directory[m
[32m+[m[32m      when S_IFREG[m
[32m+[m[32m        @type = :file[m
[32m+[m[32m      else[m
[32m+[m[32m        raise RuntimeError, "unknown type for directory entry"[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def type=(type)[m
[32m+[m[32m      case @type[m
[32m+[m[32m      when :link[m
[32m+[m[32m        @mode = (@mode & ~S_IFMT) | S_IFLNK[m
[32m+[m[32m      when :directory[m
[32m+[m[32m        @mode = (@mode & ~S_IFMT) | S_IFDIR[m
[32m+[m[32m      when :file[m
[32m+[m[32m        @mode = (@mode & ~S_IFMT) | S_IFREG[m
[32m+[m[32m      else[m
[32m+[m[32m        raise RuntimeError, "invalid type"[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def format_type[m
[32m+[m[32m      case type[m
[32m+[m[32m      when :link[m
[32m+[m[32m        'link'[m
[32m+[m[32m      when :directory[m
[32m+[m[32m        'tree'[m
[32m+[m[32m      when :file[m
[32m+[m[32m        'blob'[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def format_mode[m
[32m+[m[32m      "%06o" % @mode[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def raw[m
[32m+[m[32m      "%o %s\0%s" % [@mode, @name, [@sha1].pack("H*")][m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m
[32m+[m[32m  def self.read_bytes_until(io, char)[m
[32m+[m[32m    string = ''[m
[32m+[m[32m    if RUBY_VERSION > '1.9'[m
[32m+[m[32m      while ((next_char = io.getc) != char) && !io.eof[m
[32m+[m[32m        string += next_char[m
[32m+[m[32m      end[m
[32m+[m[32m    else[m
[32m+[m[32m      while ((next_char = io.getc.chr) != char) && !io.eof[m
[32m+[m[32m        string += next_char[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m[32m    string[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m
[32m+[m[32m  class Tree < Object[m
[32m+[m[32m    attr_accessor :entry[m
[32m+[m
[32m+[m[32m    def self.from_raw(rawobject, repository=nil)[m
[32m+[m[32m      raw = StringIO.new(rawobject.content)[m
[32m+[m
[32m+[m[32m      entries = [][m
[32m+[m[32m      while !raw.eof?[m
[32m+[m[32m        mode      = Grit::GitRuby.read_bytes_until(raw, ' ')[m
[32m+[m[32m        file_name = Grit::GitRuby.read_bytes_until(raw, "\0")[m
[32m+[m[32m        raw_sha   = raw.read(20)[m
[32m+[m[32m        sha = raw_sha.unpack("H*").first[m
[32m+[m
[32m+[m[32m        entries << DirectoryEntry.new(mode, file_name, sha)[m
[32m+[m[32m      end[m
[32m+[m[32m      new(entries, repository)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def initialize(entries=[], repository = nil)[m
[32m+[m[32m      @entry = entries[m
[32m+[m[32m      @repository = repository[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def type[m
[32m+[m[32m      :tree[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def raw_content[m
[32m+[m[32m      # TODO: sort correctly[m
[32m+[m[32m      #@entry.sort { |a,b| a.name <=> b.name }.[m
[32m+[m[32m      @entry.collect { |e| [[e.format_mode, e.format_type, e.sha1].join(' '), e.name].join("\t") }.join("\n")[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def actual_raw[m
[32m+[m[32m      #@entry.collect { |e| e.raw.join(' '), e.name].join("\t") }.join("\n")[m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  class Commit < Object[m
[32m+[m[32m    attr_accessor :author, :committer, :tree, :parent, :message, :headers[m
[32m+[m
[32m+[m[32m    def self.from_raw(rawobject, repository=nil)[m
[32m+[m[32m      parent = [][m
[32m+[m[32m      tree = author = committer = nil[m
[32m+[m
[32m+[m[32m      headers, message = rawobject.content.split(/\n\n/, 2)[m
[32m+[m[32m      all_headers = headers.split(/\n/).map { |header| header.split(/ /, 2) }[m
[32m+[m[32m      all_headers.each do |key, value|[m
[32m+[m[32m        case key[m
[32m+[m[32m        when "tree"[m
[32m+[m[32m          tree = value[m
[32m+[m[32m        when "parent"[m
[32m+[m[32m          parent.push(value)[m
[32m+[m[32m        when "author"[m
[32m+[m[32m          author = UserInfo.new(value)[m
[32m+[m[32m        when "committer"[m
[32m+[m[32m          committer = UserInfo.new(value)[m
[32m+[m[32m        else[m
[32m+[m[32m          warn "unknown header '%s' in commit %s" % \[m
[32m+[m[32m            [key, rawobject.sha1.unpack("H*")[0]][m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m[32m      if not tree && author && committer[m
[32m+[m[32m        raise RuntimeError, "incomplete raw commit object"[m
[32m+[m[32m      end[m
[32m+[m[32m      new(tree, parent, author, committer, message, headers, repository)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def initialize(tree, parent, author, committer, message, headers, repository=nil)[m
[32m+[m[32m      @tree = tree[m
[32m+[m[32m      @author = author[m
[32m+[m[32m      @parent = parent[m
[32m+[m[32m      @committer = committer[m
[32m+[m[32m      @message = message[m
[32m+[m[32m      @headers = headers[m
[32m+[m[32m      @repository = repository[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def type[m
[32m+[m[32m      :commit[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def raw_content[m
[32m+[m[32m      "tree %s\n%sauthor %s\ncommitter %s\n\n" % [[m
[32m+[m[32m        @tree,[m
[32m+[m[32m        @parent.collect { |i| "parent %s\n" % i }.join,[m
[32m+[m[32m        @author, @committer] + @message[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def raw_log(sha)[m
[32m+[m[32m      output = "commit #{sha}\n"[m
[32m+[m[32m      output += @headers + "\n\n"[m
[32m+[m[32m      output += @message.split("\n").map { |l| '    ' + l }.join("\n") + "\n\n"[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  class Tag < Object[m
[32m+[m[32m    attr_accessor :object, :type, :tag, :tagger, :message[m
[32m+[m
[32m+[m[32m    def self.from_raw(rawobject, repository=nil)[m
[32m+[m[32m      headers, message = rawobject.content.split(/\n\n/, 2)[m
[32m+[m[32m      headers = headers.split(/\n/).map { |header| header.split(/ /, 2) }[m
[32m+[m[32m      headers.each do |key, value|[m
[32m+[m[32m        case key[m
[32m+[m[32m        when "object"[m
[32m+[m[32m          object = value[m
[32m+[m[32m        when "type"[m
[32m+[m[32m          if !["blob", "tree", "commit", "tag"].include?(value)[m
[32m+[m[32m            raise RuntimeError, "invalid type in tag"[m
[32m+[m[32m          end[m
[32m+[m[32m          type = value.to_sym[m
[32m+[m[32m        when "tag"[m
[32m+[m[32m          tag = value[m
[32m+[m[32m        when "tagger"[m
[32m+[m[32m          tagger = UserInfo.new(value)[m
[32m+[m[32m        else[m
[32m+[m[32m          warn "unknown header '%s' in tag" % \[m
[32m+[m[32m            [key, rawobject.sha1.unpack("H*")[0]][m
[32m+[m[32m        end[m
[32m+[m[32m        if not object && type && tag && tagger[m
[32m+[m[32m          raise RuntimeError, "incomplete raw tag object"[m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m[32m      new(object, type, tag, tagger, repository)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def initialize(object, type, tag, tagger, repository=nil)[m
[32m+[m[32m      @object = object[m
[32m+[m[32m      @type = type[m
[32m+[m[32m      @tag = tag[m
[32m+[m[32m      @tagger = tagger[m
[32m+[m[32m      @repository = repository[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def raw_content[m
[32m+[m[32m      "object %s\ntype %s\ntag %s\ntagger %s\n\n" % \[m
[32m+[m[32m        [@object, @type, @tag, @tagger] + @message[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    def type[m
[32m+[m[32m      :tag[m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  end[m
[32m+[m[32mend[m
[1mdiff --git a/grit/lib/grit/git-ruby/repository.rb b/grit/lib/grit/git-ruby/repository.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..261c8a8[m
[1m--- /dev/null[m
[1m+++ b/grit/lib/grit/git-ruby/repository.rb[m
[36m@@ -0,0 +1,767 @@[m
[32m+[m[32m#[m
[32m+[m[32m# converted from the gitrb project[m
[32m+[m[32m#[m
[32m+[m[32m# authors:[m
[32m+[m[32m#    Matthias Lederhofer <matled@gmx.net>[m
[32m+[m[32m#    Simon 'corecode' Schubert <corecode@fs.ei.tum.de>[m
[32m+[m[32m#    Scott Chacon <schacon@gmail.com>[m
[32m+[m[32m#[m
[32m+[m[32m# provides native ruby access to git objects and pack files[m
[32m+[m[32m#[m
[32m+[m[32mrequire 'grit/git-ruby/internal/raw_object'[m
[32m+[m[32mrequire 'grit/git-ruby/internal/pack'[m
[32m+[m[32mrequire 'grit/git-ruby/internal/loose'[m
[32m+[m[32mrequire 'grit/git-ruby/git_object'[m
[32m+[m
[32m+[m[32mrequire 'rubygems'[m
[32m+[m[32mrequire 'diff/lcs'[m
[32m+[m[32mrequire 'diff/lcs/hunk'[m
[32m+[m
[32m+[m[32m# have to do this so it doesn't interfere with Grit::Diff[m
[32m+[m[32mmodule Difference[m
[32m+[m[32m  include Diff[m
[32m+[m[32mend[m
[32m+[m
[32m+[m[32mmodule Grit[m
[32m+[m[32m  module GitRuby[m
[32m+[m[32m    class Repository[m
[32m+[m
[32m+[m[32m      class NoSuchShaFound < StandardError[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      class NoSuchPath < StandardError[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      attr_accessor :git_dir, :options[m
[32m+[m
[32m+[m[32m      def initialize(git_dir, options = {})[m
[32m+[m[32m        @git_dir = git_dir[m
[32m+[m[32m        @options = options[m
[32m+[m[32m        @packs = [][m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      # returns the loose objects object lazily[m
[32m+[m[32m      def loose[m
[32m+[m[32m        @loose ||= initloose[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      # returns the array of pack list objects[m
[32m+[m[32m      def packs[m
[32m+[m[32m        @packs ||= initpacks[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m
[32m+[m[32m      # prints out the type, shas and content of all of the pack files[m
[32m+[m[32m      def show[m
[32m+[m[32m        packs.each do |p|[m
[32m+[m[32m          puts p.name[m
[32m+[m[32m          puts[m
[32m+[m[32m          p.each_sha1 do |s|[m
[32m+[m[32m            puts "**#{p[s].type}**"[m
[32m+[m[32m            if p[s].type.to_s == 'commit'[m
[32m+[m[32m              puts s.unpack('H*')[m
[32m+[m[32m              puts p[s].content[m
[32m+[m[32m            end[m
[32m+[m[32m          end[m
[32m+[m[32m          puts[m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m
[32m+[m[32m      # returns a raw object given a SHA1[m
[32m+[m[32m      def get_raw_object_by_sha1(sha1o)[m
[32m+[m[32m        raise NoSuchShaFound if sha1o.nil? || sha1o.empty? || !sha1o.is_a?(String)[m
[32m+[m
[32m+[m[32m        sha1 = [sha1o.chomp].pack("H*")[m
[32m+[m[32m        # try packs[m
[32m+[m[32m        packs.each do |pack|[m
[32m+[m[32m          o = pack[sha1][m
[32m+[m[32m          return pack[sha1] if o[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        # try loose storage[m
[32m+[m[32m        loose.each do |lsobj|[m
[32m+[m[32m          o = lsobj[sha1][m
[32m+[m[32m          return o if o[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        # try packs again, maybe the object got packed in the meantime[m
[32m+[m[32m        initpacks[m
[32m+[m[32m        packs.each do |pack|[m
[32m+[m[32m          o = pack[sha1][m
[32m+[m[32m          return o if o[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m#        puts "*#{sha1o}*"[m
[32m+[m[32m        raise NoSuchShaFound[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      def cached(key, object, do_cache = true)[m
[32m+[m[32m        object[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      # returns GitRuby object of any type given a SHA1[m
[32m+[m[32m      def get_object_by_sha1(sha1)[m
[32m+[m[32m        r = get_raw_object_by_sha1(sha1)[m
[32m+[m[32m        return nil if !r[m
[32m+[m[32m        GitObject.from_raw(r)[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      # writes a raw object into the git repo[m
[32m+[m[32m      def put_raw_object(content, type)[m
[32m+[m[32m        loose.first.put_raw_object(content, type)[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      # returns true or false if that sha exists in the db[m
[32m+[m[32m      def object_exists?(sha1)[m
[32m+[m[32m        sha_hex = [sha1].pack("H*")[m
[32m+[m[32m        return true if in_packs?(sha_hex)[m
[32m+[m[32m        return true if in_loose?(sha_hex)[m
[32m+[m[32m        initpacks[m
[32m+[m[32m        return true if in_packs?(sha_hex) #maybe the object got packed in the meantime[m
[32m+[m[32m        false[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      # returns true if the hex-packed sha is in the packfiles[m
[32m+[m[32m      def in_packs?(sha_hex)[m
[32m+[m[32m        # try packs[m
[32m+[m[32m        packs.each do |pack|[m
[32m+[m[32m          return true if pack[sha_hex][m
[32m+[m[32m        end[m
[32m+[m[32m        false[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      # returns true if the hex-packed sha is in the loose objects[m
[32m+[m[32m      def in_loose?(sha_hex)[m
[32m+[m[32m        loose.each do |lsobj|[m
[32m+[m[32m          return true if lsobj[sha_hex][m
[32m+[m[32m        end[m
[32m+[m[32m        false[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m
[32m+[m[32m      # returns the file type (as a symbol) of this sha[m
[32m+[m[32m      def cat_file_type(sha)[m
[32m+[m[32m        get_raw_object_by_sha1(sha).type[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      # returns the file size (as an int) of this sha[m
[32m+[m[32m      def cat_file_size(sha)[m
[32m+[m[32m        get_raw_object_by_sha1(sha).content.size[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      # returns the raw file contents of this sha[m
[32m+[m[32m      def cat_file(sha)[m
[32m+[m[32m        get_object_by_sha1(sha).raw_content[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      # returns a 2-d hash of the tree[m
[32m+[m[32m      # ['blob']['FILENAME'] = {:mode => '100644', :sha => SHA}[m
[32m+[m[32m      # ['tree']['DIRNAME'] = {:mode => '040000', :sha => SHA}[m
[32m+[m[32m      def list_tree(sha)[m
[32m+[m[32m        data = {'blob' => {}, 'tree' => {}, 'link' => {}, 'commit' => {}}[m
[32m+[m[32m        get_object_by_sha1(sha).entry.each do |e|[m
[32m+[m[32m          data[e.format_type][e.name] = {:mode => e.format_mode, :sha => e.sha1}[m
[32m+[m[32m        end[m
[32m+[m[32m        data[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      # returns the raw (cat-file) output for a tree[m
[32m+[m[32m      # if given a commit sha, it will print the tree of that commit[m
[32m+[m[32m      # if given a path limiter array, it will limit the output to those[m
[32m+[m[32m      # if asked for recrusive trees, will traverse trees[m
[32m+[m[32m      def ls_tree(sha, paths = [], recursive = false)[m
[32m+[m[32m        if paths.size > 0[m
[32m+[m[32m          # pathing[m
[32m+[m[32m          part = [][m
[32m+[m[32m          paths.each do |path|[m
[32m+[m[32m            part += ls_tree_path(sha, path)[m
[32m+[m[32m          end[m
[32m+[m[32m          return part.join("\n")[m
[32m+[m[32m        else[m
[32m+[m[32m          get_raw_tree(sha, recursive)[m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      def get_raw_tree(sha, recursive = false)[m
[32m+[m[32m        o = get_raw_object_by_sha1(sha)[m
[32m+[m[32m        if o.type == :commit[m
[32m+[m[32m          tree = get_object_by_sha1(sha).tree[m
[32m+[m[32m        elsif o.type == :tag[m
[32m+[m[32m          commit_sha = get_object_by_sha1(sha).object[m
[32m+[m[32m          tree = get_object_by_sha1(commit_sha).tree[m
[32m+[m[32m        elsif o.type == :tree[m
[32m+[m[32m          tree = sha[m
[32m+[m[32m        else[m
[32m+[m[32m          return nil[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        recursive ? get_raw_trees(tree) : cat_file(tree)[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      # Grabs tree contents recursively,[m
[32m+[m[32m      #   e.g. `git ls-tree -r sha`[m
[32m+[m[32m      def get_raw_trees(sha, path = '')[m
[32m+[m[32m        out = ''[m
[32m+[m[32m        cat_file(sha).split("\n").each do |line|[m
[32m+[m[32m          mode, type, sha, name = line.split(/\s/)[m
[32m+[m
[32m+[m[32m          if type == 'tree'[m
[32m+[m[32m            full_name = path.empty? ? name : "#{path}/#{name}"[m
[32m+[m[32m            out << get_raw_trees(sha, full_name)[m
[32m+[m[32m          elsif path.empty?[m
[32m+[m[32m            out << line + "\n"[m
[32m+[m[32m          else[m
[32m+[m[32m            out << line.gsub(name, "#{path}/#{name}") + "\n"[m
[32m+[m[32m          end[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        out[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      # return array of tree entries[m
[32m+[m[32m      ## TODO : refactor this to remove the fugly[m
[32m+[m[32m      def ls_tree_path(sha, path, append = nil)[m
[32m+[m[32m        tree = get_raw_tree(sha)[m
[32m+[m[32m        if path =~ /\//[m
[32m+[m[32m          paths = path.split('/')[m
[32m+[m[32m          last = path[path.size - 1, 1][m
[32m+[m[32m          if (last == '/') && (paths.size == 1)[m
[32m+[m[32m            append = append ? File.join(append, paths.first) : paths.first[m
[32m+[m[32m            dir_name = tree.split("\n").select { |p| p.split("\t")[1] == paths.first }.first[m
[32m+[m[32m            raise NoSuchPath if !dir_name[m
[32m+[m[32m            next_sha = dir_name.split(' ')[2][m
[32m+[m[32m            tree = get_raw_tree(next_sha)[m
[32m+[m[32m            tree = tree.split("\n")[m
[32m+[m[32m            if append[m
[32m+[m[32m              mod_tree = [][m
[32m+[m[32m              tree.each do |ent|[m
[32m+[m[32m                (info, fpath) = ent.split("\t")[m
[32m+[m[32m                mod_tree << [info, File.join(append, fpath)].join("\t")[m
[32m+[m[32m              end[m
[32m+[m[32m              mod_tree[m
[32m+[m[32m            else[m
[32m+[m[32m              tree[m
[32m+[m[32m            end[m
[32m+[m[32m          else[m
[32m+[m[32m            raise NoSuchPath if tree.nil?[m
[32m+[m[32m            next_path = paths.shift[m
[32m+[m[32m            dir_name = tree.split("\n").select { |p| p.split("\t")[1] == next_path }.first[m
[32m+[m[32m            raise NoSuchPath if !dir_name[m
[32m+[m[32m            next_sha = dir_name.split(' ')[2][m
[32m+[m[32m            next_path = append ? File.join(append, next_path) : next_path[m
[32m+[m[32m            if (last == '/')[m
[32m+[m[32m              ls_tree_path(next_sha, paths.join("/") + '/', next_path)[m
[32m+[m[32m            else[m
[32m+[m[32m              ls_tree_path(next_sha, paths.join("/"), next_path)[m
[32m+[m[32m            end[m
[32m+[m[32m          end[m
[32m+[m[32m        else[m
[32m+[m[32m          raise NoSuchPath if tree.nil?[m
[32m+[m[32m          tree = tree.split("\n")[m
[32m+[m[32m          tree = tree.select { |p| p.split("\t")[1] == path }[m
[32m+[m[32m          if append[m
[32m+[m[32m            mod_tree = [][m
[32m+[m[32m            tree.each do |ent|[m
[32m+[m[32m              (info, fpath) = ent.split("\t")[m
[32m+[m[32m              mod_tree << [info, File.join(append, fpath)].join("\t")[m
[32m+[m[32m            end[m
[32m+[m[32m            mod_tree[m
[32m+[m[32m          else[m
[32m+[m[32m            tree[m
[32m+[m[32m          end[m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      # returns an array of GitRuby Commit objects[m
[32m+[m[32m      # [ [sha, raw_output], [sha, raw_output], [sha, raw_output] ... ][m
[32m+[m[32m      #[m
[32m+[m[32m      # takes the following options:[m
[32m+[m[32m      #  :since - Time object specifying that you don't want commits BEFORE this[m
[32m+[m[32m      #  :until - Time object specifying that you don't want commit AFTER this[m
[32m+[m[32m      #  :first_parent - tells log to only walk first parent[m
[32m+[m[32m      #  :path_limiter - string or array of strings to limit path[m
[32m+[m[32m      #  :max_count - number to limit the output[m
[32m+[m[32m      def log(sha, options = {})[m
[32m+[m[32m        @already_searched = {}[m
[32m+[m[32m        walk_log(sha, options)[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      def truncate_arr(arr, sha)[m
[32m+[m[32m        new_arr = [][m
[32m+[m[32m        arr.each do |a|[m
[32m+[m[32m          if a[0] == sha[m
[32m+[m[32m            return new_arr[m
[32m+[m[32m          end[m
[32m+[m[32m          new_arr << a[m
[32m+[m[32m        end[m
[32m+[m[32m        return new_arr[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      def rev_list(sha, options)[m
[32m+[m[32m        if sha.is_a? Array[m
[32m+[m[32m          (end_sha, sha) = sha[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        log = log(sha, options)[m
[32m+[m[32m        log = log.sort { |a, b| a[2] <=> b[2] }.reverse[m
[32m+[m
[32m+[m[32m        if end_sha[m
[32m+[m[32m          log = truncate_arr(log, end_sha)[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        # shorten the list if it's longer than max_count (had to get everything in branches)[m
[32m+[m[32m        if options[:max_count][m
[32m+[m[32m          if (opt_len = options[:max_count].to_i) < log.size[m
[32m+[m[32m            log = log[0, opt_len][m
[32m+[m[32m          end[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        if options[:pretty] == 'raw'[m
[32m+[m[32m          log.map {|k, v| v }.join('')[m
[32m+[m[32m        else[m
[32m+[m[32m          log.map {|k, v| k }.join("\n")[m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      # called by log() to recursively walk the tree[m
[32m+[m[32m      def walk_log(sha, opts, total_size = 0)[m
[32m+[m[32m        return [] if @already_searched[sha] # to prevent rechecking branches[m
[32m+[m[32m        @already_searched[sha] = true[m
[32m+[m
[32m+[m[32m        array = [][m
[32m+[m[32m        if (sha)[m
[32m+[m[32m          o = get_raw_object_by_sha1(sha)[m
[32m+[m[32m          if o.type == :tag[m
[32m+[m[32m            commit_sha = get_object_by_sha1(sha).object[m
[32m+[m[32m            c = get_object_by_sha1(commit_sha)[m
[32m+[m[32m          else[m
[32m+[m[32m            c = GitObject.from_raw(o)[m
[32m+[m[32m          end[m
[32m+[m
[32m+[m[32m          return [] if c.type != :commit[m
[32m+[m
[32m+[m[32m          add_sha = true[m
[32m+[m
[32m+[m[32m          if opts[:since] && opts[:since].is_a?(Time) && (opts[:since] > c.committer.date)[m
[32m+[m[32m            add_sha = false[m
[32m+[m[32m          end[m
[32m+[m[32m          if opts[:until] && opts[:until].is_a?(Time) && (opts[:until] < c.committer.date)[m
[32m+[m[32m            add_sha = false[m
[32m+[m[32m          end[m
[32m+[m
[32m+[m[32m          # follow all parents unless '--first-parent' is specified #[m
[32m+[m[32m          subarray = [][m
[32m+[m
[32m+[m[32m          if !c.parent.first && opts[:path_limiter]  # check for the last commit[m
[32m+[m[32m            add_sha = false[m
[32m+[m[32m          end[m
[32m+[m
[32m+[m[32m          if (!opts[:max_count] || ((array.size + total_size) < opts[:max_count]))[m
[32m+[m
[32m+[m[32m            if !opts[:path_limiter][m
[32m+[m[32m              output = c.raw_log(sha)[m
[32m+[m[32m              array << [sha, output, c.committer.date][m
[32m+[m[32m            end[m
[32m+[m
[32m+[m[32m            if (opts[:max_count] && (array.size + total_size) >= opts[:max_count])[m
[32m+[m[32m              return array[m
[32m+[m[32m            end[m
[32m+[m
[32m+[m[32m            c.parent.each do |psha|[m
[32m+[m[32m              if psha && !files_changed?(c.tree, get_object_by_sha1(psha).tree,[m
[32m+[m[32m                                        opts[:path_limiter])[m
[32m+[m[32m                add_sha = false[m
[32m+[m[32m              end[m
[32m+[m[32m              subarray += walk_log(psha, opts, (array.size + total_size))[m
[32m+[m[32m              next if opts[:first_parent][m
[32m+[m[32m            end[m
[32m+[m
[32m+[m[32m            if opts[:path_limiter] && add_sha[m
[32m+[m[32m              output = c.raw_log(sha)[m
[32m+[m[32m              array << [sha, output, c.committer.date][m
[32m+[m[32m            end[m
[32m+[m
[32m+[m[32m            if add_sha[m
[32m+[m[32m              array += subarray[m
[32m+[m[32m            end[m
[32m+[m[32m          end[m
[32m+[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        array[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      def diff(commit1, commit2, options = {})[m
[32m+[m[32m        patch = ''[m
[32m+[m
[32m+[m[32m        commit_obj1 = get_object_by_sha1(commit1)[m
[32m+[m[32m        tree1 = commit_obj1.tree[m
[32m+[m[32m        if commit2[m
[32m+[m[32m          tree2 = get_object_by_sha1(commit2).tree[m
[32m+[m[32m        else[m
[32m+[m[32m          tree2 = get_object_by_sha1(commit_obj1.parent.first).tree[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        qdiff = quick_diff(tree1, tree2)[m
[32m+[m
[32m+[m[32m        qdiff.sort.each do |diff_arr|[m
[32m+[m[32m          path, status, treeSHA1, treeSHA2 = *diff_arr[m
[32m+[m[32m          format, lines, output = :unified, 3, ''[m
[32m+[m[32m          file_length_difference = 0[m
[32m+[m
[32m+[m[32m          fileA = treeSHA1 ? cat_file(treeSHA1) : ''[m
[32m+[m[32m          fileB = treeSHA2 ? cat_file(treeSHA2) : ''[m
[32m+[m
[32m+[m[32m          sha1 = treeSHA1 || '0000000000000000000000000000000000000000'[m
[32m+[m[32m          sha2 = treeSHA2 || '0000000000000000000000000000000000000000'[m
[32m+[m
[32m+[m[32m          data_old = fileA.split(/\n/).map! { |e| e.chomp }[m
[32m+[m[32m          data_new = fileB.split(/\n/).map! { |e| e.chomp }[m
[32m+[m
[32m+[m[32m          diffs = Difference::LCS.diff(data_old, data_new)[m
[32m+[m[32m          next if diffs.empty?[m
[32m+[m
[32m+[m[32m          a_path = "a/#{path.gsub('./', '')}"[m
[32m+[m[32m          b_path = "b/#{path.gsub('./', '')}"[m
[32m+[m
[32m+[m[32m          header = "diff --git #{a_path} #{b_path}"[m
[32m+[m[32m          if options[:full_index][m
[32m+[m[32m            header << "\n" + 'index ' + sha1 + '..' + sha2[m
[32m+[m[32m            header << ' 100644' if treeSHA2 # hard coding this because i don't think we use it[m
[32m+[m[32m          else[m
[32m+[m[32m            header << "\n" + 'index ' + sha1[0,7] + '..' + sha2[0,7][m
[32m+[m[32m            header << ' 100644' if treeSHA2 # hard coding this because i don't think we use it[m
[32m+[m[32m          end[m
[32m+[m[32m          header << "\n--- " + (treeSHA1 ? a_path : '/dev/null')[m
[32m+[m[32m          header << "\n+++ " + (treeSHA2 ? b_path : '/dev/null')[m
[32m+[m[32m          header += "\n"[m
[32m+[m
[32m+[m[32m          oldhunk = hunk = nil[m
[32m+[m
[32m+[m[32m          diffs.each do |piece|[m
[32m+[m[32m            begin[m
[32m+[m[32m              hunk = Difference::LCS::Hunk.new(data_old, data_new, piece, lines, file_length_difference)[m
[32m+[m[32m              file_length_difference = hunk.file_length_difference[m
[32m+[m
[32m+[m[32m              next unless oldhunk[m
[32m+[m
[32m+[m[32m              if lines > 0 && hunk.overlaps?(oldhunk)[m
[32m+[m[32m                hunk.unshift(oldhunk)[m
[32m+[m[32m              else[m
[32m+[m[32m                output << oldhunk.diff(format)[m
[32m+[m[32m              end[m
[32m+[m[32m            ensure[m
[32m+[m[32m              oldhunk = hunk[m
[32m+[m[32m              output << "\n"[m
[32m+[m[32m            end[m
[32m+[m[32m          end[m
[32m+[m
[32m+[m[32m          output << oldhunk.diff(format)[m
[32m+[m[32m          output << "\n"[m
[32m+[m
[32m+[m[32m          patch << header + output.lstrip[m
[32m+[m[32m        end[m
[32m+[m[32m        patch[m
[32m+[m[32m      rescue[m
[32m+[m[32m        '' # one of the trees was bad or lcs isn't there - no diff[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      # takes 2 tree shas and recursively walks them to find out what[m
[32m+[m[32m      # files or directories have been modified in them and returns an[m
[32m+[m[32m      # array of changes[m
[32m+[m[32m      # [ [full_path, 'added', tree1_hash, nil],[m
[32m+[m[32m      #   [full_path, 'removed', nil, tree2_hash],[m
[32m+[m[32m      #   [full_path, 'modified', tree1_hash, tree2_hash][m
[32m+[m[32m      #  ][m
[32m+[m[32m      def quick_diff(tree1, tree2, path = '.', recurse = true)[m
[32m+[m[32m         # handle empty trees[m
[32m+[m[32m         changed = [][m
[32m+[m[32m         return changed if tree1 == tree2[m
[32m+[m
[32m+[m[32m         t1 = list_tree(tree1) if tree1[m
[32m+[m[32m         t2 = list_tree(tree2) if tree2[m
[32m+[m
[32m+[m[32m         # finding files that are different[m
[32m+[m[32m         t1['blob'].each do |file, hsh|[m
[32m+[m[32m           t2_file = t2['blob'][file] rescue nil[m
[32m+[m[32m           full = File.join(path, file)[m
[32m+[m[32m           if !t2_file[m
[32m+[m[32m             changed << [full, 'added', hsh[:sha], nil]      # not in parent[m
[32m+[m[32m           elsif (hsh[:sha] != t2_file[:sha])[m
[32m+[m[32m             changed << [full, 'modified', hsh[:sha], t2_file[:sha]]   # file changed[m
[32m+[m[32m           end[m
[32m+[m[32m         end if t1[m
[32m+[m[32m         t2['blob'].each do |file, hsh|[m
[32m+[m[32m           if !t1 || !t1['blob'][file][m
[32m+[m[32m             changed << [File.join(path, file), 'removed', nil, hsh[:sha]][m
[32m+[m[32m           end[m
[32m+[m[32m         end if t2[m
[32m+[m
[32m+[m[32m         t1['tree'].each do |dir, hsh|[m
[32m+[m[32m           t2_tree = t2['tree'][dir] rescue nil[m
[32m+[m[32m           full = File.join(path, dir)[m
[32m+[m[32m           if !t2_tree[m
[32m+[m[32m             if recurse[m
[32m+[m[32m               changed += quick_diff(hsh[:sha], nil, full, true)[m
[32m+[m[32m             else[m
[32m+[m[32m               changed << [full, 'added', hsh[:sha], nil]      # not in parent[m
[32m+[m[32m             end[m
[32m+[m[32m           elsif (hsh[:sha] != t2_tree[:sha])[m
[32m+[m[32m             if recurse[m
[32m+[m[32m               changed += quick_diff(hsh[:sha], t2_tree[:sha], full, true)[m
[32m+[m[32m             else[m
[32m+[m[32m               changed << [full, 'modified', hsh[:sha], t2_tree[:sha]]   # file changed[m
[32m+[m[32m             end[m
[32m+[m[32m           end[m
[32m+[m[32m         end if t1[m
[32m+[m[32m         t2['tree'].each do |dir, hsh|[m
[32m+[m[32m           t1_tree = t1['tree'][dir] rescue nil[m
[32m+[m[32m           full = File.join(path, dir)[m
[32m+[m[32m           if !t1_tree[m
[32m+[m[32m             if recurse[m
[32m+[m[32m               changed += quick_diff(nil, hsh[:sha], full, true)[m
[32m+[m[32m             else[m
[32m+[m[32m               changed << [full, 'removed', nil, hsh[:sha]][m
[32m+[m[32m             end[m
[32m+[m[32m           end[m
[32m+[m[32m         end if t2[m
[32m+[m
[32m+[m[32m         changed[m
[32m+[m[32m       end[m
[32m+[m
[32m+[m[32m      # returns true if the files in path_limiter were changed, or no path limiter[m
[32m+[m[32m      # used by the log() function when passed with a path_limiter[m
[32m+[m[32m      def files_changed?(tree_sha1, tree_sha2, path_limiter = nil)[m
[32m+[m[32m        if path_limiter[m
[32m+[m[32m          mod = quick_diff(tree_sha1, tree_sha2)[m
[32m+[m[32m          files = mod.map { |c| c.first }[m
[32m+[m[32m          path_limiter.to_a.each do |filepath|[m
[32m+[m[32m            if files.include?(filepath)[m
[32m+[m[32m              return true[m
[32m+[m[32m            end[m
[32m+[m[32m          end[m
[32m+[m[32m          return false[m
[32m+[m[32m        end[m
[32m+[m[32m        true[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      def get_subtree(commit_sha, path)[m
[32m+[m[32m        tree_sha = get_object_by_sha1(commit_sha).tree[m
[32m+[m
[32m+[m[32m        if path && !(path == '' || path == '.' || path == './')[m
[32m+[m[32m          paths = path.split('/')[m
[32m+[m[32m          paths.each do |path|[m
[32m+[m[32m            tree = get_object_by_sha1(tree_sha)[m
[32m+[m[32m            if entry = tree.entry.select { |e| e.name == path }.first[m
[32m+[m[32m              tree_sha = entry.sha1 rescue nil[m
[32m+[m[32m            else[m
[32m+[m[32m              return false[m
[32m+[m[32m            end[m
[32m+[m[32m          end[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        tree_sha[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      def blame_tree(commit_sha, path)[m
[32m+[m[32m        # find subtree[m
[32m+[m[32m        tree_sha = get_subtree(commit_sha, path)[m
[32m+[m[32m        return {} if !tree_sha[m
[32m+[m
[32m+[m[32m        looking_for = [][m
[32m+[m[32m        get_object_by_sha1(tree_sha).entry.each do |e|[m
[32m+[m[32m          looking_for << File.join('.', e.name)[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        @already_searched = {}[m
[32m+[m[32m        commits = look_for_commits(commit_sha, path, looking_for)[m
[32m+[m
[32m+[m[32m        # cleaning up array[m
[32m+[m[32m        arr = {}[m
[32m+[m[32m        commits.each do |commit_array|[m
[32m+[m[32m          key = commit_array[0].gsub('./', '')[m
[32m+[m[32m          arr[key] = commit_array[1][m
[32m+[m[32m        end[m
[32m+[m[32m        arr[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      def look_for_commits(commit_sha, path, looking_for, options = {})[m
[32m+[m[32m        return [] if @already_searched[commit_sha] # to prevent rechecking branches[m
[32m+[m
[32m+[m[32m        @already_searched[commit_sha] = true[m
[32m+[m
[32m+[m[32m        commit = get_object_by_sha1(commit_sha)[m
[32m+[m[32m        tree_sha = get_subtree(commit_sha, path)[m
[32m+[m
[32m+[m[32m        found_data = [][m
[32m+[m
[32m+[m[32m        # at the beginning of the branch[m
[32m+[m[32m        if commit.parent.size == 0[m
[32m+[m[32m          looking_for.each do |search|[m
[32m+[m[32m            # prevents the rare case of multiple branch starting points with[m
[32m+[m[32m            # files that have never changed[m
[32m+[m[32m            if found_data.assoc(search)[m
[32m+[m[32m              found_data << [search, commit_sha][m
[32m+[m[32m            end[m
[32m+[m[32m          end[m
[32m+[m[32m          return found_data[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        # go through the parents recursively, looking for somewhere this has been changed[m
[32m+[m[32m        commit.parent.each do |pc|[m
[32m+[m[32m          diff = quick_diff(tree_sha, get_subtree(pc, path), '.', false)[m
[32m+[m
[32m+[m[32m          # remove anything found[m
[32m+[m[32m          looking_for.each do |search|[m
[32m+[m[32m            if match = diff.assoc(search)[m
[32m+[m[32m              found_data << [search, commit_sha, match][m
[32m+[m[32m              looking_for.delete(search)[m
[32m+[m[32m            end[m
[32m+[m[32m          end[m
[32m+[m
[32m+[m[32m          if looking_for.size <= 0  # we're done[m
[32m+[m[32m            return found_data[m
[32m+[m[32m          end[m
[32m+[m
[32m+[m[32m          found_data += look_for_commits(pc, path, looking_for)  # recurse into parent[m
[32m+[m[32m          return found_data if options[:first_parent][m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        ## TODO : find most recent commit with change in any parent[m
[32m+[m[32m        found_data[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      # initialize a git repository[m
[32m+[m[32m      def self.init(dir, bare = true)[m
[32m+[m
[32m+[m[32m        FileUtils.mkdir_p(dir) if !File.exists?(dir)[m
[32m+[m
[32m+[m[32m        FileUtils.cd(dir) do[m
[32m+[m[32m          if(File.exists?('objects'))[m
[32m+[m[32m            return false # already initialized[m
[32m+[m[32m          else[m
[32m+[m[32m            # initialize directory[m
[32m+[m[32m            create_initial_config(bare)[m
[32m+[m[32m            FileUtils.mkdir_p('refs/heads')[m
[32m+[m[32m            FileUtils.mkdir_p('refs/tags')[m
[32m+[m[32m            FileUtils.mkdir_p('objects/info')[m
[32m+[m[32m            FileUtils.mkdir_p('objects/pack')[m
[32m+[m[32m            FileUtils.mkdir_p('branches')[m
[32m+[m[32m            add_file('description', 'Unnamed repository; edit this file to name it for gitweb.')[m
[32m+[m[32m            add_file('HEAD', "ref: refs/heads/master\n")[m
[32m+[m[32m            FileUtils.mkdir_p('hooks')[m
[32m+[m[32m            FileUtils.cd('hooks') do[m
[32m+[m[32m              add_file('applypatch-msg', '# add shell script and make executable to enable')[m
[32m+[m[32m              add_file('post-commit', '# add shell script and make executable to enable')[m
[32m+[m[32m              add_file('post-receive', '# add shell script and make executable to enable')[m
[32m+[m[32m              add_file('post-update', '# add shell script and make executable to enable')[m
[32m+[m[32m              add_file('pre-applypatch', '# add shell script and make executable to enable')[m
[32m+[m[32m              add_file('pre-commit', '# add shell script and make executable to enable')[m
[32m+[m[32m              add_file('pre-rebase', '# add shell script and make executable to enable')[m
[32m+[m[32m              add_file('update', '# add shell script and make executable to enable')[m
[32m+[m[32m            end[m
[32m+[m[32m            FileUtils.mkdir_p('info')[m
[32m+[m[32m            add_file('info/exclude', "# *.[oa]\n# *~")[m
[32m+[m[32m          end[m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      def self.create_initial_config(bare = false)[m
[32m+[m[32m        bare ? bare_status = 'true' : bare_status = 'false'[m
[32m+[m[32m        config = "[core]\n\trepositoryformatversion = 0\n\tfilemode = true\n\tbare = #{bare_status}\n\tlogallrefupdates = true"[m
[32m+[m[32m        add_file('config', config)[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      def self.add_file(name, contents)[m
[32m+[m[32m        File.open(name, 'w') do |f|[m
[32m+[m[32m          f.write contents[m
[32m+[m[32m        end[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      def close[m
[32m+[m[32m        @packs.each do |pack|[m
[32m+[m[32m          pack.close[m
[32m+[m[32m        end if @packs[m
[32m+[m[32m      end[m
[32m+[m
[32m+[m[32m      protected[m
[32m+[m
[32m+[m[32m        def git_path(path)[m
[32m+[m[32m          return "#@git_dir/#{path}"[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m      private[m
[32m+[m
[32m+[m[32m        def initloose[m
[32m+[m[32m          @loaded = [][m
[32m+[m[32m          @loose = [][m
[32m+[m[32m          load_loose(git_path('objects'))[m
[32m+[m[32m          load_alternate_loose(git_path('objects'))[m
[32m+[m[32m          @loose[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        def load_alternate_loose(path)[m
[32m+[m[32m          # load alternate loose, too[m
[32m+[m[32m          alt = File.join(path, 'info/alternates')[m
[32m+[m[32m          if File.exists?(alt)[m
[32m+[m[32m            File.readlines(alt).each do |line|[m
[32m+[m[32m              next if @loaded.include?(line.chomp)[m
[32m+[m[32m              if line[0, 2] == '..'[m
[32m+[m[32m                line = File.expand_path(File.join(@git_dir, line))[m
[32m+[m[32m              end[m
[32m+[m[32m              load_loose(line.chomp)[m
[32m+[m[32m              load_alternate_loose(line.chomp)[m
[32m+[m[32m            end[m
[32m+[m[32m          end[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        def load_loose(path)[m
[32m+[m[32m          @loaded << path[m
[32m+[m[32m          return if !File.exists?(path)[m
[32m+[m[32m          @loose << Grit::GitRuby::Internal::LooseStorage.new(path)[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        def initpacks[m
[32m+[m[32m          close[m
[32m+[m[32m          @loaded_packs = [][m
[32m+[m[32m          @packs = [][m
[32m+[m[32m          load_packs(git_path("objects/pack"))[m
[32m+[m[32m          load_alternate_packs(git_path('objects'))[m
[32m+[m[32m          @packs[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        def load_alternate_packs(path)[m
[32m+[m[32m          alt = File.join(path, 'info/alternates')[m
[32m+[m[32m          if File.exists?(alt)[m
[32m+[m[32m            File.readlines(alt).each do |line|[m
[32m+[m[32m              if line[0, 2] == '..'[m
[32m+[m[32m                line = File.expand_path(File.join(@git_dir, line))[m
[32m+[m[32m              end[m
[32m+[m[32m              full_pack = File.join(line.chomp, 'pack')[m
[32m+[m[32m              next if @loaded_packs.include?(full_pack)[m
[32m+[m[32m              load_packs(full_pack)[m
[32m+[m[32m              load_alternate_packs(File.join(line.chomp))[m
[32m+[m[32m            end[m
[32m+[m[32m          end[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        def load_packs(path)[m
[32m+[m[32m          @loaded_packs << path[m
[32m+[m[32m          return if !File.exists?(path)[m
[32m+[m[32m           Dir.open(path) do |dir|[m
[32m+[m[32m            dir.each do |entry|[m
[32m+[m[32m              next if !(entry =~ /\.pack$/i)[m
[32m+[m[32m              pack = Grit::GitRuby::Internal::PackStorage.new(File.join(path,entry))[m
[32m+[m[32m              if @options[:map_packfile][m
[32m+[m[32m                pack.cache_objects[m
[32m+[m[32m              end[m
[32m+[m[32m              @packs << pack[m
[32m+[m[32m            end[m
[32m+[m[32m          end[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m  end[m
[32m+[m[32mend[m

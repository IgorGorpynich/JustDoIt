[32m+[m[32mrequire File.dirname(__FILE__) + '/helper'[m
[32m+[m[32mrequire 'pp'[m
[32m+[m
[32m+[m[32mclass TestBlame < Test::Unit::TestCase[m
[32m+[m
[32m+[m[32m  def setup[m
[32m+[m[32m    @r = Repo.new(File.join(File.dirname(__FILE__), *%w[dot_git]), :is_bare => true)[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_simple_blame[m
[32m+[m[32m    commit = '2d3acf90f35989df8f262dc50beadc4ee3ae1560'[m
[32m+[m[32m    blame = @r.blame('History.txt', commit)[m
[32m+[m[32m    assert_equal 5, blame.lines.size[m
[32m+[m[32m    line = blame.lines[2][m
[32m+[m[32m    assert_equal '* 1 major enhancement', line.line[m
[32m+[m[32m    assert_equal 3, line.lineno[m
[32m+[m[32m    assert_equal 3, line.oldlineno[m
[32m+[m[32m    assert_equal '634396b2f541a9f2d58b00be1a07f0c358b999b3', line.commit.id[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_depth_blame[m
[32m+[m[32m    commit = '2d3acf90f35989df8f262dc50beadc4ee3ae1560'[m
[32m+[m[32m    blame = @r.blame('lib/grit.rb', commit)[m
[32m+[m[32m    assert_equal 37, blame.lines.size[m
[32m+[m[32m    line = blame.lines[24][m
[32m+[m[32m    assert_equal "require 'grit/diff'", line.line[m
[32m+[m[32m    assert_equal 25, line.lineno[m
[32m+[m[32m    assert_equal 16, line.oldlineno[m
[32m+[m[32m    assert_equal '46291865ba0f6e0c9818b11be799fe2db6964d56', line.commit.id[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32mend[m

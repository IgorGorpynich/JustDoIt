[32m+[m[32m  def test_alternates_with_two_alternates[m
[32m+[m[32m    File.expects(:exist?).with("#{absolute_project_path}/.git/objects/info/alternates").returns(true)[m
[32m+[m[32m    File.expects(:read).with("#{absolute_project_path}/.git/objects/info/alternates").returns("/path/to/repo1/.git/objects\n/path/to/repo2.git/objects\n")[m
[32m+[m
[32m+[m[32m    assert_equal ["/path/to/repo1/.git/objects", "/path/to/repo2.git/objects"], @r.alternates[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_alternates_no_file[m
[32m+[m[32m    File.expects(:exist?).returns(false)[m
[32m+[m
[32m+[m[32m    assert_equal [], @r.alternates[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  # alternates=[m
[32m+[m
[32m+[m[32m  def test_alternates_setter_ok[m
[32m+[m[32m    alts = %w{/path/to/repo.git/objects /path/to/repo2.git/objects}[m
[32m+[m
[32m+[m[32m    alts.each do |alt|[m
[32m+[m[32m      File.expects(:exist?).with(alt).returns(true)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    File.any_instance.expects(:write).with(alts.join("\n"))[m
[32m+[m
[32m+[m[32m    assert_nothing_raised do[m
[32m+[m[32m      @r.alternates = alts[m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_alternates_setter_bad[m
[32m+[m[32m    alts = %w{/path/to/repo.git/objects}[m
[32m+[m
[32m+[m[32m    alts.each do |alt|[m
[32m+[m[32m      File.expects(:exist?).with(alt).returns(false)[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    File.any_instance.expects(:write).never[m
[32m+[m
[32m+[m[32m    assert_raise RuntimeError do[m
[32m+[m[32m      @r.alternates = alts[m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_alternates_setter_empty[m
[32m+[m[32m    File.any_instance.expects(:write)[m
[32m+[m[32m    @r.alternates = [][m
[32m+[m[32m  end[m

[32m+[m[32m    assert_equal "Tom Werner", a.name[m
[32m+[m[32m    assert_equal "tom@example.com", a.email[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_from_string_should_handle_just_name[m
[32m+[m[32m    a = Actor.from_string("Tom Werner")[m
[32m+[m[32m    assert_equal "Tom Werner", a.name[m
[32m+[m[32m    assert_equal nil, a.email[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  # inspect[m

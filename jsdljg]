[32m+[m
[32m+[m[32m    Git.any_instance.expects(:diff).with({}, 'master^', 'master', '--', 'foo/bar')[m
[32m+[m[32m    @r.diff('master^', 'master', 'foo/bar')[m
[32m+[m
[32m+[m[32m    Git.any_instance.expects(:diff).with({}, 'master^', 'master', '--', 'foo/bar', 'foo/baz')[m
[32m+[m[32m    @r.diff('master^', 'master', 'foo/bar', 'foo/baz')[m
[32m+[m[32m  end[m

[32m+[m[32m+class Merb::BootLoader::MimeTypes < Merb::BootLoader[m
[32m+[m[32m+  def self.run[m
[32m+[m[32m+    # Sets the default mime-types[m
[32m+[m[32m+    #[m[41m [m
[32m+[m[32m+    # By default, the mime-types include:[m
[32m+[m[32m+    # :all:: no transform, */*[m
[32m+[m[32m+    # :yaml:: to_yaml, application/x-yaml or text/yaml[m
[32m+[m[32m+    # :text:: to_text, text/plain[m
[32m+[m[32m+    # :html:: to_html, text/html or application/xhtml+xml or application/html[m
[32m+[m[32m+    # :xml:: to_xml, application/xml or text/xml or application/x-xml, adds "Encoding: UTF-8" response header[m
[32m+[m[32m+    # :js:: to_json, text/javascript ot application/javascript or application/x-javascript[m
[32m+[m[32m+    # :json:: to_json, application/json or text/x-json[m
[32m+[m[32m+    Merb.available_mime_types.clear[m
[32m+[m[32m+    Merb.add_mime_type(:all,  nil,      %w[*/*])[m
[32m+[m[32m+    Merb.add_mime_type(:yaml, :to_yaml, %w[application/x-yaml text/yaml])[m
[32m+[m[32m+    Merb.add_mime_type(:text, :to_text, %w[text/plain])[m
[32m+[m[32m+    Merb.add_mime_type(:html, :to_html, %w[text/html application/xhtml+xml application/html])[m
[32m+[m[32m+    Merb.add_mime_type(:xml,  :to_xml,  %w[application/xml text/xml application/x-xml], :Encoding => "UTF-8")[m
[32m+[m[32m+    Merb.add_mime_type(:js,   :to_json, %w[text/javascript application/javascript application/x-javascript])[m
[32m+[m[32m+    Merb.add_mime_type(:json, :to_json, %w[application/json text/x-json])[m[41m      [m
[32m+[m[32m+  end[m
[32m+[m[32m end[m

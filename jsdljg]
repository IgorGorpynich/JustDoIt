[32m+[m[32m    assert t.is_a?(Blob)[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_data_should_return_blob_contents[m
[32m+[m[32m    Git.any_instance.expects(:cat_file).returns(fixture('cat_file_blob'))[m
[32m+[m[32m    blob = Blob.create(@r, :id => 'abc')[m
[32m+[m[32m    assert_equal "Hello world", blob.data[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_data_should_cache[m
[32m+[m[32m    Git.any_instance.expects(:cat_file).returns(fixture('cat_file_blob')).times(1)[m
[32m+[m[32m    blob = Blob.create(@r, :id => 'abc')[m
[32m+[m[32m    blob.data[m
[32m+[m[32m    blob.data[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  # size[m
[32m+[m
[32m+[m[32m  def test_size_should_return_file_size[m
[32m+[m[32m    Git.any_instance.expects(:cat_file).returns(fixture('cat_file_blob_size'))[m
[32m+[m[32m    blob = Blob.create(@r, :id => 'abc')[m
[32m+[m[32m    assert_equal 11, blob.size[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  # data[m
[32m+[m

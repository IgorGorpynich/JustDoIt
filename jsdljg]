[32m+[m[32m    blob = Blob.create(@r, :id => 'abc')[m
[32m+[m[32m    assert_equal "text/plain", blob.mime_type[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  # blame[m
[32m+[m
[32m+[m[32m  def test_blame[m
[32m+[m[32m    Git.any_instance.expects(:blame).returns(fixture('blame'))[m
[32m+[m[32m    b = Blob.blame(@r, 'master', 'lib/grit.rb')[m
[32m+[m[32m    assert_equal 13, b.size[m
[32m+[m[32m    assert_equal 25, b.inject(0) { |acc, x| acc + x.last.size }[m
[32m+[m[32m    assert_equal b[0].first.object_id, b[9].first.object_id[m
[32m+[m[32m    c = b.first.first[m
[32m+[m[32m    c.expects(:__bake__).times(0)[m
[32m+[m[32m    assert_equal '634396b2f541a9f2d58b00be1a07f0c358b999b3', c.id[m
[32m+[m[32m    assert_equal 'Tom Preston-Werner', c.author.name[m
[32m+[m[32m    assert_equal 'tom@mojombo.com', c.author.email[m
[32m+[m[32m    assert_equal Time.at(1191997100), c.authored_date[m
[32m+[m[32m    assert_equal 'Tom Preston-Werner', c.committer.name[m
[32m+[m[32m    assert_equal 'tom@mojombo.com', c.committer.email[m
[32m+[m[32m    assert_equal Time.at(1191997100), c.committed_date[m
[32m+[m[32m    assert_equal 'initial grit setup', c.message[m
[32m+[m[32m    # c.expects(:__bake__).times(1)[m
[32m+[m[32m    # assert_equal Tree, c.tree.class[m
[32m+[m[32m  end[m
[32m+[m

[32m+[m[32m  if ((defined? RUBY_VERSION) && (RUBY_VERSION[0..2] == "1.9"))[m
[32m+[m[32m    def getord(offset); self[offset].ord; end[m
[32m+[m[32m  else[m
[32m+[m[32m    alias :getord :[][m
[32m+[m[32m  end[m
[32m+[m[32mend[m

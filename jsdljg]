[32m+[m[32mrequire File.dirname(__FILE__) + '/helper'[m
[32m+[m
[32m+[m[32mclass TestSubmodule < Test::Unit::TestCase[m
[32m+[m[32m  def setup[m
[32m+[m[32m    @r = Repo.new(GRIT_REPO)[m
[32m+[m[32m    @s = Submodule.allocate[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  # config[m
[32m+[m
[32m+[m[32m  def test_config[m
[32m+[m[32m    data = fixture('gitmodules')[m
[32m+[m[32m    blob = stub(:data => data, :id => 'abc')[m
[32m+[m[32m    tree = stub(:'/' => blob)[m
[32m+[m[32m    commit = stub(:tree => tree)[m
[32m+[m[32m    repo = stub(:commit => commit)[m
[32m+[m
[32m+[m[32m    config = Submodule.config(repo)[m
[32m+[m
[32m+[m[32m    assert_equal "git://github.com/mojombo/glowstick", config['test/glowstick']['url'][m
[32m+[m[32m    assert_equal "git://github.com/mojombo/god", config['god']['url'][m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_config_with_windows_lineendings[m
[32m+[m[32m    data = fixture('gitmodules').gsub(/\n/, "\r\n")[m
[32m+[m[32m    blob = stub(:data => data, :id => 'abc')[m
[32m+[m[32m    tree = stub(:'/' => blob)[m
[32m+[m[32m    commit = stub(:tree => tree)[m
[32m+[m[32m    repo = stub(:commit => commit)[m
[32m+[m
[32m+[m[32m    config = Submodule.config(repo)[m
[32m+[m
[32m+[m[32m    assert_equal "git://github.com/mojombo/glowstick", config['test/glowstick']['url'][m
[32m+[m[32m    assert_equal "git://github.com/mojombo/god", config['god']['url'][m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_no_config[m
[32m+[m[32m    tree = stub(:'/' => nil)[m
[32m+[m[32m    commit = stub(:tree => tree)[m
[32m+[m[32m    repo = stub(:commit => commit)[m
[32m+[m
[32m+[m[32m    config = Submodule.config(repo)[m
[32m+[m
[32m+[m[32m    assert_equal({}, config)[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_empty_config[m
[32m+[m[32m    blob = stub(:data => '', :id => 'abc')[m
[32m+[m[32m    tree = stub(:'/' => blob)[m
[32m+[m[32m    commit = stub(:tree => tree)[m
[32m+[m[32m    repo = stub(:commit => commit)[m
[32m+[m
[32m+[m[32m    config = Submodule.config(repo)[m
[32m+[m
[32m+[m[32m    assert_equal({}, config)[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  # inspect[m
[32m+[m
[32m+[m[32m  def test_inspect[m
[32m+[m[32m    @t = Submodule.create(@r, :id => 'abc')[m
[32m+[m[32m    assert_equal %Q{#<Grit::Submodule "abc">}, @t.inspect[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_basename[m

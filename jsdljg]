[32m+[m[32mrequire File.dirname(__FILE__) + '/helper'[m
[32m+[m[32mrequire 'pp'[m
[32m+[m
[32m+[m[32mclass TestRubyGitIndex < Test::Unit::TestCase[m
[32m+[m
[32m+[m[32m  def setup[m
[32m+[m[32m    @base_repo = create_temp_repo(File.join(File.dirname(__FILE__), *%w[dot_git_iv2]))[m
[32m+[m[32m    @git = Grit::Repo.new(@base_repo, :is_bare => true)[m
[32m+[m[32m    @rgit = @git.git.ruby_git[m
[32m+[m[32m    @user = Actor.from_string("Tom Werner <tom@example.com>")[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def teardown[m
[32m+[m[32m    FileUtils.rm_r(@base_repo)[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def create_temp_repo(clone_path)[m
[32m+[m[32m    filename = 'git_test' + Time.now.to_i.to_s + rand(300).to_s.rjust(3, '0')[m
[32m+[m[32m    tmp_path = File.join("/tmp/", filename)[m
[32m+[m[32m    FileUtils.mkdir_p(tmp_path)[m
[32m+[m[32m    FileUtils.cp_r(clone_path, tmp_path)[m
[32m+[m[32m    File.join(tmp_path, 'dot_git_iv2')[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_add_files[m
[32m+[m[32m    sha = @git.commits.first.tree.id[m
[32m+[m
[32m+[m[32m    i = @git.index[m
[32m+[m[32m    i.read_tree(sha)[m
[32m+[m[32m    i.add('atester.rb', 'test stuff')[m
[32m+[m[32m    i.commit('message', [@git.commits.first], @user, nil, 'master')[m
[32m+[m
[32m+[m[32m    b = @git.commits.first.tree/'atester.rb'[m
[32m+[m[32m    assert_equal 'f80c3b68482d5e1c8d24c9b8139340f0d0a928d0', b.id[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_add_path_file[m
[32m+[m[32m    sha = @git.commits.first.tree.id[m
[32m+[m
[32m+[m[32m    i = @git.index[m
[32m+[m[32m    i.read_tree(sha)[m
[32m+[m[32m    i.add('lib/atester.rb', 'test stuff')[m
[32m+[m[32m    i.commit('message', [@git.commits.first], @user, nil, 'master')[m
[32m+[m
[32m+[m[32m    b = @git.commits.first.tree/'lib'/'atester.rb'[m
[32m+[m[32m    assert_equal 'f80c3b68482d5e1c8d24c9b8139340f0d0a928d0', b.id[m
[32m+[m[32m    b = @git.commits.first.tree/'lib'/'grit.rb'[m
[32m+[m[32m    assert_equal '77aa887449c28a922a660b2bb749e4127f7664e5', b.id[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_ordered_properly[m
[32m+[m[32m    sha = @git.commits.first.tree.id[m
[32m+[m
[32m+[m[32m    i = @git.index[m
[32m+[m[32m    i.read_tree(sha)[m
[32m+[m[32m    i.add('lib.rb', 'test stuff')[m
[32m+[m[32m    i.commit('message', [@git.commits.first], @user, nil, 'master')[m
[32m+[m
[32m+[m[32m    tr = @git.commits.first.tree.contents[m
[32m+[m[32m    entries = tr.select { |c| c.name[0, 3] == 'lib' }.map { |c| c.name }[m
[32m+[m[32m    assert_equal 'lib.rb', entries[0][m
[32m+[m[32m    assert_equal 'lib', entries[1][m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_modify_file[m
[32m+[m[32m    sha = @git.commits.first.tree.id[m
[32m+[m
[32m+[m[32m    i = @git.index[m
[32m+[m[32m    i.read_tree(sha)[m
[32m+[m[32m    i.add('README.txt', 'test more stuff')[m
[32m+[m[32m    i.commit('message', [@git.commits.first], @user, nil, 'master')[m
[32m+[m
[32m+[m[32m    b = @git.commits.first.tree/'README.txt'[m
[32m+[m[32m    assert_equal 'e45d6b418e34951ddaa3e78e4fc4d3d92a46d3d1', b.id[m
[32m+[m[32m  end[m
[32m+[m[32mend[m

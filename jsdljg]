[32m+[m[32mrequire File.dirname(__FILE__) + '/helper'[m
[32m+[m
[32m+[m[32mclass TestGit < Test::Unit::TestCase[m
[32m+[m[32m  def setup[m
[32m+[m[32m    @git = Git.new(File.join(File.dirname(__FILE__), *%w[..]))[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def teardown[m
[32m+[m[32m    Grit.debug = false[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_method_missing[m
[32m+[m[32m    assert_match(/^git version [\w\.]*$/, @git.version)[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_logs_stderr[m
[32m+[m[32m    Grit.debug = true[m
[32m+[m[32m    Grit.stubs(:log)[m
[32m+[m[32m    Grit.expects(:log).with(includes("git: 'bad' is not a git-command"))[m
[32m+[m[32m    @git.bad[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def testl_logs_stderr_when_skipping_timeout[m
[32m+[m[32m    Grit.debug = true[m
[32m+[m[32m    Grit.stubs(:log)[m
[32m+[m[32m    Grit.expects(:log).with(includes("git: 'bad' is not a git-command"))[m
[32m+[m[32m    @git.bad :timeout => false[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_transform_options[m
[32m+[m[32m    assert_equal ["-s"], @git.transform_options({:s => true})[m
[32m+[m[32m    assert_equal [], @git.transform_options({:s => false})[m
[32m+[m[32m    assert_equal ["-s '5'"], @git.transform_options({:s => 5})[m
[32m+[m
[32m+[m[32m    assert_equal ["--max-count"], @git.transform_options({:max_count => true})[m
[32m+[m[32m    assert_equal ["--max-count='5'"], @git.transform_options({:max_count => 5})[m
[32m+[m
[32m+[m[32m    assert_equal ["-s", "-t"], @git.transform_options({:s => true, :t => true}).sort[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_uses_custom_sh_method[m
[32m+[m[32m    @git.expects(:sh)[m
[32m+[m[32m    @git.something[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_can_skip_timeout[m
[32m+[m[32m    @git.expects(:wild_sh)[m
[32m+[m[32m    @git.something(:timeout => false)[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_raises_if_too_many_bytes[m
[32m+[m[32m    @git.instance_variable_set(:@bytes_read, 6000000)[m
[32m+[m[32m    assert_raises Grit::Git::GitTimeout do[m
[32m+[m[32m      @git.version[m
[32m+[m[32m    end[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_raises_on_slow_shell[m
[32m+[m[32m    Grit::Git.git_timeout = 0.0000001[m
[32m+[m[32m    assert_raises Grit::Git::GitTimeout do[m
[32m+[m[32m      @git.version[m
[32m+[m[32m    end[m
[32m+[m[32m    Grit::Git.git_timeout = 5.0[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_it_really_shell_escapes_arguments_to_the_git_shell[m
[32m+[m[32m    @git.expects(:sh).with("#{Git.git_binary} --git-dir='#{@git.git_dir}' foo --bar='bazz\\'er'")[m
[32m+[m[32m    @git.foo(:bar => "bazz'er")[m
[32m+[m[32m    @git.expects(:sh).with("#{Git.git_binary} --git-dir='#{@git.git_dir}' bar -x 'quu\\'x'")[m
[32m+[m[32m    @git.bar(:x => "quu'x")[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_it_shell_escapes_the_standalone_argument[m
[32m+[m[32m    @git.expects(:sh).with("#{Git.git_binary} --git-dir='#{@git.git_dir}' foo 'bar\\'s'")[m
[32m+[m[32m    @git.foo({}, "bar's")[m
[32m+[m
[32m+[m[32m    @git.expects(:sh).with("#{Git.git_binary} --git-dir='#{@git.git_dir}' foo 'bar' '\\; echo \\'noooo\\''")[m
[32m+[m[32m    @git.foo({}, "bar", "; echo 'noooo'")[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_piping_should_work_on_1_9[m
[32m+[m[32m    @git.expects(:sh).with("#{Git.git_binary} --git-dir='#{@git.git_dir}' archive 'master' | gzip")[m
[32m+[m[32m    @git.archive({}, "master", "| gzip")[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_fs_read[m
[32m+[m[32m    f = stub[m
[32m+[m[32m    File.expects(:read).with(File.join(@git.git_dir, 'foo')).returns('bar')[m
[32m+[m[32m    assert_equal 'bar', @git.fs_read('foo')[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_fs_write[m
[32m+[m[32m    f = stub[m
[32m+[m[32m    f.expects(:write).with('baz')[m
[32m+[m[32m    FileUtils.expects(:mkdir_p).with(File.join(@git.git_dir, 'foo'))[m
[32m+[m[32m    File.expects(:open).with(File.join(@git.git_dir, 'foo/bar'), 'w').yields(f)[m
[32m+[m[32m    @git.fs_write('foo/bar', 'baz')[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_fs_delete[m
[32m+[m[32m    FileUtils.expects(:rm_rf).with(File.join(@git.git_dir, 'foo'))[m
[32m+[m[32m    @git.fs_delete('foo')[m
[32m+[m[32m  end[m
[32m+[m[32mend[m

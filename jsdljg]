[32m+[m[32mrequire '../lib/grit'[m
[32m+[m
[32m+[m[32mcount = 1[m
[32m+[m[32mDir.chdir("/Users/schacon/projects/atest") do[m
[32m+[m[32m  r = Grit::Repo.new('.')[m
[32m+[m[32m  i = r.index[m
[32m+[m[32m  while(count < 10) do[m
[32m+[m[32m    fname = Time.now.to_i.to_s + count.to_s[m
[32m+[m[32m    i.add(fname, 'hello ' + fname)[m
[32m+[m[32m    count += 1[m
[32m+[m[32m  end[m
[32m+[m[32m  puts i.commit('my commit')[m
[32m+[m[32m  puts i.inspect[m
[32m+[m[32mend[m

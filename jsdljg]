[32m+[m[32m    @old_debug  = Grit.debug[m
[32m+[m[32m    @old_logger = Grit.logger[m
[32m+[m[32m    Grit.debug  = true[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def teardown[m
[32m+[m[32m    Grit.debug  = @old_debug[m
[32m+[m[32m    Grit.logger = @old_logger[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_uses_stdout_logger_by_default[m
[32m+[m[32m    assert_equal STDOUT, Grit.logger.instance_variable_get(:@logdev).dev[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_can_override_logger[m
[32m+[m[32m    my_logger = Logger.new(io = StringIO.new)[m
[32m+[m[32m    Grit.logger = my_logger[m
[32m+[m[32m    assert_equal my_logger, Grit.logger[m
[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  def test_logs_to_specified_logger[m
[32m+[m[32m    Grit.logger = Logger.new(io = StringIO.new)[m
[32m+[m[32m    Grit.log 'hi mom'[m
[32m+[m[32m    io.rewind[m
[32m+[m[32m    assert io.read.include?('hi mom')[m
[32m+[m[32m  end[m

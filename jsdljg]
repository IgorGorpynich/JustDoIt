[32m+[m
[32m+[m[32mg.checkout('new_branch')[m
[32m+[m[32mg.checkout(g.branch('new_branch'))[m
[32m+[m
[32m+[m[32mg.branch(name).merge(branch2)[m
[32m+[m[32mg.branch(branch2).merge  # merges HEAD with branch2[m
[32m+[m
[32m+[m[32mg.branch(name).in_branch(message) { # add files }  # auto-commits[m
[32m+[m[32mg.merge('new_branch')[m
[32m+[m[32mg.merge('origin/remote_branch')[m
[32m+[m[32mg.merge(b.branch('master'))[m
[32m+[m[32mg.merge([branch1, branch2])[m
[32m+[m

[32m+[m[32m+require File.join(File.dirname(__FILE__), "spec_helper")[m
[32m+[m[32m+[m
[32m+[m[32m+describe Merb::Controller, " responds" do[m
[32m+[m[32m+[m[41m  [m
[32m+[m[32m+  before do[m
[32m+[m[32m+    Merb.push_path(:layout, File.dirname(__FILE__) / "controllers" / "views" / "layouts")[m[41m    [m
[32m+[m[32m+    Merb::Router.prepare do |r|[m
[32m+[m[32m+      r.default_routes[m
[32m+[m[32m+    end[m
[32m+[m[32m+  end[m
[32m+[m[32m+[m[41m  [m
[32m+[m[32m+  it "should default the mime-type to HTML" do[m
[32m+[m[32m+    dispatch_to(Merb::Test::Fixtures::TestHtmlDefault, :index).body.should == "HTML: Default"[m
[32m+[m[32m+  end[m
[32m+[m[32m+[m
[32m+[m[32m+  it "should use other mime-types if they are provided on the class level" do[m
[32m+[m[32m+    controller = dispatch_to(Merb::Test::Fixtures::TestClassProvides, :index, {}, :http_accept => "application/xml")[m
[32m+[m[32m+    controller.body.should == "<XML:Class provides='true' />"[m
[32m+[m[32m+  end[m
[32m+[m[32m+[m
[32m+[m[32m+  it "should fail if none of the acceptable mime-types are available" do[m
[32m+[m[32m+    calling { dispatch_to(Merb::Test::Fixtures::TestClassProvides, :index, {}, :http_accept => "application/json") }.[m
[32m+[m[32m+      should raise_error(Merb::ControllerExceptions::NotAcceptable)[m
[32m+[m[32m+  end[m
[32m+[m[32m+[m[41m  [m
[32m+[m[32m+  it "should use mime-types that are provided at the local level" do[m
[32m+[m[32m+    controller = dispatch_to(Merb::Test::Fixtures::TestLocalProvides, :index, {}, :http_accept => "application/xml")[m
[32m+[m[32m+    controller.body.should == "<XML:Local provides='true' />"[m[41m    [m
[32m+[m[32m+  end[m
[32m+[m[32m+[m[41m    [m
[32m+[m[32m+end[m

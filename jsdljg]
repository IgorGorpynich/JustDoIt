[32m+[m[32m      commit1 = '5e3ee1198672257164ce3fe31dea3e40848e68d5'[m
[32m+[m[32m      commit2 = 'ca8a30f5a7f0f163bbe3b6f0abf18a6c83b0687a'[m
[32m+[m[41m    [m
[32m+[m[32m      Benchmark.bm(8) do |x|[m
[32m+[m[41m            [m
[32m+[m[32m        run_code(x, 'packobj') do[m
[32m+[m[32m          @commit = git.commit('5e3ee1198672257164ce3fe31dea3e40848e68d5')[m
[32m+[m[32m          @tree = git.tree('cd7422af5a2e0fff3e94d6fb1a8fff03b2841881')[m
[32m+[m[32m          @blob = git.blob('4232d073306f01cf0b895864e5a5cfad7dd76fce')[m
[32m+[m[32m          @commit.parents[0].parents[0].parents[0][m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        run_code(x, 'commits 1') do[m
[32m+[m[32m          git.commits.size[m
[32m+[m[32m        end[m
[32m+[m[41m              [m
[32m+[m[32m        run_code(x, 'commits 2') do[m
[32m+[m[32m          log = git.commits('master', 15)[m
[32m+[m[32m          log.size[m
[32m+[m[32m          log.size[m
[32m+[m[32m          log.first[m
[32m+[m[32m          git.commits('testing').map { |c| c.message }[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        run_code(x, 'big revlist') do[m
[32m+[m[32m          c = git.commits('master', 200)[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        run_code(x, 'log') do[m
[32m+[m[32m          log = git.log('master')[m
[32m+[m[32m          log.size[m
[32m+[m[32m          log.size[m
[32m+[m[32m          log.first[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        run_code(x, 'diff') do[m
[32m+[m[32m          c = git.diff(commit1, commit2)[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        run_code(x, 'commit-diff') do[m
[32m+[m[32m          c = git.commit_diff(commit1)[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m        run_code(x, 'heads') do[m
[32m+[m[32m          c = git.heads.collect { |b| b.commit.id }[m
[32m+[m[32m        end[m
[32m+[m
[32m+[m[32m       # run_code(x, 'config', 100) do[m
[32m+[m[32m       #   c = git.config['user.name'][m
[32m+[m[32m       #   c = git.config['user.email'][m
[32m+[m[32m       # end[m
[32m+[m
[32m+[m[32m        #run_code(x, 'commit count') do[m
[32m+[m[32m        #  c = git.commit_count('testing')[m
[32m+[m[32m        #end[m
[32m+[m
[32m+[m
[32m+[m[32m      end[m
[32m+[m[32m    #end[m
[32m+[m
[32m+[m[32m    #printer = RubyProf::FlatPrinter.new(result)[m
[32m+[m[32m    #printer.print(STDOUT, 0)[m
[32m+[m[41m    [m
[32m+[m[32m  end[m
[32m+[m
[32m+[m
[32m+[m[32mend[m
[32m+[m
[32m+[m
[32m+[m[32mdef run_code(x, name, times = 30)[m
[32m+[m[32m    x.report(name.ljust(12)) do[m
[32m+[m[32m      for i in 1..times do[m
[32m+[m[32m        yield i[m
[32m+[m[32m      end[m
[32m+[m[32m    end[m
[32m+[m[41m  [m
[32m+[m[32m  #end[m
[32m+[m[41m  [m
[32m+[m[32m  # Print a graph profile to text[m
[32m+[m[32mend[m
[32m+[m
[32m+[m[32mdef new_file(name, contents)[m
[32m+[m[32m  File.open(name, 'w') do |f|[m
[32m+[m[32m    f.puts contents[m
[32m+[m[32m  end[m
[32m+[m[32mend[m
[32m+[m
[32m+[m
[32m+[m[32mdef in_temp_dir(remove_after = true)[m
[32m+[m[32m  filename = 'git_test' + Time.now.to_i.to_s + rand(300).to_s.rjust(3, '0')[m
[32m+[m[32m  tmp_path = File.join("/tmp/", filename)[m
[32m+[m[32m  FileUtils.mkdir(tmp_path)[m
[32m+[m[32m  Dir.chdir tmp_path do[m
[32m+[m[32m    FileUtils.cp_r(@wbare, File.join(tmp_path, '.git'))[m
[32m+[m[32m    yield tmp_path[m
[32m+[m[32m  end[m
[32m+[m[32m  puts tmp_path[m
[32m+[m[32m  #FileUtils.rm_r(tmp_path) if remove_after[m
[32m+[m[32mend[m
[32m+[m
[32m+[m[32mmain()[m

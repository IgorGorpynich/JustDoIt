[32m+[m
[32m+[m[32m  def test_file_type[m
[32m+[m[32m    out = @git.file_type(@tree_sha).to_s[m
[32m+[m[32m    assert_equal 'tree', out[m
[32m+[m[32m    out = @git.file_type(@blob_sha).to_s[m
[32m+[m[32m    assert_equal 'blob', out[m
[32m+[m[32m    out = @git.file_type(@commit_sha).to_s[m
[32m+[m[32m    assert_equal 'commit', out[m
[32m+[m[32m  end[m

[1mdiff --git a/grit/examples/ex_add_commit.rb b/grit/examples/ex_add_commit.rb[m
[1mnew file mode 100644[m
[1mindex 0000000..02bd62a[m
[1m--- /dev/null[m
[1m+++ b/grit/examples/ex_add_commit.rb[m
[36m@@ -0,0 +1,13 @@[m
[32m+[m[32mrequire '../lib/grit'[m
[32m+[m
[32m+[m[32mcount = 1[m
[32m+[m[32mDir.chdir("/Users/schacon/projects/atest") do[m
[32m+[m[32m  r = Grit::Repo.new('.')[m
[32m+[m[32m  while(count < 10) do[m
[32m+[m[32m    fname = Time.now.to_i.to_s + count.to_s[m
[32m+[m[32m    File.open(fname, 'w') { |f| f.write('hellor ' + fname) }[m
[32m+[m[32m    r.add(fname)[m
[32m+[m[32m    count += 1[m
[32m+[m[32m  end[m
[32m+[m[32m  r.commit_index('my commit')[m
[32m+[m[32mend[m
